[{"ImagePath": "https://schale.gg/images/student/collection/10043.webp", "MessageJP": "ワカモ（水着）", "MessageTW": "若藻(泳裝)", "MessageEN": "Wakamo (Swimsuit)"}, {"MessageId": 100420260, "Flag": 2, "Type": 4, "NextId": 100430260, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100430260, "Flag": 2, "Type": 0, "NextId": 100430270, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、お元気でしょうか？……その、もしよろしければ", "MessageTW": "老師，別來無恙？……那個，如果可以的話", "MessageEN": "Sensei. If you're okay with it..."}, {"MessageId": 100430270, "Flag": 1, "Type": 0, "NextId": 100430280, "MessageType": "Text", "ImagePath": "", "MessageJP": "以前お約束を頂戴した通り、一緒に……", "MessageTW": "按照之前的約定，一起……", "MessageEN": "As we promised before..."}, {"MessageId": 100430280, "Flag": 2, "Type": 3, "NextId": 100430290, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめん、ワカモ。ちょっと今忙しくて……", "MessageTW": "抱歉，若藻。現在有點忙……", "MessageEN": "Sorry, Wakamo! I'm really busy right now."}, {"MessageId": 100430290, "Flag": 2, "Type": 3, "NextId": 100430300, "MessageType": "Text", "ImagePath": "", "MessageJP": "少しだけ待ってくれる？", "MessageTW": "能等我一下嗎？", "MessageEN": "Can you wait for me?"}, {"MessageId": 100430300, "Flag": 2, "Type": 0, "NextId": 100430310, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい。分かりました", "MessageTW": "好的。我明白了", "MessageEN": "I can do that."}, {"MessageId": 100430310, "Flag": 1, "Type": 0, "NextId": 100420320, "MessageType": "Text", "ImagePath": "", "MessageJP": "このワカモ、いつまでもあなた様をお待ちしております", "MessageTW": "若藻我，會永遠等著閣下的", "MessageEN": "I'll always wait for you."}, {"MessageId": 100420320, "Flag": 2, "Type": 2, "NextId": 100430320, "MessageType": "Text", "ImagePath": "", "MessageJP": "ワカモ（水着）の絆イベントへ", "MessageTW": "前往若藻(泳裝)的羈絆劇情", "MessageEN": "Go to Wakamo (Swimsuit)'s Event"}, {"MessageId": 100430320, "Flag": 2, "Type": 0, "NextId": 100430330, "MessageType": "Text", "ImagePath": "", "MessageJP": "本日は一日、ありがとうございました", "MessageTW": "今日一日，非常感謝您", "MessageEN": "Thank you so much for spending time with me today, Sensei."}, {"MessageId": 100430330, "Flag": 1, "Type": 0, "NextId": 100430340, "MessageType": "Text", "ImagePath": "", "MessageJP": "このワカモ、今日という日が、大切な思い出の日となりました", "MessageTW": "對若藻我來說，這一天，成為了重要的回憶之日", "MessageEN": "I made very special memories today."}, {"MessageId": 100430340, "Flag": 2, "Type": 3, "NextId": 100430350, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、私の方こそ", "MessageTW": "不會，我才是", "MessageEN": "So did I."}, {"MessageId": 100430340, "Flag": 2, "Type": 3, "NextId": 100430350, "MessageType": "Text", "ImagePath": "", "MessageJP": "次はもっと普通に会おうね", "MessageTW": "下次再正常一點見面吧", "MessageEN": "Let's meet in a more...normal way next time."}, {"MessageId": 100430350, "Flag": 2, "Type": 0, "NextId": 100430360, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい、あなた様のそのお言葉、胸の奥に刻んでおきます", "MessageTW": "好的，閣下的那句話，我會銘記在心", "MessageEN": "Yes, I'll make sure to remember that."}, {"MessageId": 100430360, "Flag": 1, "Type": 0, "NextId": 100430361, "MessageType": "Text", "ImagePath": "", "MessageJP": "次は……そうですね", "MessageTW": "下次……也是呢", "MessageEN": "Next time... Yes."}, {"MessageId": 100430361, "Flag": 2, "Type": 0, "NextId": 100430370, "MessageType": "Text", "ImagePath": "", "MessageJP": "このような事がないよう、いつもと同じように", "MessageTW": "為了避免這種事態，會一如往常", "MessageEN": "Instead of waiting like this..."}, {"MessageId": 100430370, "Flag": 1, "Type": 0, "NextId": 100430380, "MessageType": "Text", "ImagePath": "", "MessageJP": "シャーレの正門から、お伺いするように致します", "MessageTW": "從夏萊的正門，前去拜訪", "MessageEN": "I'll go see you at the main gate of Schale."}, {"MessageId": 100430380, "Flag": 2, "Type": 3, "NextId": 100430390, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、正門は普段セキュリティがかかっているから", "MessageTW": "啊，正門平時有設保全系統", "MessageEN": "There's security cameras there, though."}, {"MessageId": 100430390, "Flag": 2, "Type": 0, "NextId": 100430400, "MessageType": "Text", "ImagePath": "", "MessageJP": "うふふっ、そんなセキュリティなんて、このワカモには何の障害にもなりません", "MessageTW": "唔哼哼，那種保全系統，對若藻我來說構成不了任何阻礙", "MessageEN": "Heeheehee. A security isn't an issue for me, not at all."}, {"MessageId": 100430400, "Flag": 2, "Type": 3, "NextId": 100430410, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ワカモ？ちょっと待って、それって……", "MessageTW": "……若藻？等一下，那是指……", "MessageEN": "...Huh? Wakamo? Wait, that means..."}, {"MessageId": 100430410, "Flag": 2, "Type": 0, "NextId": 100430420, "MessageType": "Text", "ImagePath": "", "MessageJP": "不一", "MessageTW": "不悉", "MessageEN": "See you later, then. Ta-ta."}, {"MessageId": 100430420, "Flag": 2, "Type": 3, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっ、ワカモ！？待って！？", "MessageTW": "咦，若藻！？等等！？", "MessageEN": "Oh, h-hey! Wakamo?!"}]