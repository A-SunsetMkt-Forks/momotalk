[{"ImagePath": "https://schale.gg/images/student/collection/10042.webp", "MessageJP": "アツコ", "MessageTW": "敦子", "MessageEN": "Atsuko"}, {"MessageId": 100410010, "Flag": 2, "Type": 4, "NextId": 100420010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100420010, "Flag": 2, "Type": 0, "NextId": 100420020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、いる？", "MessageTW": "老師，在嗎？", "MessageEN": "Sensei!"}, {"MessageId": 100420020, "Flag": 1, "Type": 0, "NextId": 100420030, "MessageType": "Text", "ImagePath": "", "MessageJP": "今大丈夫？このトーク届いてる？", "MessageTW": "現在沒事嗎？訊息有收到嗎？", "MessageEN": "Sensei, are you there?"}, {"MessageId": 100420030, "Flag": 2, "Type": 3, "NextId": 100420040, "MessageType": "Text", "ImagePath": "", "MessageJP": "アツコ？", "MessageTW": "敦子？", "MessageEN": "Atsuko?"}, {"MessageId": 100420040, "Flag": 2, "Type": 0, "NextId": 100420050, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、そうだよ。おはよう", "MessageTW": "嗯，是的。早安", "MessageEN": "Yes. Good morning."}, {"MessageId": 100420050, "Flag": 2, "Type": 3, "NextId": 100420060, "MessageType": "Text", "ImagePath": "", "MessageJP": "おはよう", "MessageTW": "早安", "MessageEN": "Good morning."}, {"MessageId": 100420050, "Flag": 2, "Type": 3, "NextId": 100420060, "MessageType": "Text", "ImagePath": "", "MessageJP": "もうそろそろ夜だけど、おはよう", "MessageTW": "雖然差不多快要晚上了，但早安", "MessageEN": "It's not morning anymore though."}, {"MessageId": 100420060, "Flag": 2, "Type": 0, "NextId": 100420061, "MessageType": "Text", "ImagePath": "", "MessageJP": "私たち、今あっちこっちを転々としてて", "MessageTW": "我們現在到處轉移陣地", "MessageEN": "As you know, we're wandering around all over the place now."}, {"MessageId": 100420061, "Flag": 2, "Type": 0, "NextId": 100420070, "MessageType": "Text", "ImagePath": "", "MessageJP": "一つの場所に長居しちゃいけないから", "MessageTW": "因為不能在同個地方待太久", "MessageEN": "We can't stay in one place for long."}, {"MessageId": 100420070, "Flag": 1, "Type": 0, "NextId": 100420071, "MessageType": "Text", "ImagePath": "", "MessageJP": "それと、みんなバラバラでいる事が多くて", "MessageTW": "而且大家都很分散", "MessageEN": "Not only that, but we spend a lot of time away from each other."}, {"MessageId": 100420071, "Flag": 2, "Type": 0, "NextId": 100420080, "MessageType": "Text", "ImagePath": "", "MessageJP": "一人でいると、退屈なの", "MessageTW": "一個人的話很無聊", "MessageEN": "It's boring to be alone."}, {"MessageId": 100420080, "Flag": 1, "Type": 0, "NextId": 100420081, "MessageType": "Text", "ImagePath": "", "MessageJP": "だから先生に会うには", "MessageTW": "所以，如果老師不來的話", "MessageEN": "So, for us to meet, "}, {"MessageId": 100420081, "Flag": 2, "Type": 0, "NextId": 100420090, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生がこっちに来てくれないと難しい", "MessageTW": "就很難跟老師見面", "MessageEN": "you have to come here."}, {"MessageId": 100420090, "Flag": 2, "Type": 3, "NextId": 100420100, "MessageType": "Text", "ImagePath": "", "MessageJP": "要するに、退屈だから会おうって捉え方で良い……？", "MessageTW": "簡單來說，是因為無聊，所以想跟我見面嗎……？", "MessageEN": "You want me to come see you because you're bored...?"}, {"MessageId": 100420100, "Flag": 2, "Type": 0, "NextId": 100420101, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん", "MessageTW": "嗯", "MessageEN": "Yes."}, {"MessageId": 100420101, "Flag": 2, "Type": 0, "NextId": 100420110, "MessageType": "Text", "ImagePath": "", "MessageJP": "その通り", "MessageTW": "就是那樣", "MessageEN": "Exactly."}, {"MessageId": 100420110, "Flag": 1, "Type": 0, "NextId": 100410120, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、後で", "MessageTW": "那麼，晚點見", "MessageEN": "I'll see you soon."}, {"MessageId": 100410120, "Flag": 2, "Type": 2, "NextId": 100420120, "MessageType": "Text", "ImagePath": "", "MessageJP": "アツコの絆イベントへ", "MessageTW": "前往敦子的羈絆劇情", "MessageEN": "Go to Atsuko's Event"}, {"MessageId": 100420120, "Flag": 2, "Type": 0, "NextId": 100420130, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、こんばんは", "MessageTW": "老師，晚上好", "MessageEN": "Sensei. "}, {"MessageId": 100420130, "Flag": 1, "Type": 0, "NextId": 100420131, "MessageType": "Text", "ImagePath": "", "MessageJP": "今からここを離れることになったよ", "MessageTW": "我現在要離開這裡了", "MessageEN": "I've decided to leave. "}, {"MessageId": 100420131, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、また。場所が変わったら連絡する", "MessageTW": "那麼，再見。換了地方後會再聯繫你的", "MessageEN": "I'll see you later. I'll contact you if the location changes. "}]