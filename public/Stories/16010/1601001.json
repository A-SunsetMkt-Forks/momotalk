[{"ImagePath": "https://schale.gg/images/student/collection/16010.webp", "MessageJP": "ヒビキ（応援団）", "MessageTW": "響(應援團)", "MessageEN": "Hibiki (Cheer Squad)"}, {"MessageId": 160090010, "Flag": 2, "Type": 4, "NextId": 160100010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160100010, "Flag": 2, "Type": 0, "NextId": 160100020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生は休日に何をして過ごすことが多いの？", "MessageTW": "老師在放假的時候通常都會做什麼呢？", "MessageEN": "Sensei, what do you normally do on weekends?"}, {"MessageId": 160100020, "Flag": 2, "Type": 3, "NextId": 160100030, "MessageType": "Text", "ImagePath": "", "MessageJP": "何もしてないかな……", "MessageTW": "什麼都不做吧……", "MessageEN": "Nothing in particular..."}, {"MessageId": 160100020, "Flag": 2, "Type": 3, "NextId": 160100040, "MessageType": "Text", "ImagePath": "", "MessageJP": "溜まった書類仕事と戦ってるよ", "MessageTW": "和堆積如山的文書工作戰鬥唷", "MessageEN": "I mostly end up working over the weekend anyway."}, {"MessageId": 160100030, "Flag": 2, "Type": 0, "NextId": 160100031, "MessageType": "Text", "ImagePath": "", "MessageJP": "たしかに身体を休めるのも大切だよね", "MessageTW": "好好讓身體休息也是很重要的呢", "MessageEN": "Rest is an important part of life."}, {"MessageId": 160100031, "Flag": 2, "Type": 0, "NextId": 160100050, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生が休日を楽しんでるみたいで良かった", "MessageTW": "看來老師有好好地享受假日，真是太好了", "MessageEN": "I was hoping to hear that you enjoyed your days off."}, {"MessageId": 160100040, "Flag": 1, "Type": 0, "NextId": 160100041, "MessageType": "Text", "ImagePath": "", "MessageJP": "……無理しすぎじゃない？", "MessageTW": "……是不是太勉強了？", "MessageEN": "...You're not overworking yourself, are you?"}, {"MessageId": 160100041, "Flag": 2, "Type": 0, "NextId": 160100050, "MessageType": "Text", "ImagePath": "", "MessageJP": "休みなしに働き続けるのは、却って非効率的だよ", "MessageTW": "不休息地持續工作，反而更沒效率", "MessageEN": "Work without rest is unproductive and inefficient."}, {"MessageId": 160100050, "Flag": 2, "Type": 3, "NextId": 160100060, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういうヒビキは？", "MessageTW": "那響呢？", "MessageEN": "Hibiki, what do you do to enjoy your weekends?"}, {"MessageId": 160100060, "Flag": 2, "Type": 0, "NextId": 160100070, "MessageType": "Text", "ImagePath": "", "MessageJP": "……私？", "MessageTW": "……我嗎？", "MessageEN": "...Me?"}, {"MessageId": 160100070, "Flag": 1, "Type": 0, "NextId": 160100080, "MessageType": "Text", "ImagePath": "", "MessageJP": "うーん、私はどうだろ……", "MessageTW": "唔──嗯，我的話……", "MessageEN": "My weekends are usually filled with..."}, {"MessageId": 160100080, "Flag": 1, "Type": 0, "NextId": 160090080, "MessageType": "Text", "ImagePath": "", "MessageJP": "だいたい何かを作ってるよ", "MessageTW": "大概都是在製作些什麼吧", "MessageEN": "engineering and tinkering, of course. Oh, and sometimes even drafting!"}, {"MessageId": 160090080, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ヒビキ（応援団）の絆イベントへ", "MessageTW": "前往響(應援團)的羈絆劇情", "MessageEN": "Go to Hibiki (Cheer Squad)'s Event"}]