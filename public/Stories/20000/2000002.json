[{"ImagePath": "https://schale.gg/images/student/collection/20000.webp", "MessageJP": "ヒビキ", "MessageTW": "響", "MessageEN": "Hibiki"}, {"MessageId": 199990090, "Flag": 2, "Type": 4, "NextId": 200000097, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200000097, "Flag": 2, "Type": 0, "NextId": 200000104, "MessageType": "Text", "ImagePath": "", "MessageJP": "花火を打ち上げるための機械を作ってほしいって依頼を受けたの", "MessageTW": "收到了製作放煙火機器的委託", "MessageEN": "I was asked to create a fireworks machine."}, {"MessageId": 200000104, "Flag": 1, "Type": 0, "NextId": 200000110, "MessageType": "Text", "ImagePath": "", "MessageJP": "よかったらミレニアムに来て、手伝ってくれない？", "MessageTW": "如果可以的話，能來千年幫忙嗎？", "MessageEN": "If it's okay with you, can you come to Millennium and help me out?"}, {"MessageId": 200000110, "Flag": 2, "Type": 3, "NextId": 200000120, "MessageType": "Text", "ImagePath": "", "MessageJP": "花火？", "MessageTW": "煙火？", "MessageEN": "Fireworks?"}, {"MessageId": 200000120, "Flag": 2, "Type": 3, "NextId": 200000135, "MessageType": "Text", "ImagePath": "", "MessageJP": "私、役に立てるかな？", "MessageTW": "我能幫上忙嗎？", "MessageEN": "Do you think I'll be helpful?"}, {"MessageId": 200000135, "Flag": 2, "Type": 0, "NextId": 200000132, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん", "MessageTW": "嗯", "MessageEN": "Yes."}, {"MessageId": 200000132, "Flag": 2, "Type": 0, "NextId": 199990140, "MessageType": "Text", "ImagePath": "", "MessageJP": "来てくれると嬉しい", "MessageTW": "能來的話我會很高興", "MessageEN": "I'll be so happy if you come with me."}, {"MessageId": 199990140, "Flag": 2, "Type": 2, "NextId": 200000149, "MessageType": "Text", "ImagePath": "", "MessageJP": "ヒビキの絆イベントへ", "MessageTW": "前往響的羈絆劇情", "MessageEN": "Go to Hibiki's Event"}, {"MessageId": 200000149, "Flag": 2, "Type": 0, "NextId": 200000146, "MessageType": "Text", "ImagePath": "", "MessageJP": "負傷者が出なくてよかった", "MessageTW": "幸好沒有人受傷", "MessageEN": "I'm so glad no one got hurt. "}, {"MessageId": 200000146, "Flag": 2, "Type": 0, "NextId": 200000153, "MessageType": "Text", "ImagePath": "", "MessageJP": "やっぱり先生といると……ドキドキすることがよく起きるね", "MessageTW": "果然和老師在一起的話……會經常發生讓心臟怦怦跳的事", "MessageEN": "I feel so excited...whenever I'm with you."}, {"MessageId": 200000153, "Flag": 1, "Type": 0, "NextId": 200000160, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生は……今日、どうだった？", "MessageTW": "老師……今天怎麼樣？", "MessageEN": "How was your day, Sensei?"}, {"MessageId": 200000160, "Flag": 2, "Type": 3, "NextId": 200000174, "MessageType": "Text", "ImagePath": "", "MessageJP": "楽しかったよ", "MessageTW": "很開心", "MessageEN": "It was fun."}, {"MessageId": 200000160, "Flag": 2, "Type": 3, "NextId": 200000174, "MessageType": "Text", "ImagePath": "", "MessageJP": "死ぬかと思った……", "MessageTW": "還以為會死……", "MessageEN": "I thought I was going to die."}, {"MessageId": 200000174, "Flag": 2, "Type": 0, "NextId": 200000171, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日はめちゃくちゃだったけど、いつか本当にすごい花火を……", "MessageTW": "今天雖然一團糟，但總有一天會放出真正厲害的煙火……", "MessageEN": "Today was a complete mess, but maybe next time..."}, {"MessageId": 200000171, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、それよりもっとすごいものを見せてあげるから、期待してて", "MessageTW": "不，會讓你看到比那個更厲害的東西，好好期待吧", "MessageEN": "I'll show you something even more amazing. Just watch."}]