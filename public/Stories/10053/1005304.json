[{"ImagePath": "https://schale.gg/images/student/collection/10053.webp", "MessageJP": "ユウカ（体操服）", "MessageTW": "優香(體育服)", "MessageEN": "Yuuka (Track)"}, {"MessageId": 100520220, "Flag": 2, "Type": 4, "NextId": 100530220, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100530220, "Flag": 2, "Type": 0, "NextId": 100530230, "MessageType": "Text", "ImagePath": "", "MessageJP": "慌ただしかった[ruby=こうりんたいさい]晄輪大祭[/ruby]も、やっと終わりましたね……", "MessageTW": "令人暈頭轉向的晄輪大祭總算是結束了呢……", "MessageEN": "The Kivotos Halo Festival and all the madness that came with it have both finally ended..."}, {"MessageId": 100530230, "Flag": 1, "Type": 0, "NextId": 100530240, "MessageType": "Text", "ImagePath": "", "MessageJP": "お疲れさまでした、先生", "MessageTW": "辛苦你了，老師", "MessageEN": "Thank you for all your hard work, Sensei."}, {"MessageId": 100530240, "Flag": 2, "Type": 3, "NextId": 100530250, "MessageType": "Text", "ImagePath": "", "MessageJP": "ユウカもお疲れさま", "MessageTW": "優香也辛苦了", "MessageEN": "You too, Yuuka."}, {"MessageId": 100530250, "Flag": 2, "Type": 0, "NextId": 100530260, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます。ですが、セミナーの役員として当然のことですから", "MessageTW": "謝謝。但這是身為研討會幹部的我應該做的", "MessageEN": "Not at all. As a member of Seminar, it was only natural to be so involved."}, {"MessageId": 100530260, "Flag": 1, "Type": 0, "NextId": 100530270, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、そうでした晄輪大祭で立て込んでいたので、シャーレに協力を要請した件の事務処理ができていませんでしたね", "MessageTW": "對了，老師因為忙著處理晄輪大祭，所以向夏萊請求協助的作業還沒有完成", "MessageEN": "By the way, we're supposed to discuss the Schale cooperation I requested."}, {"MessageId": 100530270, "Flag": 1, "Type": 0, "NextId": 100530271, "MessageType": "Text", "ImagePath": "", "MessageJP": "手持ちの業務を片付けたら、シャーレに伺いますね", "MessageTW": "等處理完手邊的業務後，我會去夏萊一趟", "MessageEN": "I'll visit Schale as soon as I wrap up my work at the academy."}, {"MessageId": 100530271, "Flag": 2, "Type": 0, "NextId": 100520280, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、また後ほどお願いします", "MessageTW": "那麼，之後再麻煩了", "MessageEN": "I look forward to seeing you."}, {"MessageId": 100520280, "Flag": 2, "Type": 2, "NextId": 100530280, "MessageType": "Text", "ImagePath": "", "MessageJP": "ユウカ（体操服）の絆イベントへ", "MessageTW": "前往優香(體育服)的羈絆劇情", "MessageEN": "Go to Yuuka (Track)'s Event"}, {"MessageId": 100530280, "Flag": 2, "Type": 0, "NextId": 100530290, "MessageType": "Image", "ImagePath": "./Stickers/Mo_Yuuka_01.webp", "MessageJP": "", "MessageTW": "", "MessageEN": ""}, {"MessageId": 100530290, "Flag": 1, "Type": 0, "NextId": 100530291, "MessageType": "Text", "ImagePath": "", "MessageJP": "一言、添えてみたんですが……", "MessageTW": "我在上面試著加了一句話……", "MessageEN": "So, I ended up writing a few lines..."}, {"MessageId": 100530291, "Flag": 2, "Type": 0, "NextId": 100530300, "MessageType": "Text", "ImagePath": "", "MessageJP": "ど、どうでしょう？", "MessageTW": "老，老師覺得怎麼樣？", "MessageEN": "...What do you think?"}, {"MessageId": 100530300, "Flag": 2, "Type": 3, "NextId": 100530310, "MessageType": "Text", "ImagePath": "", "MessageJP": "もう少し可愛らしくても……", "MessageTW": "好像可以再可愛一點……", "MessageEN": "It's a little wooden..."}, {"MessageId": 100530300, "Flag": 2, "Type": 3, "NextId": 100530310, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと堅苦しくない？", "MessageTW": "會不會有點嚴肅？", "MessageEN": "Try lightening it up with a little more passion..."}, {"MessageId": 100530310, "Flag": 2, "Type": 0, "NextId": 100530320, "MessageType": "Text", "ImagePath": "", "MessageJP": "うーん……", "MessageTW": "唔嗯……", "MessageEN": "Plead all you want..."}, {"MessageId": 100530320, "Flag": 1, "Type": 0, "NextId": 100530330, "MessageType": "Image", "ImagePath": "./Stickers/Mo_Yuuka_02.webp", "MessageJP": "", "MessageTW": "", "MessageEN": ""}, {"MessageId": 100530330, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "……先生だけですからね", "MessageTW": "……只有老師才有哦", "MessageEN": "This is a one-time-only thing."}]