[{"ImagePath": "https://schale.gg/images/student/collection/10023.webp", "MessageJP": "イオリ（水着）", "MessageTW": "伊織(泳裝)", "MessageEN": "Iori (Swimsuit)"}, {"MessageId": 100220070, "Flag": 2, "Type": 4, "NextId": 100230070, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100230070, "Flag": 2, "Type": 0, "NextId": 100230080, "MessageType": "Text", "ImagePath": "", "MessageJP": "……準備はできたか？", "MessageTW": "……準備好了嗎？", "MessageEN": "You have what I asked for, right?"}, {"MessageId": 100230080, "Flag": 1, "Type": 0, "NextId": 100230090, "MessageType": "Text", "ImagePath": "", "MessageJP": "念押しになるけど、絶対に他の生徒には言うなよ！", "MessageTW": "再提醒一次，絕對不准跟其他學生說！", "MessageEN": "Again, you can't tell other students about it!"}, {"MessageId": 100230090, "Flag": 2, "Type": 3, "NextId": 100230100, "MessageType": "Text", "ImagePath": "", "MessageJP": "……イオリ？", "MessageTW": "……伊織？", "MessageEN": "Iori?"}, {"MessageId": 100230090, "Flag": 2, "Type": 3, "NextId": 100230100, "MessageType": "Text", "ImagePath": "", "MessageJP": "これ私に送るメッセージで合ってる？", "MessageTW": "這是要發給我的訊息嗎？", "MessageEN": "Did you mean to send this to me?"}, {"MessageId": 100230100, "Flag": 2, "Type": 0, "NextId": 100230110, "MessageType": "Text", "ImagePath": "", "MessageJP": "……は？", "MessageTW": "……哈？", "MessageEN": "Sensei?"}, {"MessageId": 100230110, "Flag": 1, "Type": 0, "NextId": 100230111, "MessageType": "Text", "ImagePath": "", "MessageJP": "ｊぐぁｋｌｆ", "MessageTW": "ｊㄎㄚｋｌｆ", "MessageEN": "afjhdaklfhdalk"}, {"MessageId": 100230111, "Flag": 2, "Type": 0, "NextId": 100230112, "MessageType": "Text", "ImagePath": "", "MessageJP": "やっ、何でもない！", "MessageTW": "沒，沒事！", "MessageEN": "N-Nothing!"}, {"MessageId": 100230112, "Flag": 2, "Type": 0, "NextId": 100230120, "MessageType": "Text", "ImagePath": "", "MessageJP": "今送ったメッセージのことは忘れて！", "MessageTW": "請忘記剛才發的訊息！", "MessageEN": "Ignore the message I just sent you."}, {"MessageId": 100230120, "Flag": 2, "Type": 3, "NextId": 100220130, "MessageType": "Text", "ImagePath": "", "MessageJP": "そう言われると、尚更気になる……", "MessageTW": "你這樣說，反而更讓人在意……", "MessageEN": "Now I want to know even more..."}, {"MessageId": 100220130, "Flag": 2, "Type": 2, "NextId": 100230130, "MessageType": "Text", "ImagePath": "", "MessageJP": "イオリ（水着）の絆イベントへ", "MessageTW": "前往伊織(泳裝)的羈絆劇情", "MessageEN": "Go to Iori (Swimsuit)'s Event"}, {"MessageId": 100230130, "Flag": 2, "Type": 0, "NextId": 100230140, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……泳ぎの練習、付き合ってくれてありがと。先生", "MessageTW": "那個……謝謝你陪我練習游泳。老師", "MessageEN": "Thank you for helping with swim practice, Sensei."}, {"MessageId": 100230140, "Flag": 1, "Type": 0, "NextId": 100230141, "MessageType": "Text", "ImagePath": "", "MessageJP": "おかげで、お世辞にも習得したとは言えないけど、何とか泳げるくらいにはなったし……", "MessageTW": "託你的福，雖說還不是很熟練，但總算會游一點了……", "MessageEN": "I'm still not used to it, but I know how to swim now."}, {"MessageId": 100230141, "Flag": 2, "Type": 0, "NextId": 100230150, "MessageType": "Text", "ImagePath": "", "MessageJP": "これで合宿に浮き輪を持っていかずに済むかも", "MessageTW": "這樣就不用帶游泳圈去合宿了", "MessageEN": "I don't think I need the support tubes anymore."}, {"MessageId": 100230150, "Flag": 2, "Type": 3, "NextId": 100230160, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは良かった。ところでイオリ", "MessageTW": "那真是太好了。對了，伊織", "MessageEN": "Hey, Iori."}, {"MessageId": 100230160, "Flag": 2, "Type": 0, "NextId": 100230170, "MessageType": "Text", "ImagePath": "", "MessageJP": "……何？", "MessageTW": "……怎麼了？", "MessageEN": "What?"}, {"MessageId": 100230170, "Flag": 2, "Type": 3, "NextId": 100230180, "MessageType": "Text", "ImagePath": "", "MessageJP": "浮き輪に描かれてたけど、イオリはアヒルさんが好きなの？", "MessageTW": "看游泳圈上的圖案，難道伊織喜歡鴨子？", "MessageEN": "Did you pick that tube because of the cute duck on the side?"}, {"MessageId": 100230180, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "うるさい！死ねっ！", "MessageTW": "吵死了！去死！", "MessageEN": "Shut up! I'll kill you!"}]