[{"ImagePath": "https://schale.gg/images/student/collection/13006.webp", "MessageJP": "ムツキ", "MessageTW": "無月", "MessageEN": "Mutsuki"}, {"MessageId": 130050180, "Flag": 2, "Type": 4, "NextId": 130060185, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 130060185, "Flag": 2, "Type": 0, "NextId": 130060192, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、うちの便利屋に変わった依頼が届いたの！", "MessageTW": "老師，我們便利屋收到了一個特別的委託！", "MessageEN": "Sensei, Problem Solver has received a special request!"}, {"MessageId": 130060192, "Flag": 1, "Type": 0, "NextId": 130060199, "MessageType": "Text", "ImagePath": "", "MessageJP": "これがまた、すっごい変な依頼でさ～？", "MessageTW": "又會是非常奇怪的委託吧～？", "MessageEN": "Another huge request?"}, {"MessageId": 130060199, "Flag": 2, "Type": 0, "NextId": 130060200, "MessageType": "Text", "ImagePath": "", "MessageJP": "でも同時にすっごく面白そうでね～？", "MessageTW": "但同時又會很有趣吧～？", "MessageEN": "Another interesting request?"}, {"MessageId": 130060200, "Flag": 2, "Type": 3, "NextId": 130060210, "MessageType": "Text", "ImagePath": "", "MessageJP": "ムツキがすっごい変って言うほどの依頼っていったい……", "MessageTW": "究竟是什麼奇怪的委託能讓無月說成這樣……", "MessageEN": "When you say \"huge,\" I don't know what you expect..."}, {"MessageId": 130060200, "Flag": 2, "Type": 3, "NextId": 130060210, "MessageType": "Text", "ImagePath": "", "MessageJP": "ムツキがすっごく面白そうって言うような依頼っていったい……", "MessageTW": "究竟是什麼有趣的委託能讓無月說成這樣……", "MessageEN": "When you say \"interesting,\" I don't know what you expect..."}, {"MessageId": 130060210, "Flag": 2, "Type": 0, "NextId": 130060217, "MessageType": "Text", "ImagePath": "", "MessageJP": "ん～、まあまあ！細かいことは置いといてさ！", "MessageTW": "嗯～嘛嘛！先別管細節了！", "MessageEN": "Um... It's fine now."}, {"MessageId": 130060217, "Flag": 2, "Type": 0, "NextId": 130050220, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生に手伝ってほしいな～？", "MessageTW": "真希望老師能幫忙啊～？", "MessageEN": "Help me, Sensei!"}, {"MessageId": 130050220, "Flag": 2, "Type": 2, "NextId": 130060224, "MessageType": "Text", "ImagePath": "", "MessageJP": "ムツキの絆イベントへ", "MessageTW": "前往無月的羈絆劇情", "MessageEN": "Go to Mutsuki's Event"}, {"MessageId": 130060224, "Flag": 2, "Type": 0, "NextId": 130060231, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は一日楽しかったよ～先生！", "MessageTW": "今天一天過得很開心唷～老師！", "MessageEN": "I had a lot of fun today, Sensei!"}, {"MessageId": 130060231, "Flag": 1, "Type": 0, "NextId": 130060238, "MessageType": "Text", "ImagePath": "", "MessageJP": "次は絶っ対に成功させてみせるから、", "MessageTW": "下次絕對會成功給你看", "MessageEN": "This next one will be a total success."}, {"MessageId": 130060238, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "覚悟しててね～！＼( 'ω') /", "MessageTW": "做好覺悟吧～！＼( ╹ω╹) /", "MessageEN": "You better watch out! ＼( ╹ω╹) /"}]