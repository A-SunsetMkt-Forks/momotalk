[{"ImagePath": "https://schale.gg/images/student/collection/16005.webp", "MessageJP": "ツルギ（水着）", "MessageTW": "弦生(泳裝)", "MessageEN": "Tsurugi (Swimsuit)"}, {"MessageId": 15995010, "Flag": 2, "Type": 4, "NextId": 16005010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 16005010, "Flag": 2, "Type": 0, "NextId": 16005020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生。今お時間大丈夫でしょうか", "MessageTW": "老師。現在時間上方便嗎？", "MessageEN": "Sensei, are you free right now?"}, {"MessageId": 16005020, "Flag": 1, "Type": 0, "NextId": 16005021, "MessageType": "Text", "ImagePath": "", "MessageJP": "少々困った状況でして", "MessageTW": "我遇到了有點麻煩的狀況", "MessageEN": "I'm in a bit of a pinch."}, {"MessageId": 16005021, "Flag": 2, "Type": 0, "NextId": 16005030, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしければ、先生に助けていただきたいです", "MessageTW": "如果可以的話，想請老師幫忙", "MessageEN": "Could I ask for your help?"}, {"MessageId": 16005030, "Flag": 2, "Type": 3, "NextId": 16005040, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫？すぐに行くね", "MessageTW": "沒事吧？馬上過去", "MessageEN": "Huh? Help?"}, {"MessageId": 16005030, "Flag": 2, "Type": 3, "NextId": 16005040, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん、今どこにいる？", "MessageTW": "當然，現在在哪裡？", "MessageEN": "Sure. Where are you?"}, {"MessageId": 16005040, "Flag": 2, "Type": 0, "NextId": 16005041, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます、先生", "MessageTW": "非常感謝老師", "MessageEN": "Thank you very much, Sensei."}, {"MessageId": 16005041, "Flag": 2, "Type": 0, "NextId": 15995050, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、あの海辺で待機しております", "MessageTW": "那麼，我在那個海邊待命", "MessageEN": "I will be waiting at the beach, then."}, {"MessageId": 15995050, "Flag": 2, "Type": 2, "NextId": 16005050, "MessageType": "Text", "ImagePath": "", "MessageJP": "ツルギ（水着）の絆イベントへ", "MessageTW": "前往弦生(泳裝)的羈絆劇情", "MessageEN": "Go to Tsurugi (Swimsuit)'s Event"}, {"MessageId": 16005050, "Flag": 2, "Type": 0, "NextId": 16005060, "MessageType": "Text", "ImagePath": "", "MessageJP": "その", "MessageTW": "那個", "MessageEN": "Um..."}, {"MessageId": 16005060, "Flag": 1, "Type": 0, "NextId": 16005061, "MessageType": "Text", "ImagePath": "", "MessageJP": "申し訳ございません、先生", "MessageTW": "真的非常對不起，老師", "MessageEN": "I'm so sorry, Sensei."}, {"MessageId": 16005061, "Flag": 2, "Type": 0, "NextId": 16005070, "MessageType": "Text", "ImagePath": "", "MessageJP": "自分からお呼びしておいて、あんな風に……", "MessageTW": "明明是我請您來的，結果卻變成這樣……", "MessageEN": "I have no excuses, even after I asked first."}, {"MessageId": 16005070, "Flag": 2, "Type": 3, "NextId": 16005080, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、気にしないで", "MessageTW": "不會，別放在心上", "MessageEN": "Don't worry. These things happen."}, {"MessageId": 16005070, "Flag": 2, "Type": 3, "NextId": 16005080, "MessageType": "Text", "ImagePath": "", "MessageJP": "またいつでも気軽に呼んで", "MessageTW": "有事隨時都能找我，不要有壓力", "MessageEN": "You can call me later again."}, {"MessageId": 16005080, "Flag": 2, "Type": 0, "NextId": 16005081, "MessageType": "Text", "ImagePath": "", "MessageJP": "え", "MessageTW": "咦", "MessageEN": "Oh..."}, {"MessageId": 16005081, "Flag": 2, "Type": 0, "NextId": 16005090, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当に、良いのですか……？", "MessageTW": "真的可以嗎……？", "MessageEN": "Is that really okay?"}, {"MessageId": 16005090, "Flag": 1, "Type": 0, "NextId": 16005100, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 16005100, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます。また連絡いたします", "MessageTW": "非常感謝。會再聯絡的", "MessageEN": "Okay. Thank you."}]