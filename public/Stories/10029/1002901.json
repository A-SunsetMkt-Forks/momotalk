[{"ImagePath": "https://schale.gg/images/student/collection/10029.webp", "MessageJP": "ナツ", "MessageTW": "夏", "MessageEN": "Natsu"}, {"MessageId": 100280010, "Flag": 2, "Type": 4, "NextId": 100290010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100290010, "Flag": 2, "Type": 0, "NextId": 100290020, "MessageType": "Text", "ImagePath": "", "MessageJP": "行きたいところに行き、寝たい時に眠り", "MessageTW": "想去哪裡就去哪裡，想睡的時候就睡", "MessageEN": "Go where you want to go. Sleep where you want to sleep."}, {"MessageId": 100290020, "Flag": 1, "Type": 0, "NextId": 100290030, "MessageType": "Text", "ImagePath": "", "MessageJP": "そして、食べたいものを食べたいところで食べる", "MessageTW": "然後，在想吃的地方吃想吃的東西", "MessageEN": "And eat where you want to eat."}, {"MessageId": 100290030, "Flag": 1, "Type": 0, "NextId": 100290031, "MessageType": "Text", "ImagePath": "", "MessageJP": "何にも縛られず、制約など知ったことなく、思いのままに生きていく！", "MessageTW": "不受任何拘縛，不拘泥於制約，隨心所欲地生活！", "MessageEN": "Be unconstrained, be unaware of limitations, and live how you want to live!"}, {"MessageId": 100290031, "Flag": 2, "Type": 0, "NextId": 100290040, "MessageType": "Text", "ImagePath": "", "MessageJP": "それはまるで、手作りのクッキーのように！", "MessageTW": "就像是手工餅乾一樣！", "MessageEN": "Just like the life of a cookie made at your fingertips!"}, {"MessageId": 100290040, "Flag": 2, "Type": 3, "NextId": 100290050, "MessageType": "Text", "ImagePath": "", "MessageJP": "……えっと、何の話？", "MessageTW": "……那個，你在說什麼？", "MessageEN": "What are you talking about?"}, {"MessageId": 100290040, "Flag": 2, "Type": 3, "NextId": 100290050, "MessageType": "Text", "ImagePath": "", "MessageJP": "何かの名言？", "MessageTW": "什麼名言？", "MessageEN": "Is that some kind of famous saying or quote?"}, {"MessageId": 100290050, "Flag": 2, "Type": 0, "NextId": 100290051, "MessageType": "Text", "ImagePath": "", "MessageJP": "うーむ……", "MessageTW": "唔嗯……", "MessageEN": "Hmm."}, {"MessageId": 100290051, "Flag": 2, "Type": 0, "NextId": 100290060, "MessageType": "Text", "ImagePath": "", "MessageJP": "まだまだだね", "MessageTW": "還差得遠呢", "MessageEN": "It's a little surprising."}, {"MessageId": 100290060, "Flag": 1, "Type": 0, "NextId": 100290061, "MessageType": "Text", "ImagePath": "", "MessageJP": "その反応じゃあ足りないなー", "MessageTW": "你的反應不夠啊──", "MessageEN": "What a shame. Truly. I didn't expect this reaction."}, {"MessageId": 100290061, "Flag": 2, "Type": 0, "NextId": 100290062, "MessageType": "Text", "ImagePath": "", "MessageJP": "ロマンが足りないよ、先生", "MessageTW": "不夠浪漫唷，老師", "MessageEN": "You're lacking romance, Sensei."}, {"MessageId": 100290062, "Flag": 2, "Type": 0, "NextId": 100290070, "MessageType": "Text", "ImagePath": "", "MessageJP": "ではそんな先生に、私がロマンを教えてあげよっかな", "MessageTW": "那麼，我就來教教老師什麼是浪漫吧", "MessageEN": "This won't do. I'll have to teach you what romance is all about."}, {"MessageId": 100290070, "Flag": 1, "Type": 0, "NextId": 100290071, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふっ、心配しないで良いよー", "MessageTW": "呵呵，不用擔心唷──", "MessageEN": "Heehee."}, {"MessageId": 100290071, "Flag": 2, "Type": 0, "NextId": 100280070, "MessageType": "Text", "ImagePath": "", "MessageJP": "退屈はさせないからさ", "MessageTW": "不會讓你覺得無聊的", "MessageEN": "Don't worry. It won't be boring."}, {"MessageId": 100280070, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ナツの絆イベントへ", "MessageTW": "前往夏的羈絆劇情", "MessageEN": "Go to Natsu's Event"}]