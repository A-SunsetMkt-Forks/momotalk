[{"ImagePath": "https://schale.gg/images/student/collection/10061.webp", "MessageJP": "サクラコ", "MessageTW": "櫻子", "MessageEN": "Sakurako"}, {"MessageId": 100600250, "Flag": 2, "Type": 4, "NextId": 100610250, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100610250, "Flag": 2, "Type": 0, "NextId": 100610260, "MessageType": "Text", "ImagePath": "", "MessageJP": "夜分申し訳ありません、先生……", "MessageTW": "這麼晚打擾您非常抱歉，老師……", "MessageEN": "I know it's late...and on short notice, but..."}, {"MessageId": 100610260, "Flag": 1, "Type": 0, "NextId": 100610270, "MessageType": "Text", "ImagePath": "", "MessageJP": "少々お時間よろしいでしょうか？", "MessageTW": "能占用您一些時間嗎？", "MessageEN": "Sensei, would it be all right if I asked for your company for a while?"}, {"MessageId": 100610270, "Flag": 2, "Type": 3, "NextId": 100610280, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫だけど、どうしたの？", "MessageTW": "可以是可以，怎麼了嗎？", "MessageEN": "Any time. What's wrong?"}, {"MessageId": 100610270, "Flag": 2, "Type": 3, "NextId": 100610280, "MessageType": "Text", "ImagePath": "", "MessageJP": "何かあったの？", "MessageTW": "發生什麼事了？", "MessageEN": "What's wrong?"}, {"MessageId": 100610280, "Flag": 2, "Type": 0, "NextId": 100610290, "MessageType": "Text", "ImagePath": "", "MessageJP": "大したことではないのですが、", "MessageTW": "雖然不是什麼嚴重的事", "MessageEN": "Oh, it's nothing of concern..."}, {"MessageId": 100610290, "Flag": 1, "Type": 0, "NextId": 100610300, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ今回は……いえ、これまでもそうなのですが", "MessageTW": "但這次……不，一直以來都是這樣", "MessageEN": "But, well, I was just wondering... Not that this is out of the ordinary..."}, {"MessageId": 100610300, "Flag": 1, "Type": 0, "NextId": 100610310, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしければ、先生お一人で来ていただけませんか？", "MessageTW": "方便的話，能請您單獨過來嗎？", "MessageEN": "Do you mind coming alone?"}, {"MessageId": 100610310, "Flag": 1, "Type": 0, "NextId": 100610311, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、何と申し上げれば良いのでしょう……", "MessageTW": "那個，該怎麼說才好呢……", "MessageEN": "It's..."}, {"MessageId": 100610311, "Flag": 2, "Type": 0, "NextId": 100600320, "MessageType": "Text", "ImagePath": "", "MessageJP": "二人だけで、お話したいことがございまして……", "MessageTW": "有些話，我想與您單獨討論……", "MessageEN": "I have a secret to tell you. A private one."}, {"MessageId": 100600320, "Flag": 2, "Type": 2, "NextId": 100610320, "MessageType": "Text", "ImagePath": "", "MessageJP": "サクラコの絆イベントへ", "MessageTW": "前往櫻子的羈絆劇情", "MessageEN": "Go to Sakurako's Event"}, {"MessageId": 100610320, "Flag": 2, "Type": 0, "NextId": 100610330, "MessageType": "Text", "ImagePath": "", "MessageJP": "ええと……やはり", "MessageTW": "那，那個……果然", "MessageEN": "Yes."}, {"MessageId": 100610330, "Flag": 1, "Type": 0, "NextId": 100610331, "MessageType": "Text", "ImagePath": "", "MessageJP": "夜遅くにお呼び立てしてしまったのでは、と心配になってしまいました", "MessageTW": "我還是很擔心，會不會太晚找您出來", "MessageEN": "I'm sorry... I contacted you too late."}, {"MessageId": 100610331, "Flag": 2, "Type": 0, "NextId": 100610340, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生にご相談のうえ、お休みする場所を用意すれば良かったですね", "MessageTW": "既然是我有事找老師商量，應該提供休息的地方給老師才對", "MessageEN": "I didn't have time to prepare a bed for you or anything."}, {"MessageId": 100610340, "Flag": 1, "Type": 0, "NextId": 100610350, "MessageType": "Text", "ImagePath": "", "MessageJP": "申し訳ありません……", "MessageTW": "非常抱歉……", "MessageEN": "Okay."}, {"MessageId": 100610350, "Flag": 1, "Type": 0, "NextId": 100610351, "MessageType": "Text", "ImagePath": "", "MessageJP": "次からはそうしようと思います", "MessageTW": "下次開始我會這麼做的", "MessageEN": "Next time, I'll be properly prepared."}, {"MessageId": 100610351, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "では……いつも平穏無事でありますよう", "MessageTW": "那麼……希望您能永遠平安", "MessageEN": "May you always be at peace and full of luck."}]