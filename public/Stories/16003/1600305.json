[{"ImagePath": "https://schale.gg/images/student/collection/16003.webp", "MessageJP": "スズミ", "MessageTW": "鈴美", "MessageEN": "Suzumi"}, {"MessageId": 160020400, "Flag": 2, "Type": 4, "NextId": 160030400, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160030400, "Flag": 2, "Type": 0, "NextId": 160030410, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生。スズミです", "MessageTW": "你好，老師。我是鈴美", "MessageEN": "Hello, Sensei. It's Suzumi."}, {"MessageId": 160030410, "Flag": 1, "Type": 0, "NextId": 160030411, "MessageType": "Text", "ImagePath": "", "MessageJP": "近々、トリニティにいらっしゃるご予定があると伺いました", "MessageTW": "聽說你近期打算來三一", "MessageEN": "I heard you'll be visiting Trinity soon."}, {"MessageId": 160030411, "Flag": 2, "Type": 0, "NextId": 160030420, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……", "MessageTW": "那個……", "MessageEN": "If you happen to need..."}, {"MessageId": 160030420, "Flag": 1, "Type": 0, "NextId": 160030421, "MessageType": "Text", "ImagePath": "", "MessageJP": "もし、よろしければ", "MessageTW": "如果你不介意", "MessageEN": "Um..."}, {"MessageId": 160030421, "Flag": 2, "Type": 0, "NextId": 160030422, "MessageType": "Text", "ImagePath": "", "MessageJP": "その際に、エスコートをさせていただけたらと思います", "MessageTW": "到那個時候，不知道能不能由我來進行護衛", "MessageEN": "I could be your escort...a guide, I suppose."}, {"MessageId": 160030422, "Flag": 2, "Type": 0, "NextId": 160030430, "MessageType": "Text", "ImagePath": "", "MessageJP": "私はいつでも大丈夫ですので", "MessageTW": "我隨時都沒問題", "MessageEN": "Please just ask, and I'll be there."}, {"MessageId": 160030430, "Flag": 1, "Type": 0, "NextId": 160030440, "MessageType": "Text", "ImagePath": "", "MessageJP": "ご連絡いただけると、とても嬉しいです", "MessageTW": "老師願意聯絡我的話，我會很開心的", "MessageEN": "That's all... Thank you."}, {"MessageId": 160030440, "Flag": 2, "Type": 3, "NextId": 160030450, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつもありがとう、スズミ。お願いしてもいい？", "MessageTW": "一直以來都很感謝你，鈴美。可以拜託你嗎？", "MessageEN": "I'd really appreciate that. Thank you, Suzumi."}, {"MessageId": 160030440, "Flag": 2, "Type": 3, "NextId": 160030450, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、久しぶりに二人で歩こうか", "MessageTW": "那，我們久違的一起散個步吧", "MessageEN": "Oh! Should we take a walk together? It's been a while."}, {"MessageId": 160030450, "Flag": 2, "Type": 0, "NextId": 160030460, "MessageType": "Text", "ImagePath": "", "MessageJP": "は、はい", "MessageTW": "好，好的", "MessageEN": "Oh."}, {"MessageId": 160030460, "Flag": 1, "Type": 0, "NextId": 160030470, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます。それでは、また", "MessageTW": "謝謝你。那麼，再見", "MessageEN": "Let's do it."}, {"MessageId": 160030470, "Flag": 1, "Type": 0, "NextId": 160020480, "MessageType": "Text", "ImagePath": "", "MessageJP": "いらっしゃる際に伺いますね", "MessageTW": "老師來的時候，我會去找你的", "MessageEN": "Thank you. I'll see you when you visit, then."}, {"MessageId": 160020480, "Flag": 2, "Type": 2, "NextId": 160030480, "MessageType": "Text", "ImagePath": "", "MessageJP": "スズミの絆イベントへ", "MessageTW": "前往鈴美的羈絆劇情", "MessageEN": "Go to Suzumi's Event"}, {"MessageId": 160030480, "Flag": 2, "Type": 0, "NextId": 160030490, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は本当にありがとうございました", "MessageTW": "今天真的非常感謝", "MessageEN": "Thank you so much for today."}, {"MessageId": 160030490, "Flag": 1, "Type": 0, "NextId": 160030500, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生のおかげで、とても楽しかったです", "MessageTW": "託老師的福，過的非常開心", "MessageEN": "It was unexpectedly pleasant."}, {"MessageId": 160030500, "Flag": 2, "Type": 3, "NextId": 160030510, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとう。私も楽しかったよ", "MessageTW": "謝謝。我也很開心唷", "MessageEN": "I was the one being shown around."}, {"MessageId": 160030500, "Flag": 2, "Type": 3, "NextId": 160030510, "MessageType": "Text", "ImagePath": "", "MessageJP": "こちらこそ、これからもよろしくね", "MessageTW": "我才應該謝謝你，之後也拜託你囉", "MessageEN": "So I should be thanking you."}, {"MessageId": 160030510, "Flag": 2, "Type": 0, "NextId": 160030520, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや", "MessageTW": "呃", "MessageEN": "Um..."}, {"MessageId": 160030520, "Flag": 1, "Type": 0, "NextId": 160030530, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは、その……", "MessageTW": "那個……", "MessageEN": "Actually..."}, {"MessageId": 160030530, "Flag": 1, "Type": 0, "NextId": 160030531, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい", "MessageTW": "好的", "MessageEN": "Alright."}, {"MessageId": 160030531, "Flag": 2, "Type": 0, "NextId": 160030540, "MessageType": "Text", "ImagePath": "", "MessageJP": "これからもよろしくお願いします、先生", "MessageTW": "之後也請多多指教，老師", "MessageEN": "I'm looking forward to next time, Sensei."}, {"MessageId": 160030540, "Flag": 1, "Type": 0, "NextId": 160030550, "MessageType": "Text", "ImagePath": "", "MessageJP": "何かありましたら、お気軽にご連絡ください", "MessageTW": "有什麼事的話，歡迎隨時跟我聯絡，不用覺得有壓力", "MessageEN": "Anytime you need something, please just ask."}, {"MessageId": 160030550, "Flag": 1, "Type": 0, "NextId": 160030551, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえ", "MessageTW": "不", "MessageEN": "And also..."}, {"MessageId": 160030551, "Flag": 2, "Type": 0, "NextId": 160030552, "MessageType": "Text", "ImagePath": "", "MessageJP": "理由なんてなくても大丈夫です", "MessageTW": "就算沒有理由也沒關係", "MessageEN": "Um..."}, {"MessageId": 160030552, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "……先生なら、いつでも", "MessageTW": "……老師的話，隨時都可以", "MessageEN": "It can be about anything. Not just if you need a guide."}]