[{"ImagePath": "https://schale.gg/images/student/collection/10015.webp", "MessageJP": "アリス", "MessageTW": "愛麗絲", "MessageEN": "Aris"}, {"MessageId": 1001490010, "Flag": 2, "Type": 4, "NextId": 1001500010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 1001500010, "Flag": 2, "Type": 0, "NextId": 1001500020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、いよいよアリスは冒険に出ます", "MessageTW": "老師，愛麗絲終於要去冒險了", "MessageEN": "I am finally going on an adventure, Sensei."}, {"MessageId": 1001500020, "Flag": 1, "Type": 0, "NextId": 1001500021, "MessageType": "Text", "ImagePath": "", "MessageJP": "まずは「はじまりの村」周辺でレベルを上げなくてはいけないと聞きました", "MessageTW": "聽說首先得在「起始的村莊」附近提升等級", "MessageEN": "I hear I have to train near the town where I am starting out."}, {"MessageId": 1001500021, "Flag": 2, "Type": 0, "NextId": 1001500030, "MessageType": "Text", "ImagePath": "", "MessageJP": "行ってきます。どうか幸運を祈っていてください", "MessageTW": "愛麗絲要出發了。請祝愛麗絲好運", "MessageEN": "So, wish me luck."}, {"MessageId": 1001500030, "Flag": 2, "Type": 3, "NextId": 1001490040, "MessageType": "Text", "ImagePath": "", "MessageJP": "冒険……？", "MessageTW": "冒險……？", "MessageEN": "Huh? An adventure?"}, {"MessageId": 1001500030, "Flag": 2, "Type": 3, "NextId": 1001490040, "MessageType": "Text", "ImagePath": "", "MessageJP": "いよいよ其方も旅立つ時が来たか……", "MessageTW": "終於到了啟程的時候嗎……", "MessageEN": "I guess it's finally time..."}, {"MessageId": 1001490040, "Flag": 2, "Type": 2, "NextId": 1001500040, "MessageType": "Text", "ImagePath": "", "MessageJP": "アリスの絆イベントへ", "MessageTW": "前往愛麗絲的羈絆劇情", "MessageEN": "Go to Aris's Event"}, {"MessageId": 1001500040, "Flag": 2, "Type": 0, "NextId": 1001500050, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日の冒険は良い感じでした", "MessageTW": "今天的冒險感覺很好", "MessageEN": "Today's adventure went smoothly."}, {"MessageId": 1001500050, "Flag": 1, "Type": 0, "NextId": 1001500060, "MessageType": "Text", "ImagePath": "", "MessageJP": "また次も、一緒にパーティを組んでくれますか？", "MessageTW": "下次也可以一起組隊嗎？", "MessageEN": "Will you join the party next time too?"}, {"MessageId": 1001500060, "Flag": 2, "Type": 3, "NextId": 1001500070, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、もちろん。パーティだからね", "MessageTW": "嗯，當然。畢竟是隊伍嘛", "MessageEN": "Of course. I wouldn't miss it for the world."}, {"MessageId": 1001500060, "Flag": 2, "Type": 3, "NextId": 1001500070, "MessageType": "Text", "ImagePath": "", "MessageJP": "大事なマスコットだからね！", "MessageTW": "畢竟是重要的吉祥物嘛！", "MessageEN": "A mascot is an important companion!"}, {"MessageId": 1001500070, "Flag": 2, "Type": 0, "NextId": 1001500071, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい、よろしくお願いします", "MessageTW": "沒錯", "MessageEN": "That's right. "}, {"MessageId": 1001500071, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生との冒険、アリスはすごく楽しいです", "MessageTW": "和老師一起冒險，愛麗絲非常開心", "MessageEN": "I love going on adventures with you, Sensei."}]