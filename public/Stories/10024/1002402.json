[{"ImagePath": "https://schale.gg/images/student/collection/10024.webp", "MessageJP": "シロコ（ライディング）", "MessageTW": "白子(單車)", "MessageEN": "Shiroko (Cycling)"}, {"MessageId": 100230110, "Flag": 2, "Type": 4, "NextId": 100240110, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100240110, "Flag": 2, "Type": 0, "NextId": 100240120, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "How are you, Sensei?"}, {"MessageId": 100240120, "Flag": 1, "Type": 0, "NextId": 100240130, "MessageType": "Text", "ImagePath": "", "MessageJP": "今アビドスを完全に抜けたところ、もうすぐ海岸道路に着きそう", "MessageTW": "現在已經完全離開阿拜多斯，馬上就要到海岸道路了", "MessageEN": "I'm outside of Abydos now. I should reach the coastal road soon."}, {"MessageId": 100240130, "Flag": 2, "Type": 3, "NextId": 100240140, "MessageType": "Text", "ImagePath": "", "MessageJP": "速いね……！？", "MessageTW": "真快呢……！？", "MessageEN": "That far already? That's amazing."}, {"MessageId": 100240130, "Flag": 2, "Type": 3, "NextId": 100240140, "MessageType": "Text", "ImagePath": "", "MessageJP": "プロ選手みたいな速度だね……今のところ順調？", "MessageTW": "像職業選手一樣的速度呢……目前為止還順利嗎？", "MessageEN": "Like a professional, I should say."}, {"MessageId": 100240140, "Flag": 2, "Type": 0, "NextId": 100240141, "MessageType": "Text", "ImagePath": "", "MessageJP": "ん", "MessageTW": "嗯", "MessageEN": "Uhhh..."}, {"MessageId": 100240141, "Flag": 2, "Type": 0, "NextId": 100240150, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、そういえば……", "MessageTW": "啊，話說回來……", "MessageEN": "Um..."}, {"MessageId": 100240150, "Flag": 2, "Type": 3, "NextId": 100240160, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "Shiroko?"}, {"MessageId": 100240160, "Flag": 2, "Type": 0, "NextId": 100240170, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、いや", "MessageTW": "啊，不", "MessageEN": "No."}, {"MessageId": 100240170, "Flag": 1, "Type": 0, "NextId": 100240171, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめん、やっぱり大丈夫", "MessageTW": "抱歉，還是算了", "MessageEN": "Yes."}, {"MessageId": 100240171, "Flag": 2, "Type": 0, "NextId": 100240180, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとう", "MessageTW": "謝謝", "MessageEN": "Thanks."}, {"MessageId": 100240180, "Flag": 1, "Type": 0, "NextId": 100240181, "MessageType": "Text", "ImagePath": "", "MessageJP": "この後は、海岸道路沿いにずっと走るつもり", "MessageTW": "接下來，我打算沿著海岸道路一直騎", "MessageEN": "Then I'll head straight for the coastal road."}, {"MessageId": 100240181, "Flag": 2, "Type": 0, "NextId": 100230190, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあまた連絡するね、先生", "MessageTW": "那麼再聯絡吧，老師", "MessageEN": "I'll message you there, Sensei."}, {"MessageId": 100230190, "Flag": 2, "Type": 2, "NextId": 100240190, "MessageType": "Text", "ImagePath": "", "MessageJP": "シロコ（ライディング）の絆イベントへ", "MessageTW": "前往白子(單車)的羈絆劇情", "MessageEN": "Go to Shiroko (Cycling)'s Event"}, {"MessageId": 100240190, "Flag": 2, "Type": 0, "NextId": 100240191, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとう、先生。色々と助かった", "MessageTW": "謝謝老師幫我這麼多", "MessageEN": "I was able to stay on the move thanks to you."}, {"MessageId": 100240191, "Flag": 2, "Type": 0, "NextId": 100240200, "MessageType": "Text", "ImagePath": "", "MessageJP": "カバンの中に入れてると、どうしても飲み物はぬるくなっちゃうし", "MessageTW": "飲料放在背包裡總是會變溫的", "MessageEN": "Since the stuff in my bag heats up after a while"}, {"MessageId": 100240200, "Flag": 1, "Type": 0, "NextId": 100240201, "MessageType": "Text", "ImagePath": "", "MessageJP": "持ってきてくれたドリンク、すっごく冷たくて嬉しかった", "MessageTW": "老師幫我帶來的飲料非常冰涼，我很開心", "MessageEN": "The drink I just got is cooler than those."}, {"MessageId": 100240201, "Flag": 2, "Type": 0, "NextId": 100240210, "MessageType": "Text", "ImagePath": "", "MessageJP": "タオルも", "MessageTW": "毛巾也是", "MessageEN": "And the towels"}, {"MessageId": 100240210, "Flag": 1, "Type": 0, "NextId": 100240211, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ", "MessageTW": "啊", "MessageEN": "Oh."}, {"MessageId": 100240211, "Flag": 2, "Type": 0, "NextId": 100240220, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういえばさっき先生に渡したタオル、良ければ……", "MessageTW": "這麼說來，剛才給老師的毛巾，如果可以的話……", "MessageEN": "What happened to the towel I gave you before?"}, {"MessageId": 100240220, "Flag": 2, "Type": 3, "NextId": 100240230, "MessageType": "Text", "ImagePath": "", "MessageJP": "洗濯してちゃんと保管しておいたよ。今度返すね", "MessageTW": "已經洗好收起來了。下次再還給你", "MessageEN": "I washed it and put it away."}, {"MessageId": 100240230, "Flag": 2, "Type": 0, "NextId": 100240231, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと", "MessageTW": "那個", "MessageEN": "Ah"}, {"MessageId": 100240231, "Flag": 2, "Type": 0, "NextId": 100240240, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、やっぱりそうだよね……", "MessageTW": "嗯，果然是這樣啊……", "MessageEN": "Yes..."}, {"MessageId": 100240240, "Flag": 1, "Type": 0, "NextId": 100240241, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、ありがとう", "MessageTW": "沒事，謝謝", "MessageEN": "I see. Thanks."}, {"MessageId": 100240241, "Flag": 2, "Type": 0, "NextId": 100240242, "MessageType": "Text", "ImagePath": "", "MessageJP": "んと", "MessageTW": "然後", "MessageEN": "Anyway"}, {"MessageId": 100240242, "Flag": 2, "Type": 0, "NextId": 100240250, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ", "MessageTW": "那就", "MessageEN": "I'll talk to you next time, Sensei."}, {"MessageId": 100240250, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "また連絡するね、先生", "MessageTW": "下次再連絡，老師", "MessageEN": "Until we meet again."}]