[{"ImagePath": "https://schale.gg/images/student/collection/10056.webp", "MessageJP": "セリナ（クリスマス）", "MessageTW": "芹奈(聖誕節)", "MessageEN": "Serina (Christmas)"}, {"MessageId": 100550080, "Flag": 2, "Type": 4, "NextId": 100560080, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100560080, "Flag": 2, "Type": 0, "NextId": 100560090, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生！", "MessageTW": "你好，老師！", "MessageEN": "Hello, Sensei!"}, {"MessageId": 100560090, "Flag": 1, "Type": 0, "NextId": 100560100, "MessageType": "Text", "ImagePath": "", "MessageJP": "最近、体調はいかがですか？", "MessageTW": "最近身體還好嗎？", "MessageEN": "I hope you have been taking care of your health recently."}, {"MessageId": 100560100, "Flag": 2, "Type": 3, "NextId": 100560110, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちゃんとしたい気持ちは、あるのだけど……", "MessageTW": "雖然很想好好休息……", "MessageEN": "Sometimes! Kind of!"}, {"MessageId": 100560110, "Flag": 2, "Type": 0, "NextId": 100560111, "MessageType": "Text", "ImagePath": "", "MessageJP": "も、もう……！", "MessageTW": "真，真是的……！", "MessageEN": "Sensei! Again?"}, {"MessageId": 100560111, "Flag": 2, "Type": 0, "NextId": 100560120, "MessageType": "Text", "ImagePath": "", "MessageJP": "忙しいのは分かりますが、ダメですよ", "MessageTW": "雖然知道老師很忙，不過不可以唷", "MessageEN": "I've told you before, you can't be so irresponsible."}, {"MessageId": 100560120, "Flag": 1, "Type": 0, "NextId": 100560130, "MessageType": "Text", "ImagePath": "", "MessageJP": "それが続くと、そのうち", "MessageTW": "這樣下去的話，總有一天會", "MessageEN": "You know how it makes me worry about you, Sensei!"}, {"MessageId": 100560130, "Flag": 1, "Type": 0, "NextId": 100560140, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 100560140, "Flag": 1, "Type": 0, "NextId": 100560141, "MessageType": "Text", "ImagePath": "", "MessageJP": "あの", "MessageTW": "那個", "MessageEN": "Um."}, {"MessageId": 100560141, "Flag": 2, "Type": 0, "NextId": 100560150, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日、お時間大丈夫ですか？", "MessageTW": "今天，有時間嗎？", "MessageEN": "Do you...have time today?"}, {"MessageId": 100560150, "Flag": 2, "Type": 3, "NextId": 100560160, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫じゃなくても大丈夫にします！", "MessageTW": "就算沒有也會擠出來的！", "MessageEN": "I don't! But for you, I do!"}, {"MessageId": 100560160, "Flag": 2, "Type": 0, "NextId": 100560161, "MessageType": "Text", "ImagePath": "", "MessageJP": "え、えっと……？", "MessageTW": "呃，嗯……？", "MessageEN": "...?"}, {"MessageId": 100560161, "Flag": 2, "Type": 0, "NextId": 100560170, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは嬉しいのですが……", "MessageTW": "雖然你這麼說我很高興……", "MessageEN": "Wh-What? What does that mean?"}, {"MessageId": 100560170, "Flag": 1, "Type": 0, "NextId": 100550170, "MessageType": "Text", "ImagePath": "", "MessageJP": "で、ではまた後で", "MessageTW": "那，那麼待會見", "MessageEN": "...Sensei?"}, {"MessageId": 100550170, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "セリナ（クリスマス）の絆イベントへ", "MessageTW": "前往芹奈(聖誕節)的羈絆劇情", "MessageEN": "Go to Serina (Christmas)'s Event"}]