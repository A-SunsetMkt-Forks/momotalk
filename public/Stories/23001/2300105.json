[{"ImagePath": "https://schale.gg/images/student/collection/23001.webp", "MessageJP": "フウカ", "MessageTW": "風華", "MessageEN": "Fuuka"}, {"MessageId": 230000250, "Flag": 2, "Type": 4, "NextId": 230010254, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 230010254, "Flag": 2, "Type": 0, "NextId": 230010261, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 230010261, "Flag": 1, "Type": 0, "NextId": 230010270, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日お手伝いしてくださるお話、ちゃんと覚えていらっしゃいますか？", "MessageTW": "之前有說過今天要來幫我，老師還記得嗎？", "MessageEN": "You promised to help me during lunch today. You haven't forgotten, have you?"}, {"MessageId": 230010270, "Flag": 2, "Type": 3, "NextId": 230010282, "MessageType": "Text", "ImagePath": "", "MessageJP": "覚えてるよ", "MessageTW": "記得喔", "MessageEN": "I remember."}, {"MessageId": 230010270, "Flag": 2, "Type": 3, "NextId": 230010299, "MessageType": "Text", "ImagePath": "", "MessageJP": "……今日だっけ？", "MessageTW": "……是今天嗎？", "MessageEN": "Did I say that?"}, {"MessageId": 230010282, "Flag": 2, "Type": 0, "NextId": 230010363, "MessageType": "Text", "ImagePath": "", "MessageJP": "良かったです♪", "MessageTW": "太好了♪", "MessageEN": "Oh, yeah. What a relief. ♪"}, {"MessageId": 230010299, "Flag": 1, "Type": 0, "NextId": 230010300, "MessageType": "Text", "ImagePath": "", "MessageJP": "あっ、もしかして、きちんと伝えられてなかったでしょうか……？", "MessageTW": "啊，難道那時候沒有好好傳達到嗎……？", "MessageEN": "Oh, we must have miscommunicated."}, {"MessageId": 230010300, "Flag": 2, "Type": 3, "NextId": 230010313, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめん、冗談", "MessageTW": "抱歉，開玩笑的", "MessageEN": "I'm just joking."}, {"MessageId": 230010313, "Flag": 2, "Type": 0, "NextId": 230010320, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、冗談、でしたか……", "MessageTW": "啊，原來，是開玩笑的啊……", "MessageEN": "Oh, it was a joke..."}, {"MessageId": 230010320, "Flag": 1, "Type": 0, "NextId": 230010327, "MessageType": "Text", "ImagePath": "", "MessageJP": "そんな意地悪な冗談を言われてしまうと、", "MessageTW": "被開這種欺負人的玩笑的話，", "MessageEN": "Don't be like that..."}, {"MessageId": 230010327, "Flag": 2, "Type": 0, "NextId": 230010330, "MessageType": "Text", "ImagePath": "", "MessageJP": "私、傷つきます……", "MessageTW": "我會受傷的……", "MessageEN": "Are you trying to hurt my feelings?"}, {"MessageId": 230010330, "Flag": 2, "Type": 3, "NextId": 230010341, "MessageType": "Text", "ImagePath": "", "MessageJP": "ご、ごめんなさい……！", "MessageTW": "抱，抱歉……！", "MessageEN": "I'm sorry!"}, {"MessageId": 230010341, "Flag": 2, "Type": 0, "NextId": 230010350, "MessageType": "Text", "ImagePath": "", "MessageJP": "……なんて、これも冗談ですけどね", "MessageTW": "……騙你的，這也是開玩笑的哦", "MessageEN": "Huh? It was a joke!"}, {"MessageId": 230010350, "Flag": 2, "Type": 3, "NextId": 230010365, "MessageType": "Text", "ImagePath": "", "MessageJP": "やられた！", "MessageTW": "中計了！", "MessageEN": "You got me!"}, {"MessageId": 230010365, "Flag": 2, "Type": 0, "NextId": 230010363, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふ、それではおふざけはこれぐらいにして", "MessageTW": "呵呵，玩笑話就先說到這裡", "MessageEN": "Haha! That's enough fooling around for today."}, {"MessageId": 230010363, "Flag": 1, "Type": 0, "NextId": 230000370, "MessageType": "Text", "ImagePath": "", "MessageJP": "食堂で待ってますね！", "MessageTW": "我在食堂等老師哦！", "MessageEN": "I'll wait for you at the student dining hall!"}, {"MessageId": 230000370, "Flag": 2, "Type": 2, "NextId": 230010379, "MessageType": "Text", "ImagePath": "", "MessageJP": "フウカの絆イベントへ", "MessageTW": "前往風華的羈絆劇情", "MessageEN": "Go to Fuuka's Event"}, {"MessageId": 230010379, "Flag": 2, "Type": 0, "NextId": 230010386, "MessageType": "Text", "ImagePath": "", "MessageJP": "せ、先生……", "MessageTW": "老，老師……", "MessageEN": "Sensei..."}, {"MessageId": 230010386, "Flag": 1, "Type": 0, "NextId": 230010390, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、今日のことは……", "MessageTW": "那個，今天發生的事……", "MessageEN": "What happened today..."}, {"MessageId": 230010390, "Flag": 2, "Type": 3, "NextId": 230010407, "MessageType": "Text", "ImagePath": "", "MessageJP": "何かあったっけ？", "MessageTW": "有發生什麼嗎？", "MessageEN": "What happened?"}, {"MessageId": 230010390, "Flag": 2, "Type": 3, "NextId": 230010407, "MessageType": "Text", "ImagePath": "", "MessageJP": "フウカたん、可愛かったな……", "MessageTW": "風華炭，真可愛啊……", "MessageEN": "It was cute, Fuuka."}, {"MessageId": 230010407, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "う、うぅっ……！ほ、他の人たちには、内緒ですからね……？", "MessageTW": "嗚，嗚嗚……！不，不能告訴其他人唷……？", "MessageEN": "Please don't tell anyone! Please!"}]