[{"ImagePath": "https://schale.gg/images/student/collection/10050.webp", "MessageJP": "ココナ", "MessageTW": "心菜", "MessageEN": "Kokona"}, {"MessageId": 100490150, "Flag": 2, "Type": 4, "NextId": 100500150, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100500150, "Flag": 2, "Type": 0, "NextId": 100500160, "MessageType": "Text", "ImagePath": "", "MessageJP": "考えてみたんですが", "MessageTW": "我想了想", "MessageEN": "I've been thinking."}, {"MessageId": 100500160, "Flag": 1, "Type": 0, "NextId": 100500161, "MessageType": "Text", "ImagePath": "", "MessageJP": "必ずしもコーヒーを飲めることが大人というのは違う気がしてきました", "MessageTW": "發現好像不是能喝咖啡就是大人了", "MessageEN": "I don't think enjoying coffee means you're an adult."}, {"MessageId": 100500161, "Flag": 2, "Type": 0, "NextId": 100500170, "MessageType": "Text", "ImagePath": "", "MessageJP": "コーヒーを飲まない大人もいるのではないでしょうか", "MessageTW": "也有不喝咖啡的大人吧", "MessageEN": "I'm sure there are grown-ups who don't like coffee."}, {"MessageId": 100500170, "Flag": 2, "Type": 3, "NextId": 100500180, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん。無理に飲む必要はないからね", "MessageTW": "嗯。沒有必要勉強喝", "MessageEN": "Maybe it's too early for you, Kokona."}, {"MessageId": 100500180, "Flag": 2, "Type": 0, "NextId": 100500190, "MessageType": "Text", "ImagePath": "", "MessageJP": "やはりそうですよね！", "MessageTW": "果然是那樣啊！", "MessageEN": "I'm just saying!"}, {"MessageId": 100500190, "Flag": 1, "Type": 0, "NextId": 100500191, "MessageType": "Text", "ImagePath": "", "MessageJP": "世の中にはどうしても合わない食べ物だってありますし", "MessageTW": "世上總有不合自己胃口的食物", "MessageEN": "You just can't acquire a taste for everything, you know?"}, {"MessageId": 100500191, "Flag": 2, "Type": 0, "NextId": 100500200, "MessageType": "Text", "ImagePath": "", "MessageJP": "例えば、私とニンジンのように", "MessageTW": "舉例來說，就像我之於紅蘿蔔", "MessageEN": "For example, I can't eat carrots."}, {"MessageId": 100500200, "Flag": 2, "Type": 3, "NextId": 100500210, "MessageType": "Text", "ImagePath": "", "MessageJP": "今度はコーヒーに牛乳と砂糖を入れてあげるね", "MessageTW": "我下次會幫你在咖啡裡面加牛奶跟砂糖的", "MessageEN": "I'll put milk and sugar in your coffee next time."}, {"MessageId": 100500210, "Flag": 2, "Type": 0, "NextId": 100500211, "MessageType": "Text", "ImagePath": "", "MessageJP": "牛乳と砂糖ですか……！？", "MessageTW": "牛奶跟砂糖嗎……！？", "MessageEN": "Milk and sugar...?!"}, {"MessageId": 100500211, "Flag": 2, "Type": 0, "NextId": 100500220, "MessageType": "Text", "ImagePath": "", "MessageJP": "そ、それなら考え直してもいいかも……？", "MessageTW": "那，那樣的話，好像可以重新考慮一下……？", "MessageEN": "Th-Then I might reconsider..."}, {"MessageId": 100500220, "Flag": 1, "Type": 0, "NextId": 100500221, "MessageType": "Text", "ImagePath": "", "MessageJP": "いいえ、やっぱりいいです", "MessageTW": "不，果然還是算了", "MessageEN": "No, I don't think so."}, {"MessageId": 100500221, "Flag": 2, "Type": 0, "NextId": 100500230, "MessageType": "Text", "ImagePath": "", "MessageJP": "私は立派なレディーですから、そんなものは必要ありません", "MessageTW": "我是優秀的淑女，不需要那種東西", "MessageEN": "I'm a proper lady, so I don't need that."}, {"MessageId": 100500230, "Flag": 1, "Type": 0, "NextId": 100500240, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ私とコーヒーが合わないだけですから", "MessageTW": "就只是咖啡不適合我而已", "MessageEN": "I just don't like coffee, that's all."}, {"MessageId": 100500240, "Flag": 2, "Type": 3, "NextId": 100500250, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、私は牛乳と砂糖を入れておいしいコーヒーを飲もうかな", "MessageTW": "那麼，我就來喝加了牛奶和砂糖的美味咖啡吧", "MessageEN": "Then I'll just have delicious coffee with milk and sugar by myself!"}, {"MessageId": 100500250, "Flag": 2, "Type": 0, "NextId": 100500251, "MessageType": "Text", "ImagePath": "", "MessageJP": "ずるいです！", "MessageTW": "太狡猾了！", "MessageEN": "That's not fair!"}, {"MessageId": 100500251, "Flag": 2, "Type": 0, "NextId": 100490250, "MessageType": "Text", "ImagePath": "", "MessageJP": "大人はみんなずるいです！", "MessageTW": "大人們都很狡猾！", "MessageEN": "Adults are all unfair!"}, {"MessageId": 100490250, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ココナの絆イベントへ", "MessageTW": "前往心菜的羈絆劇情", "MessageEN": "Go to Kokona's Event"}]