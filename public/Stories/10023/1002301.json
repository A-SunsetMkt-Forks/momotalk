[{"ImagePath": "https://schale.gg/images/student/collection/10023.webp", "MessageJP": "イオリ（水着）", "MessageTW": "伊織(泳裝)", "MessageEN": "Iori (Swimsuit)"}, {"MessageId": 100220010, "Flag": 2, "Type": 4, "NextId": 100230010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100230010, "Flag": 2, "Type": 0, "NextId": 100230020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、ひとつお願いしても良い？", "MessageTW": "老師，可以拜託你一件事嗎？", "MessageEN": "Sensei, could you do me a favor?"}, {"MessageId": 100230020, "Flag": 2, "Type": 3, "NextId": 100230030, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん、何でも言って", "MessageTW": "當然，什麼事都可以", "MessageEN": "Of course."}, {"MessageId": 100230020, "Flag": 2, "Type": 3, "NextId": 100230030, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、何をすれば良い？", "MessageTW": "嗯，我該做什麼？", "MessageEN": "What kind of favor?"}, {"MessageId": 100230030, "Flag": 2, "Type": 0, "NextId": 100230040, "MessageType": "Text", "ImagePath": "", "MessageJP": "今度ゲヘナに来るときに、日焼け止めを買ってきて欲しくて", "MessageTW": "下次來格黑娜的時候，想請你幫忙買個防曬乳", "MessageEN": "Could you bring me sunscreen next time you visit Gehenna Academy?"}, {"MessageId": 100230040, "Flag": 1, "Type": 0, "NextId": 100230041, "MessageType": "Text", "ImagePath": "", "MessageJP": "ずっと使ってたのが無くなっちゃったから、合宿前に買おうと思ってたんだけど", "MessageTW": "因為一直使用的用完了，所以想在合宿之前添購", "MessageEN": "I ran out of sunscreen before summer camp, and Prefect Team work is keeping me busy."}, {"MessageId": 100230041, "Flag": 2, "Type": 0, "NextId": 100230050, "MessageType": "Text", "ImagePath": "", "MessageJP": "思ったよりも委員会の仕事が忙しくって……", "MessageTW": "但風紀委員會的工作比想像中還要忙……", "MessageEN": "I don't have time to go out to buy more."}, {"MessageId": 100230050, "Flag": 2, "Type": 3, "NextId": 100230055, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫、任せて", "MessageTW": "沒問題，交給我", "MessageEN": "Okay."}, {"MessageId": 100230055, "Flag": 2, "Type": 3, "NextId": 100230060, "MessageType": "Text", "ImagePath": "", "MessageJP": "「シーサイド」の日焼け止めだよね？", "MessageTW": "是「Seaside」的防曬乳吧？", "MessageEN": "Would sun oil from Seaside do the trick?"}, {"MessageId": 100230060, "Flag": 2, "Type": 0, "NextId": 100230061, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、それで合ってる", "MessageTW": "嗯，就是那個", "MessageEN": "Yes, that's fine."}, {"MessageId": 100230061, "Flag": 2, "Type": 0, "NextId": 100220060, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、よろしく", "MessageTW": "那麼，拜託了", "MessageEN": "I'll be waiting, then."}, {"MessageId": 100220060, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "イオリ（水着）の絆イベントへ", "MessageTW": "前往伊織(泳裝)的羈絆劇情", "MessageEN": "Go to Iori (Swimsuit)'s Event"}]