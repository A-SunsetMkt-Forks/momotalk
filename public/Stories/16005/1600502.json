[{"ImagePath": "https://schale.gg/images/student/collection/16005.webp", "MessageJP": "ツルギ（水着）", "MessageTW": "弦生(泳裝)", "MessageEN": "Tsurugi (Swimsuit)"}, {"MessageId": 15995110, "Flag": 2, "Type": 4, "NextId": 16005110, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 16005110, "Flag": 2, "Type": 0, "NextId": 16005120, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 16005120, "Flag": 1, "Type": 0, "NextId": 16005130, "MessageType": "Text", "ImagePath": "", "MessageJP": "あの件は大変失礼いたしました。申し訳ありませんでした", "MessageTW": "之前那件事實在太失禮了。萬分抱歉", "MessageEN": "I'm really sorry about last time. I was so inconsiderate."}, {"MessageId": 16005130, "Flag": 2, "Type": 3, "NextId": 16005140, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫だよ、気にしないで", "MessageTW": "沒事的唷，別放在心上", "MessageEN": "Don't worry about it."}, {"MessageId": 16005130, "Flag": 2, "Type": 3, "NextId": 16005140, "MessageType": "Text", "ImagePath": "", "MessageJP": "落ち着いた？", "MessageTW": "冷靜下來了嗎？", "MessageEN": "How are you feeling now? How is the situation?"}, {"MessageId": 16005140, "Flag": 2, "Type": 0, "NextId": 16005141, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい。ありがとうございます", "MessageTW": "是的。非常感謝", "MessageEN": "Well, the situation is..."}, {"MessageId": 16005141, "Flag": 2, "Type": 0, "NextId": 16005150, "MessageType": "Text", "ImagePath": "", "MessageJP": "結局、お化けは捕まりませんでした……", "MessageTW": "結果，沒有抓到妖怪……", "MessageEN": "Actually not that different."}, {"MessageId": 16005150, "Flag": 1, "Type": 0, "NextId": 16005160, "MessageType": "Text", "ImagePath": "", "MessageJP": "他のところに行っても同じ状況なので、どうしようかなと", "MessageTW": "去別的地方也是相同的狀況，該怎麼辦呢", "MessageEN": "I'm still looking for a solution."}, {"MessageId": 16005160, "Flag": 2, "Type": 3, "NextId": 16005170, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ今からそっちに行くね", "MessageTW": "那我現在過去你那邊", "MessageEN": "Okay. I'll be right there."}, {"MessageId": 16005160, "Flag": 2, "Type": 3, "NextId": 16005170, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと待っててくれる？", "MessageTW": "能稍等一下嗎？", "MessageEN": "You're going to need my help."}, {"MessageId": 16005170, "Flag": 2, "Type": 0, "NextId": 16005180, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい", "MessageTW": "好的", "MessageEN": "Is that so...? Thank you."}, {"MessageId": 16005180, "Flag": 1, "Type": 0, "NextId": 16005181, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 16005181, "Flag": 2, "Type": 0, "NextId": 16005182, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい？", "MessageTW": "嗯？", "MessageEN": "Huh?"}, {"MessageId": 16005182, "Flag": 2, "Type": 0, "NextId": 15995190, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生？", "MessageTW": "老師？", "MessageEN": "Sensei?"}, {"MessageId": 15995190, "Flag": 2, "Type": 2, "NextId": 16005190, "MessageType": "Text", "ImagePath": "", "MessageJP": "ツルギ（水着）の絆イベントへ", "MessageTW": "前往弦生(泳裝)的羈絆劇情", "MessageEN": "Go to Tsurugi (Swimsuit)'s Event"}, {"MessageId": 16005190, "Flag": 2, "Type": 0, "NextId": 16005200, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当にありがとうございました、先生", "MessageTW": "真的非常感謝老師", "MessageEN": "Thank you so much, Sensei."}, {"MessageId": 16005200, "Flag": 1, "Type": 0, "NextId": 16005201, "MessageType": "Text", "ImagePath": "", "MessageJP": "おかげさまで初めて、想像していた通りに海を満喫できた気がします", "MessageTW": "託您的福，我第一次能和想像中一樣盡情享受大海", "MessageEN": "I feel like I really enjoyed myself."}, {"MessageId": 16005201, "Flag": 2, "Type": 0, "NextId": 16005202, "MessageType": "Text", "ImagePath": "", "MessageJP": "それも先生とだなんて", "MessageTW": "而且是和老師在一起", "MessageEN": "I think it might have been because of you, Sensei."}, {"MessageId": 16005202, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "すみません、今のは忘れてください", "MessageTW": "對不起，請忘我剛剛說的話", "MessageEN": "I'll talk to you again later."}]