[{"ImagePath": "https://schale.gg/images/student/collection/10019.webp", "MessageJP": "アズサ", "MessageTW": "梓", "MessageEN": "Azusa"}, {"MessageId": 100180170, "Flag": 2, "Type": 4, "NextId": 100190170, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100190170, "Flag": 2, "Type": 0, "NextId": 100190180, "MessageType": "Text", "ImagePath": "", "MessageJP": "こちらリス", "MessageTW": "這裡是松鼠", "MessageEN": "Squirrel. Please respond. Squirrel."}, {"MessageId": 100190180, "Flag": 1, "Type": 0, "NextId": 100190181, "MessageType": "Text", "ImagePath": "", "MessageJP": "ムササビ、応答せよ", "MessageTW": "鼯鼠，請回答", "MessageEN": "This is Chipmunk."}, {"MessageId": 100190181, "Flag": 2, "Type": 0, "NextId": 100190190, "MessageType": "Text", "ImagePath": "", "MessageJP": "今からのムササビの巣に向かう予定だ", "MessageTW": "現在打算去鼯鼠的巢穴", "MessageEN": "I'll be visiting Squirrel very soon."}, {"MessageId": 100190190, "Flag": 1, "Type": 0, "NextId": 100190200, "MessageType": "Text", "ImagePath": "", "MessageJP": "ムササビ、今は巣の中にいるのか？", "MessageTW": "鼯鼠現在在巢穴裡嗎？", "MessageEN": "Is Squirrel in the cave?"}, {"MessageId": 100190200, "Flag": 2, "Type": 3, "NextId": 100190210, "MessageType": "Text", "ImagePath": "", "MessageJP": "アズサはペンギンじゃなかったっけ……？", "MessageTW": "梓不是企鵝嗎……？", "MessageEN": "Aren't you Penguin, Azusa?"}, {"MessageId": 100190200, "Flag": 2, "Type": 3, "NextId": 100190210, "MessageType": "Text", "ImagePath": "", "MessageJP": "確かこちらはアヒルだったような……？", "MessageTW": "我記得我好像是鴨子耶……？", "MessageEN": "I'm not Squirrel. I'm Duck..."}, {"MessageId": 100190210, "Flag": 2, "Type": 0, "NextId": 100190211, "MessageType": "Text", "ImagePath": "", "MessageJP": "せんせ", "MessageTW": "老", "MessageEN": "Nah."}, {"MessageId": 100190211, "Flag": 2, "Type": 0, "NextId": 100190220, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや、ムササビ", "MessageTW": "不對，鼯鼠", "MessageEN": "Sensei...I mean, Squirrel."}, {"MessageId": 100190220, "Flag": 1, "Type": 0, "NextId": 100190221, "MessageType": "Text", "ImagePath": "", "MessageJP": "コードネームは、毎日変わるタイプのものもある", "MessageTW": "代號也有每天變換的類型", "MessageEN": "It's code. We're speaking in code."}, {"MessageId": 100190221, "Flag": 2, "Type": 0, "NextId": 100190230, "MessageType": "Text", "ImagePath": "", "MessageJP": "セキュリティ的な問題だ", "MessageTW": "這是安全上的問題", "MessageEN": "Code is meaningless if you don't change it up every day."}, {"MessageId": 100190230, "Flag": 1, "Type": 0, "NextId": 100190231, "MessageType": "Text", "ImagePath": "", "MessageJP": "席にいるのを確認した、15分後に向かう", "MessageTW": "確認在座位上，15分鐘後過去", "MessageEN": "I've confirmed that you're in place. I'll be there in 15 minutes."}, {"MessageId": 100190231, "Flag": 2, "Type": 0, "NextId": 100190232, "MessageType": "Text", "ImagePath": "", "MessageJP": "用件は到着してから伝えよう", "MessageTW": "到了再傳達要事給你", "MessageEN": "I'll disclose the details in person."}, {"MessageId": 100190232, "Flag": 2, "Type": 0, "NextId": 100190240, "MessageType": "Text", "ImagePath": "", "MessageJP": "ペンギンからは以上", "MessageTW": "企鵝完畢", "MessageEN": "Penguin."}, {"MessageId": 100190240, "Flag": 1, "Type": 0, "NextId": 100190241, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 100190241, "Flag": 2, "Type": 0, "NextId": 100190242, "MessageType": "Text", "ImagePath": "", "MessageJP": "訂正する", "MessageTW": "更正", "MessageEN": "Correction."}, {"MessageId": 100190242, "Flag": 2, "Type": 0, "NextId": 100180250, "MessageType": "Text", "ImagePath": "", "MessageJP": "リスからは以上。通信終わり", "MessageTW": "松鼠完畢，通訊結束", "MessageEN": "Squirrel."}, {"MessageId": 100180250, "Flag": 2, "Type": 2, "NextId": 100190250, "MessageType": "Text", "ImagePath": "", "MessageJP": "アズサの絆イベントへ", "MessageTW": "前往梓的羈絆劇情", "MessageEN": "Go to Azusa's Event"}, {"MessageId": 100190250, "Flag": 2, "Type": 0, "NextId": 100190260, "MessageType": "Text", "ImagePath": "", "MessageJP": "気持ちよさそうに寝てたから、静かにお暇させてもらった", "MessageTW": "看你睡得很舒服，我就悄悄離開了", "MessageEN": "I've confirmed the safety, so I've returned."}, {"MessageId": 100190260, "Flag": 1, "Type": 0, "NextId": 100190261, "MessageType": "Text", "ImagePath": "", "MessageJP": "セキュリティもそうだけど、少しは自分の健康も大事にした方が良い", "MessageTW": "安全固然重要，但也要照顧好自己的健康", "MessageEN": "Self-defense is important, but you have to take care of your health."}, {"MessageId": 100190261, "Flag": 2, "Type": 0, "NextId": 100190270, "MessageType": "Text", "ImagePath": "", "MessageJP": "頭に入れたら、「うん」とだけ応答せよ", "MessageTW": "懂了就回答「嗯」", "MessageEN": "If you understand, reply back with \"Yes.\" Squirrel."}, {"MessageId": 100190270, "Flag": 2, "Type": 3, "NextId": 100190280, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん", "MessageTW": "嗯", "MessageEN": "Fine..."}, {"MessageId": 100190280, "Flag": 2, "Type": 0, "NextId": 100190281, "MessageType": "Text", "ImagePath": "", "MessageJP": "よし、それではまた", "MessageTW": "很好，那之後再見", "MessageEN": "Good. I'll be waiting for training day."}, {"MessageId": 100190281, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "リスからは以上。通信終わり", "MessageTW": "松鼠完畢，通訊結束", "MessageEN": "That's all. Chipmunk."}]