[{"ImagePath": "https://schale.gg/images/student/collection/23007.webp", "MessageJP": "ハナコ", "MessageTW": "花子", "MessageEN": "Hanako"}, {"MessageId": 230060130, "Flag": 2, "Type": 4, "NextId": 230070130, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 230070130, "Flag": 2, "Type": 0, "NextId": 230070140, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今お時間大丈夫でしょうか？", "MessageTW": "老師，現在時間上方便嗎？", "MessageEN": "Are you free right now, Sensei?"}, {"MessageId": 230070140, "Flag": 1, "Type": 0, "NextId": 230070150, "MessageType": "Text", "ImagePath": "", "MessageJP": "少し困った状況になってしまいまして、助けていただきたく……", "MessageTW": "我遇到了有點麻煩的狀況，想請你幫幫我……", "MessageEN": "I'm in a bit of a pickle and was wondering if you could help me."}, {"MessageId": 230070150, "Flag": 2, "Type": 3, "NextId": 230070160, "MessageType": "Text", "ImagePath": "", "MessageJP": "困った状況？", "MessageTW": "麻煩狀況？", "MessageEN": "What's wrong?"}, {"MessageId": 230070150, "Flag": 2, "Type": 3, "NextId": 230070160, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "Did something happen?"}, {"MessageId": 230070160, "Flag": 2, "Type": 0, "NextId": 230070161, "MessageType": "Text", "ImagePath": "", "MessageJP": "何と言えば良いんでしょうか……", "MessageTW": "該怎麼說才好呢……", "MessageEN": "Hmm."}, {"MessageId": 230070161, "Flag": 2, "Type": 0, "NextId": 230070170, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと、体育倉庫から出られなくなってしまいまして……", "MessageTW": "我現在無法從體育倉庫出來……", "MessageEN": "I guess so. I'm locked inside the gym..."}, {"MessageId": 230070170, "Flag": 1, "Type": 0, "NextId": 230070171, "MessageType": "Text", "ImagePath": "", "MessageJP": "外からしか開けられないタイプの扉なので、来ていただけますとありがたいです", "MessageTW": "因為這扇門只能從外面打開，如果能來的話就太感謝了", "MessageEN": "The door opens from the outside, so you just have to come let me out."}, {"MessageId": 230070171, "Flag": 2, "Type": 0, "NextId": 230070180, "MessageType": "Text", "ImagePath": "", "MessageJP": "お願いできますでしょうか……？", "MessageTW": "可以拜託你嗎……？", "MessageEN": "Would that be okay?"}, {"MessageId": 230070180, "Flag": 2, "Type": 3, "NextId": 230070190, "MessageType": "Text", "ImagePath": "", "MessageJP": "すぐに行くね。", "MessageTW": "馬上過去唷", "MessageEN": "I'll be right there."}, {"MessageId": 230070180, "Flag": 2, "Type": 3, "NextId": 230070190, "MessageType": "Text", "ImagePath": "", "MessageJP": "分かった、急いで行くから落ち着いて待っててね。", "MessageTW": "知道了我馬上過去，你要冷靜地等我唷", "MessageEN": "Don't worry. I'll be right there."}, {"MessageId": 230070190, "Flag": 2, "Type": 0, "NextId": 230070191, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ふふっ、はい", "MessageTW": "……呵呵，好的", "MessageEN": "...Heehee."}, {"MessageId": 230070191, "Flag": 2, "Type": 0, "NextId": 230060200, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます、お待ちしておりますね", "MessageTW": "非常感謝，我等你", "MessageEN": "Thanks. I'll be waiting for you."}, {"MessageId": 230060200, "Flag": 2, "Type": 2, "NextId": 230070200, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハナコの絆イベントへ", "MessageTW": "前往花子的羈絆劇情", "MessageEN": "Go to Hanako's Event"}, {"MessageId": 230070200, "Flag": 2, "Type": 0, "NextId": 230070201, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 230070201, "Flag": 2, "Type": 0, "NextId": 230070210, "MessageType": "Text", "ImagePath": "", "MessageJP": "あの後、どこまで行かれてしまったんですか……？", "MessageTW": "在那之後，你跑去哪裡了呢……？", "MessageEN": "Where are you, Sensei?"}, {"MessageId": 230070210, "Flag": 1, "Type": 0, "NextId": 230070211, "MessageType": "Text", "ImagePath": "", "MessageJP": "とにかく、今日は本当にありがとうございました", "MessageTW": "總之，今天真的非常感謝", "MessageEN": "Are you listening? Thank you so much for today. I feel so much better."}, {"MessageId": 230070211, "Flag": 2, "Type": 0, "NextId": 230070220, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生が、どうお考えになっているかは分かりませんが……", "MessageTW": "雖然我不知道老師是怎麼想的……", "MessageEN": "I'm not sure how you feel about this, but..."}, {"MessageId": 230070220, "Flag": 1, "Type": 0, "NextId": 230070221, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 230070221, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "これは、本心からの言葉ですよ", "MessageTW": "但那確實是我的真心話", "MessageEN": "I'm being sincere.  "}]