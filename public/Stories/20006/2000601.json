[{"ImagePath": "https://schale.gg/images/student/collection/20006.webp", "MessageJP": "サヤ（私服）", "MessageTW": "沙耶(私服)", "MessageEN": "Saya (Casual)"}, {"MessageId": 200050010, "Flag": 2, "Type": 4, "NextId": 200060010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200060010, "Flag": 2, "Type": 0, "NextId": 200060020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今週末は時間ある？", "MessageTW": "老師，這個週末有時間嗎？", "MessageEN": "Are you free this weekend, Sensei?"}, {"MessageId": 200060020, "Flag": 2, "Type": 3, "NextId": 200060030, "MessageType": "Text", "ImagePath": "", "MessageJP": "今度は何の実験？", "MessageTW": "這次是什麼實驗？", "MessageEN": "Do I have to be a lab rat again?"}, {"MessageId": 200060020, "Flag": 2, "Type": 3, "NextId": 200060030, "MessageType": "Text", "ImagePath": "", "MessageJP": "変な薬はそろそろ勘弁……", "MessageTW": "別再叫我喝奇怪的藥了……", "MessageEN": "Please, no more experiments..."}, {"MessageId": 200060030, "Flag": 2, "Type": 0, "NextId": 200060040, "MessageType": "Text", "ImagePath": "", "MessageJP": "違う違う、そういうのじゃないのだ！", "MessageTW": "不是不是，不是那樣！", "MessageEN": "It's not like that!"}, {"MessageId": 200060040, "Flag": 1, "Type": 0, "NextId": 200060041, "MessageType": "Text", "ImagePath": "", "MessageJP": "この前先生が言ってた中華料理屋で新メニューが出るって聞いて、教えようと思っただけなのに！", "MessageTW": "我只是想告訴老師，上次老師說的中華料理店聽說有新菜色！", "MessageEN": "I just wanted to tell you that eastern restaurant you like is launching a new menu this weekend!"}, {"MessageId": 200060041, "Flag": 2, "Type": 0, "NextId": 200060050, "MessageType": "Text", "ImagePath": "", "MessageJP": "全部の用事が実験だと思われるのは心外なのだ！", "MessageTW": "所有事情都往實驗去想，真讓人心寒！", "MessageEN": "I'm trying to be nice, and this is how you repay me?"}, {"MessageId": 200060050, "Flag": 2, "Type": 3, "NextId": 200060060, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめんごめん、じゃあお詫びってわけでもないけど", "MessageTW": "抱歉抱歉，那麼就當作小小的賠罪", "MessageEN": "Sorry. I'm free this weekend."}, {"MessageId": 200060060, "Flag": 2, "Type": 3, "NextId": 200060070, "MessageType": "Text", "ImagePath": "", "MessageJP": "奢るよ、一緒に行こう", "MessageTW": "我請客，一起去吧", "MessageEN": "Let me make it up to you by picking up the tab."}, {"MessageId": 200060070, "Flag": 2, "Type": 0, "NextId": 200060071, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当か！もう今さら撤回は無しだぞ！？", "MessageTW": "真的嗎？事到如今不能反悔唷！？", "MessageEN": "Really? Don't forget about it!"}, {"MessageId": 200060071, "Flag": 2, "Type": 0, "NextId": 200050070, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ今週末、お店の前で待ち合わせなのだ！", "MessageTW": "那就這週末，在店的前面碰面！", "MessageEN": "I'll meet you there later!"}, {"MessageId": 200050070, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "サヤ（私服）の絆イベントへ", "MessageTW": "前往沙耶(私服)的羈絆劇情", "MessageEN": "Go to Saya (Casual)'s Event"}]