[{"ImagePath": "https://schale.gg/images/student/collection/10007.webp", "MessageJP": "マキ", "MessageTW": "真紀", "MessageEN": "Maki"}, {"MessageId": 100060010, "Flag": 2, "Type": 4, "NextId": 100070019, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100070019, "Flag": 2, "Type": 0, "NextId": 100070020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、ごめん……今ちょっと時間大丈夫？", "MessageTW": "老師，抱歉……可以耽誤一點時間嗎？", "MessageEN": "Sensei, I'm sorry...but do you have a minute?"}, {"MessageId": 100070020, "Flag": 2, "Type": 3, "NextId": 100070030, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫だよ", "MessageTW": "沒問題唷", "MessageEN": "Sure."}, {"MessageId": 100070020, "Flag": 2, "Type": 3, "NextId": 100070030, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "What's up?"}, {"MessageId": 100070030, "Flag": 2, "Type": 0, "NextId": 100070037, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……あたしのこと、迎えに来てくれない？", "MessageTW": "那個……能來接我嗎？", "MessageEN": "Um, can you come pick me up?"}, {"MessageId": 100070037, "Flag": 2, "Type": 0, "NextId": 100070044, "MessageType": "Text", "ImagePath": "", "MessageJP": "身元保証人が必要みたいなんだけど、生徒会には連絡しづらくって……", "MessageTW": "說是需要身份保證人，但不太好聯絡學生會……", "MessageEN": "They said I need a guarantor to get out of here, but I don't want to contact the Student Council."}, {"MessageId": 100070044, "Flag": 1, "Type": 0, "NextId": 100070050, "MessageType": "Text", "ImagePath": "", "MessageJP": "場所はその、えっと……交番", "MessageTW": "地點是那個，這個……派出所", "MessageEN": "Right now, I'm...at a police station."}, {"MessageId": 100070050, "Flag": 2, "Type": 3, "NextId": 100060060, "MessageType": "Text", "ImagePath": "", "MessageJP": "交番！？", "MessageTW": "派出所！？", "MessageEN": "A police station?!"}, {"MessageId": 100060060, "Flag": 2, "Type": 2, "NextId": 100070068, "MessageType": "Text", "ImagePath": "", "MessageJP": "マキの絆イベントへ", "MessageTW": "前往真紀的羈絆劇情", "MessageEN": "Go to Maki's Event"}, {"MessageId": 100070068, "Flag": 2, "Type": 0, "NextId": 100070065, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は来てくれてありがと、先生", "MessageTW": "今天謝謝你來，老師", "MessageEN": "Thank you for your help today, Sensei."}, {"MessageId": 100070065, "Flag": 2, "Type": 0, "NextId": 100070070, "MessageType": "Text", "ImagePath": "", "MessageJP": "生徒会に連絡してたら、危うく二重で小言の嵐になるところだったよ", "MessageTW": "聯絡學生會的話，又要被唸一頓", "MessageEN": "I thought I was gonna get dragged into the Millennium Student Council."}, {"MessageId": 100070070, "Flag": 2, "Type": 3, "NextId": 100070089, "MessageType": "Text", "ImagePath": "", "MessageJP": "これからは気を付けてね", "MessageTW": "從今以後要注意唷", "MessageEN": "Don't go causing any more trouble in the city."}, {"MessageId": 100070089, "Flag": 2, "Type": 0, "NextId": 100070096, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん。公共の場では、グラフィティはやらないようにする……", "MessageTW": "嗯。盡量不在公共場所塗鴉……", "MessageEN": "Okay. From now on, I won't tag in public..."}, {"MessageId": 100070096, "Flag": 1, "Type": 0, "NextId": 100070093, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日約束した通り、今度は大きなキャンバスで一緒に絵を描こうね！", "MessageTW": "按照今天約定好的，下次要一起在大畫布上作畫喔！", "MessageEN": "Next time, we'll paint on a canvas. Together!"}, {"MessageId": 100070093, "Flag": 2, "Type": 0, "NextId": 100070100, "MessageType": "Text", "ImagePath": "", "MessageJP": "約束だよ！", "MessageTW": "約好囉！", "MessageEN": "Promise me! Do you promise?"}, {"MessageId": 100070100, "Flag": 2, "Type": 3, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、約束", "MessageTW": "嗯，約好了", "MessageEN": "Okay, promise."}]