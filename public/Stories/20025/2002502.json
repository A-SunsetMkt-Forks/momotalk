[{"ImagePath": "https://schale.gg/images/student/collection/20025.webp", "MessageJP": "ハルカ（正月）", "MessageTW": "遙香(正月)", "MessageEN": "Haruka (New Year)"}, {"MessageId": 200240080, "Flag": 2, "Type": 4, "NextId": 200250080, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200250080, "Flag": 2, "Type": 0, "NextId": 200250090, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 200250090, "Flag": 1, "Type": 0, "NextId": 200250100, "MessageType": "Text", "ImagePath": "", "MessageJP": "その", "MessageTW": "那個", "MessageEN": "So..."}, {"MessageId": 200250100, "Flag": 1, "Type": 0, "NextId": 200250110, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと", "MessageTW": "呃", "MessageEN": "Um..."}, {"MessageId": 200250110, "Flag": 1, "Type": 0, "NextId": 200250120, "MessageType": "Text", "ImagePath": "", "MessageJP": "私を探していたと聞いて", "MessageTW": "聽說您在找我", "MessageEN": "You were looking...for me?"}, {"MessageId": 200250120, "Flag": 2, "Type": 3, "NextId": 200250130, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、連絡ありがとう", "MessageTW": "嗯，謝謝你聯絡我", "MessageEN": "Oh, Haruka! Yes."}, {"MessageId": 200250120, "Flag": 2, "Type": 3, "NextId": 200250130, "MessageType": "Text", "ImagePath": "", "MessageJP": "今、時間大丈夫？", "MessageTW": "現在有空嗎？", "MessageEN": "Is now a good time to meet?"}, {"MessageId": 200250130, "Flag": 2, "Type": 0, "NextId": 200250140, "MessageType": "Text", "ImagePath": "", "MessageJP": "え？", "MessageTW": "咦？", "MessageEN": "Huh?"}, {"MessageId": 200250140, "Flag": 1, "Type": 0, "NextId": 200250141, "MessageType": "Text", "ImagePath": "", "MessageJP": "あっはい今すぐ行きます", "MessageTW": "好的現在馬上過去", "MessageEN": "Oh, I'll be right over!"}, {"MessageId": 200250141, "Flag": 2, "Type": 0, "NextId": 200250150, "MessageType": "Text", "ImagePath": "", "MessageJP": "私が伺いますので先生は待っていてください", "MessageTW": "我會去找老師的請老師稍等", "MessageEN": "Don't worry! I'll come to you!"}, {"MessageId": 200250150, "Flag": 1, "Type": 0, "NextId": 200240160, "MessageType": "Text", "ImagePath": "", "MessageJP": "呼んでくださってありがとうございます", "MessageTW": "謝謝您找我過去", "MessageEN": "Thank you very much for contacting me!"}, {"MessageId": 200240160, "Flag": 2, "Type": 2, "NextId": 200250160, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハルカ（正月）の絆イベントへ", "MessageTW": "前往遙香(正月)的羈絆劇情", "MessageEN": "Go to Haruka (New Year)'s Event"}, {"MessageId": 200250160, "Flag": 2, "Type": 0, "NextId": 200250170, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます、先生。お陰で家に帰るまで暖かかったです", "MessageTW": "謝謝您，老師。托您的福溫暖地回到家了", "MessageEN": "Thank you, Sensei. I got back safely, thanks to you."}, {"MessageId": 200250170, "Flag": 2, "Type": 3, "NextId": 200250180, "MessageType": "Text", "ImagePath": "", "MessageJP": "そっか。よかった", "MessageTW": "這樣啊。太好了", "MessageEN": "Glad to hear it."}, {"MessageId": 200250180, "Flag": 2, "Type": 0, "NextId": 200250190, "MessageType": "Text", "ImagePath": "", "MessageJP": "いただいたカイロは特殊加工して金庫に入れました。家宝として命に替えても守り抜きます", "MessageTW": "我將您給我的暖暖包做了特殊加工後放進保險箱裡了。我會將其視為傳家之寶拚命守護它", "MessageEN": "Also I saved the hot pack you gave me and I put it into a vault to keep as an heirloom and I promise to protect it with my life."}, {"MessageId": 200250190, "Flag": 2, "Type": 3, "NextId": 200250200, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん", "MessageTW": "嗯", "MessageEN": "Uh huh."}, {"MessageId": 200250200, "Flag": 2, "Type": 3, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "……んん？", "MessageTW": "……嗯？", "MessageEN": "Huh?"}]