[{"ImagePath": "https://schale.gg/images/student/collection/13011.webp", "MessageJP": "モモイ", "MessageTW": "桃井", "MessageEN": "Momoi"}, {"MessageId": 130100090, "Flag": 2, "Type": 4, "NextId": 130110090, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 130110090, "Flag": 2, "Type": 0, "NextId": 130110100, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生ー", "MessageTW": "老師──", "MessageEN": "Sensei..."}, {"MessageId": 130110100, "Flag": 1, "Type": 0, "NextId": 130110110, "MessageType": "Text", "ImagePath": "", "MessageJP": "最高のゲームシナリオの条件って何だと思う？", "MessageTW": "你覺得最棒的遊戲劇本要具備哪些條件？", "MessageEN": "What do you think you need for the perfect story in a retro game?"}, {"MessageId": 130110110, "Flag": 2, "Type": 3, "NextId": 130110120, "MessageType": "Text", "ImagePath": "", "MessageJP": "最高のゲームシナリオ……？", "MessageTW": "最棒的遊戲劇本……？", "MessageEN": "A perfect game story...?"}, {"MessageId": 130110110, "Flag": 2, "Type": 3, "NextId": 130110120, "MessageType": "Text", "ImagePath": "", "MessageJP": "急にどうしたの？", "MessageTW": "怎麼突然這麼問呢？", "MessageEN": "...What's the problem?"}, {"MessageId": 130110120, "Flag": 2, "Type": 0, "NextId": 130110121, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは……", "MessageTW": "因為……", "MessageEN": "It's nothing..."}, {"MessageId": 130110121, "Flag": 2, "Type": 0, "NextId": 130110122, "MessageType": "Text", "ImagePath": "", "MessageJP": "ああっ、思い出したらイライラしてきた！", "MessageTW": "啊啊！光回想起來就覺得好生氣！", "MessageEN": "Those dummies! "}, {"MessageId": 130110122, "Flag": 2, "Type": 0, "NextId": 130110130, "MessageType": "Text", "ImagePath": "", "MessageJP": "あいつら、好き放題言ってくれちゃって！", "MessageTW": "那些傢伙竟敢在那邊亂講話！", "MessageEN": "They're just blabbing away!"}, {"MessageId": 130110130, "Flag": 1, "Type": 0, "NextId": 130110131, "MessageType": "Text", "ImagePath": "", "MessageJP": "発想が古いとか、展開が中だるみしてるだとか", "MessageTW": "說什麼想法過時，什麼劇情節奏崩壞", "MessageEN": "The story is boring, the game mechanics aren't tight... Yada, yada, yada..."}, {"MessageId": 130110131, "Flag": 2, "Type": 0, "NextId": 130110140, "MessageType": "Text", "ImagePath": "", "MessageJP": "もうダメ！先生、今からゲーム開発部の部室に来て！", "MessageTW": "這樣不行！老師，現在到遊戲開發部的社團教室來！", "MessageEN": "I can't stand it. Meet me in the Game Development Department club room, Sensei!"}, {"MessageId": 130110140, "Flag": 2, "Type": 3, "NextId": 130100141, "MessageType": "Text", "ImagePath": "", "MessageJP": "……？", "MessageTW": "……？", "MessageEN": "Huh?"}, {"MessageId": 130100141, "Flag": 2, "Type": 2, "NextId": 130110148, "MessageType": "Text", "ImagePath": "", "MessageJP": "モモイの絆イベントへ", "MessageTW": "前往桃井的羈絆劇情", "MessageEN": "Go to Momoi's Event"}, {"MessageId": 130110148, "Flag": 2, "Type": 0, "NextId": 130110145, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしよう先生！？", "MessageTW": "怎麼辦，老師！？", "MessageEN": "What do I do, Sensei?!"}, {"MessageId": 130110145, "Flag": 2, "Type": 0, "NextId": 130110142, "MessageType": "Text", "ImagePath": "", "MessageJP": "せっかく浮かんだアイデアが思いっきり矛盾してて、", "MessageTW": "好不容易浮現的靈感整個大矛盾", "MessageEN": "My ideas are all so uninspired..."}, {"MessageId": 130110142, "Flag": 2, "Type": 0, "NextId": 130110150, "MessageType": "Text", "ImagePath": "", "MessageJP": "それを打破する次のアイデアが全然浮かばない！助けてーー！", "MessageTW": "完全想不出下一個靈感來解困！救救我──！", "MessageEN": "I can't think of anything else! Please help me!"}, {"MessageId": 130110150, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "うわあぁぁぁん！！！", "MessageTW": "嗚哇啊！！！", "MessageEN": "Waaahhh!"}]