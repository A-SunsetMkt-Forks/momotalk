[{"ImagePath": "https://schale.gg/images/student/collection/10056.webp", "MessageJP": "セリナ（クリスマス）", "MessageTW": "芹奈(聖誕節)", "MessageEN": "Serina (Christmas)"}, {"MessageId": 100550180, "Flag": 2, "Type": 4, "NextId": 100560180, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100560180, "Flag": 2, "Type": 0, "NextId": 100560181, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、お身体は大丈夫ですか？", "MessageTW": "老師，身體還好嗎？", "MessageEN": "How are you doing, Sensei?"}, {"MessageId": 100560181, "Flag": 2, "Type": 0, "NextId": 100560190, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は私、ボランティアでとある病院に行くんです", "MessageTW": "我今天要去某個醫院參加慈善活動", "MessageEN": "We are holding a charity event at the hospital today."}, {"MessageId": 100560190, "Flag": 1, "Type": 0, "NextId": 100560200, "MessageType": "Text", "ImagePath": "", "MessageJP": "患者の皆さんにはよくお会いしているのですが、今回はサンタとして行くので、少し緊張しますね", "MessageTW": "雖然常常會見到那些病患，但這次要扮成聖誕老人去，所以有點緊張", "MessageEN": "The patients all know me, so I'm not too sure how they'll react to me showing up in a Santa costume."}, {"MessageId": 100560200, "Flag": 2, "Type": 3, "NextId": 100560210, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうなんだ。それ、私も行ってもいい？", "MessageTW": "這樣啊。那個活動，我也可以去嗎？", "MessageEN": "Can I come? To the event?"}, {"MessageId": 100560210, "Flag": 2, "Type": 0, "NextId": 100560211, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい？", "MessageTW": "嗯？", "MessageEN": "Wh-What?"}, {"MessageId": 100560211, "Flag": 2, "Type": 0, "NextId": 100560220, "MessageType": "Text", "ImagePath": "", "MessageJP": "だ、ダメではないのですが……", "MessageTW": "雖，雖然不是不行……", "MessageEN": "I-I mean... I suppose there's no reason you can't."}, {"MessageId": 100560220, "Flag": 1, "Type": 0, "NextId": 100560230, "MessageType": "Text", "ImagePath": "", "MessageJP": "急にどうかされましたか？", "MessageTW": "怎麼這麼突然呢？", "MessageEN": "But how come? All of a sudden?"}, {"MessageId": 100560230, "Flag": 1, "Type": 0, "NextId": 100550230, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、先生？", "MessageTW": "那個，老師？", "MessageEN": "Sensei?"}, {"MessageId": 100550230, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "セリナ（クリスマス）の絆イベントへ", "MessageTW": "前往芹奈(聖誕節)的羈絆劇情", "MessageEN": "Go to Serina (Christmas)'s Event"}]