[{"ImagePath": "https://schale.gg/images/student/collection/13003.webp", "MessageJP": "ハスミ", "MessageTW": "蓮實", "MessageEN": "Hasumi"}, {"MessageId": 130020140, "Flag": 2, "Type": 4, "NextId": 130030147, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 130030147, "Flag": 2, "Type": 0, "NextId": 130030154, "MessageType": "Text", "ImagePath": "", "MessageJP": "時々、ひとりの時間が恋しくなることってありませんか？", "MessageTW": "有時候，不是會想念自己獨處的時間嗎？", "MessageEN": "Sometimes, it's nice to be by yourself."}, {"MessageId": 130030154, "Flag": 1, "Type": 0, "NextId": 130030160, "MessageType": "Text", "ImagePath": "", "MessageJP": "特段孤独が好きなわけではないのですが、時折そういう気持ちになるんです", "MessageTW": "雖然不是特別喜歡孤獨，但偶爾也有這樣的心情", "MessageEN": "It's not like I enjoy solitude more than others or anything."}, {"MessageId": 130030160, "Flag": 2, "Type": 3, "NextId": 130030175, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういう時もあるよね、学生の頃は特に", "MessageTW": "有這樣的時候呢，特別是當學生的時期", "MessageEN": "You seem sensitive by nature."}, {"MessageId": 130030160, "Flag": 2, "Type": 3, "NextId": 130030175, "MessageType": "Text", "ImagePath": "", "MessageJP": "思春期の乙女だね！良いと思う！", "MessageTW": "青春期的少女呢！我覺得很好！", "MessageEN": "You're pretty sensitive, right, Hasumi?"}, {"MessageId": 130030175, "Flag": 2, "Type": 0, "NextId": 130030172, "MessageType": "Text", "ImagePath": "", "MessageJP": "え、あ、その……", "MessageTW": "咦，啊，那個……", "MessageEN": "H-Huh?!"}, {"MessageId": 130030172, "Flag": 2, "Type": 0, "NextId": 130030189, "MessageType": "Text", "ImagePath": "", "MessageJP": "すみません、何だか恥ずかしくなってしまいました……", "MessageTW": "不好意思，總覺得開始感到有點羞恥……", "MessageEN": "D-Do you think so?"}, {"MessageId": 130030189, "Flag": 1, "Type": 0, "NextId": 130020190, "MessageType": "Text", "ImagePath": "", "MessageJP": "自分から言い出しておいてごめんなさい……", "MessageTW": "明明是自己先提起的，真是抱歉……", "MessageEN": "I feel kind of embarrassed... I'm sorry."}, {"MessageId": 130020190, "Flag": 2, "Type": 2, "NextId": 130030196, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハスミの絆イベントへ", "MessageTW": "前往蓮實的羈絆劇情", "MessageEN": "Go to Hasumi's Event"}, {"MessageId": 130030196, "Flag": 2, "Type": 0, "NextId": 130030193, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日もお疲れ様でした", "MessageTW": "今天也辛苦了", "MessageEN": "You did a lot today."}, {"MessageId": 130030193, "Flag": 2, "Type": 0, "NextId": 130030200, "MessageType": "Text", "ImagePath": "", "MessageJP": "なんだか今日は、何もしていないような気もしますが", "MessageTW": "總覺得今天沒做什麼事", "MessageEN": "I don't feel like I did anything."}, {"MessageId": 130030200, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "素敵な時間を過ごせました、ありがとうございます", "MessageTW": "但度過了美好的時光，非常感謝", "MessageEN": "It was more than enough. Thank you."}]