[{"ImagePath": "https://schale.gg/images/student/collection/10034.webp", "MessageJP": "ミモリ", "MessageTW": "三森", "MessageEN": "Mimori"}, {"MessageId": 100330280, "Flag": 2, "Type": 4, "NextId": 100340280, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100340280, "Flag": 2, "Type": 0, "NextId": 100340290, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今日お時間ありますでしょうか？", "MessageTW": "老師，請問今天有時間嗎？", "MessageEN": "Are you free today, Sensei?"}, {"MessageId": 100340290, "Flag": 2, "Type": 3, "NextId": 100340300, "MessageType": "Text", "ImagePath": "", "MessageJP": "ミモリのためならいつでも大丈夫だよ", "MessageTW": "如果是為了三森的話隨時都沒問題唷", "MessageEN": "I'm always free for you, Mimori."}, {"MessageId": 100340290, "Flag": 2, "Type": 3, "NextId": 100340310, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了嗎？", "MessageEN": "Yeah, what's up?"}, {"MessageId": 100340300, "Flag": 2, "Type": 0, "NextId": 100340301, "MessageType": "Text", "ImagePath": "", "MessageJP": "あっ、そ、そうなんですか……？", "MessageTW": "啊，是，是那樣嗎……？", "MessageEN": "You are...?"}, {"MessageId": 100340301, "Flag": 2, "Type": 0, "NextId": 100340320, "MessageType": "Text", "ImagePath": "", "MessageJP": "そう言ってくださると、その……すごく嬉しいです。", "MessageTW": "聽到老師那麼說，那個……我非常開心", "MessageEN": "Good to know... That makes me happy."}, {"MessageId": 100340310, "Flag": 1, "Type": 0, "NextId": 100340320, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、実は……", "MessageTW": "那個，其實……", "MessageEN": "Oh, it's nothing too serious."}, {"MessageId": 100340320, "Flag": 1, "Type": 0, "NextId": 100340321, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと、百貨店で買いたいものがありまして。", "MessageTW": "稍微，在百貨店有想買的東西", "MessageEN": "There's something I've had my eye on at the store."}, {"MessageId": 100340321, "Flag": 2, "Type": 0, "NextId": 100340322, "MessageType": "Text", "ImagePath": "", "MessageJP": "ぜひ先生と一緒に行ければと思ってご連絡したのですが……", "MessageTW": "想務必請老師和我一起去所以聯絡了老師……", "MessageEN": "I was hoping you would come with me."}, {"MessageId": 100340322, "Flag": 2, "Type": 0, "NextId": 100340330, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、ご迷惑だったりしませんでしょうか……？", "MessageTW": "那個，請問有造成困擾嗎……？", "MessageEN": "Is that okay?"}, {"MessageId": 100340330, "Flag": 2, "Type": 3, "NextId": 100340340, "MessageType": "Text", "ImagePath": "", "MessageJP": "全っ然大丈夫", "MessageTW": "完──全沒問題", "MessageEN": "Yeah, sure."}, {"MessageId": 100340330, "Flag": 2, "Type": 3, "NextId": 100340340, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつでも呼んで", "MessageTW": "隨時都能找我", "MessageEN": "I'll be there."}, {"MessageId": 100340340, "Flag": 2, "Type": 0, "NextId": 100340341, "MessageType": "Text", "ImagePath": "", "MessageJP": "はいっ、ありがとうございます！", "MessageTW": "好的，謝謝老師！", "MessageEN": "Really? Okay!"}, {"MessageId": 100340341, "Flag": 2, "Type": 0, "NextId": 100330340, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、百貨店の前でお待ちしておりますね。", "MessageTW": "那麼，我在百貨店前等你哦", "MessageEN": "I'll meet you at the department store!"}, {"MessageId": 100330340, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ミモリの絆イベントへ", "MessageTW": "前往三森的羈絆劇情", "MessageEN": "Go to Mimori's Event"}]