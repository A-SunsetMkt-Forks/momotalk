[{"ImagePath": "https://schale.gg/images/student/collection/10061.webp", "MessageJP": "サクラコ", "MessageTW": "櫻子", "MessageEN": "Sakurako"}, {"MessageId": 100600510, "Flag": 2, "Type": 4, "NextId": 100610510, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100610510, "Flag": 2, "Type": 0, "NextId": 100610520, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生", "MessageTW": "您好，老師", "MessageEN": "Sensei."}, {"MessageId": 100610520, "Flag": 1, "Type": 0, "NextId": 100610530, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……問題が起きてしまいました", "MessageTW": "那個……發生了一些問題", "MessageEN": "I have a problem."}, {"MessageId": 100610530, "Flag": 2, "Type": 3, "NextId": 100610540, "MessageType": "Text", "ImagePath": "", "MessageJP": "問題？", "MessageTW": "問題？", "MessageEN": "A problem?"}, {"MessageId": 100610530, "Flag": 2, "Type": 3, "NextId": 100610540, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "What's wrong?"}, {"MessageId": 100610540, "Flag": 2, "Type": 0, "NextId": 100610550, "MessageType": "Text", "ImagePath": "", "MessageJP": "意図したわけではなかったのですが……", "MessageTW": "雖然並非如我所願……", "MessageEN": "It was entirely unintentional, but..."}, {"MessageId": 100610550, "Flag": 1, "Type": 0, "NextId": 100610551, "MessageType": "Text", "ImagePath": "", "MessageJP": "……なんとか、先生の力を", "MessageTW": "……不過我還是希望", "MessageEN": "It's become something of a habit."}, {"MessageId": 100610551, "Flag": 2, "Type": 0, "NextId": 100600560, "MessageType": "Text", "ImagePath": "", "MessageJP": "お借りできないかと思いまして", "MessageTW": "能夠借用老師的力量", "MessageEN": "I need your help with it."}, {"MessageId": 100600560, "Flag": 2, "Type": 2, "NextId": 100610560, "MessageType": "Text", "ImagePath": "", "MessageJP": "サクラコの絆イベントへ", "MessageTW": "前往櫻子的羈絆劇情", "MessageEN": "Go to Sakurako's Event"}, {"MessageId": 100610560, "Flag": 2, "Type": 0, "NextId": 100610570, "MessageType": "Text", "ImagePath": "", "MessageJP": "申し訳ありません……せっかくお時間を頂いたのに", "MessageTW": "非常抱歉……明明占用了老師的時間", "MessageEN": "I'm sorry. I know you worked very hard..."}, {"MessageId": 100610570, "Flag": 1, "Type": 0, "NextId": 100610580, "MessageType": "Text", "ImagePath": "", "MessageJP": "何が問題だったのでしょうか……思っていたよりも、難しいですね", "MessageTW": "問題究竟出在哪裡呢……比想像中還困難呢", "MessageEN": "I don't know what was wrong. Maybe I was asking too much."}, {"MessageId": 100610580, "Flag": 2, "Type": 3, "NextId": 100610590, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、ごめんね", "MessageTW": "那個，抱歉", "MessageEN": "Oh, um... I see."}, {"MessageId": 100610580, "Flag": 2, "Type": 3, "NextId": 100610590, "MessageType": "Text", "ImagePath": "", "MessageJP": "お役に立てなくて", "MessageTW": "沒能幫上你的忙", "MessageEN": "I'm sorry I couldn't help."}, {"MessageId": 100610590, "Flag": 2, "Type": 0, "NextId": 100610591, "MessageType": "Text", "ImagePath": "", "MessageJP": "そんなことありません、大変助かりました", "MessageTW": "別這麼說，您已經幫了我很大的忙了", "MessageEN": "Not at all. You were still very helpful in a different way."}, {"MessageId": 100610591, "Flag": 2, "Type": 0, "NextId": 100610592, "MessageType": "Text", "ImagePath": "", "MessageJP": "少なくとも以前よりは確実に", "MessageTW": "至少和之前相比", "MessageEN": "At least, it seems like maybe..."}, {"MessageId": 100610592, "Flag": 2, "Type": 0, "NextId": 100610600, "MessageType": "Text", "ImagePath": "", "MessageJP": "気難しい人だという印象は減ったようですし", "MessageTW": "覺得我是個難相處的人的印象確實減少了", "MessageEN": "They're a little more comfortable than before."}, {"MessageId": 100610600, "Flag": 1, "Type": 0, "NextId": 100610610, "MessageType": "Text", "ImagePath": "", "MessageJP": "その分、つかみどころのない、", "MessageTW": "雖然，是個難以捉摸的、", "MessageEN": "But I feel like"}, {"MessageId": 100610610, "Flag": 1, "Type": 0, "NextId": 100610620, "MessageType": "Text", "ImagePath": "", "MessageJP": "変わった人という印象が……", "MessageTW": "奇怪的人的印象似乎……", "MessageEN": "I've become someone"}, {"MessageId": 100610620, "Flag": 1, "Type": 0, "NextId": 100610630, "MessageType": "Text", "ImagePath": "", "MessageJP": "できてしまったようですが", "MessageTW": "已經形成了", "MessageEN": "unpredictable and weird to them."}, {"MessageId": 100610630, "Flag": 2, "Type": 3, "NextId": 100610640, "MessageType": "Text", "ImagePath": "", "MessageJP": "ひとまず一歩前進かな？", "MessageTW": "也算是向前邁進了一步吧？", "MessageEN": "Well, it's a first step."}, {"MessageId": 100610630, "Flag": 2, "Type": 3, "NextId": 100610640, "MessageType": "Text", "ImagePath": "", "MessageJP": "一つずつ解決していこう", "MessageTW": "一步步解決吧", "MessageEN": "Let's take it step-by-step."}, {"MessageId": 100610640, "Flag": 2, "Type": 0, "NextId": 100610650, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい……", "MessageTW": "好的……", "MessageEN": "Okay."}, {"MessageId": 100610650, "Flag": 1, "Type": 0, "NextId": 100610660, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当に、ありがとうございました", "MessageTW": "真的非常感謝您", "MessageEN": "Again, thank you so much."}, {"MessageId": 100610660, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "これからも……どうぞよろしくお願いします", "MessageTW": "今後……也請多多指教", "MessageEN": "I'll look to you again if I need help."}]