[{"ImagePath": "https://schale.gg/images/student/collection/10000.webp", "MessageJP": "アル", "MessageTW": "亞瑠", "MessageEN": "Aru"}, {"MessageId": 99990250, "Flag": 2, "Type": 4, "NextId": 100000250, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100000250, "Flag": 2, "Type": 0, "NextId": 100000260, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生 ", "MessageTW": "老師", "MessageEN": "Sensei. "}, {"MessageId": 100000260, "Flag": 1, "Type": 0, "NextId": 100000270, "MessageType": "Text", "ImagePath": "", "MessageJP": "あらためてだけど……私たち便利屋一同、先生に心から感謝しているわ", "MessageTW": "雖然先前說過了……我們便利屋全體，衷心的感謝老師", "MessageEN": "We at Problem Solver 68 are grateful to you."}, {"MessageId": 100000270, "Flag": 2, "Type": 3, "NextId": 100000280, "MessageType": "Text", "ImagePath": "", "MessageJP": "そこまで言われると照れちゃうな", "MessageTW": "說成這樣真讓人害羞啊", "MessageEN": "I'm a little embarrassed to hear that from you, Aru."}, {"MessageId": 100000280, "Flag": 2, "Type": 0, "NextId": 100000281, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふふっ、先生らしくないわね", "MessageTW": "呵呵呵，真不像老師呢", "MessageEN": "Heehee. Come on, don't be embarrassed."}, {"MessageId": 100000281, "Flag": 2, "Type": 0, "NextId": 100000290, "MessageType": "Text", "ImagePath": "", "MessageJP": "この程度で恥ずかしがっていたら、この先やっていけないわよ", "MessageTW": "這種程度就害羞的話，未來就很難做下去了唷", "MessageEN": "That's unlike you."}, {"MessageId": 100000290, "Flag": 2, "Type": 3, "NextId": 100000300, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ攻守交替、アルにも恥ずかしがってもらおう", "MessageTW": "那就攻守交換，讓亞瑠也感到害羞吧", "MessageEN": "I don't want to be embarrassed alone, so I'll have to make you feel the same way!"}, {"MessageId": 100000300, "Flag": 2, "Type": 0, "NextId": 100000310, "MessageType": "Text", "ImagePath": "", "MessageJP": "どういうこと！？！？", "MessageTW": "什麼意思！？！？", "MessageEN": "Wh-What are you talking about?!"}, {"MessageId": 100000310, "Flag": 2, "Type": 3, "NextId": 100000320, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめん、言ってみたかっただけ", "MessageTW": "抱歉，只是說說而已", "MessageEN": "Well, I'm just kidding."}, {"MessageId": 100000320, "Flag": 2, "Type": 0, "NextId": 100000330, "MessageType": "Text", "ImagePath": "", "MessageJP": "そ、そう……", "MessageTW": "是，是嗎……", "MessageEN": "*sigh* You really surprise me sometimes when I talk to you."}, {"MessageId": 100000330, "Flag": 1, "Type": 0, "NextId": 100000331, "MessageType": "Text", "ImagePath": "", "MessageJP": "とにかく本題なのだけど、普段お世話になってるお礼として", "MessageTW": "總之進入正題，作為平常受到關照的回禮", "MessageEN": "A-Anyway! I've been wanting to repay you for all the help..."}, {"MessageId": 100000331, "Flag": 2, "Type": 0, "NextId": 100000340, "MessageType": "Text", "ImagePath": "", "MessageJP": "食事でも一緒に、どうかしら？", "MessageTW": "一起吃頓飯，如何呢？", "MessageEN": "I want to invite you out. What do you think?"}, {"MessageId": 100000340, "Flag": 2, "Type": 3, "NextId": 100000350, "MessageType": "Text", "ImagePath": "", "MessageJP": "ぜひ！", "MessageTW": "務必！", "MessageEN": "I accept, of course."}, {"MessageId": 100000350, "Flag": 2, "Type": 0, "NextId": 100000351, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは良かったわ", "MessageTW": "那太好了", "MessageEN": "Heeheehee, really?"}, {"MessageId": 100000351, "Flag": 2, "Type": 0, "NextId": 99990350, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ待ってるわよ、先生", "MessageTW": "那我等著唷，老師", "MessageEN": "Then, I'll be waiting, Sensei."}, {"MessageId": 99990350, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "アルの絆イベントへ", "MessageTW": "前往亞瑠的羈絆劇情", "MessageEN": "Go to Aru's Event"}]