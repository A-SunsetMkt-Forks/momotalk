[{"ImagePath": "https://schale.gg/images/student/collection/23005.webp", "MessageJP": "アヤネ", "MessageTW": "綾音", "MessageEN": "Ayane"}, {"MessageId": 230040010, "Flag": 2, "Type": 4, "NextId": 230050018, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 230050018, "Flag": 2, "Type": 0, "NextId": 230050025, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生。アヤネです", "MessageTW": "你好，老師。我是綾音", "MessageEN": "Hello, Sensei. This is Ayane."}, {"MessageId": 230050025, "Flag": 1, "Type": 0, "NextId": 230050030, "MessageType": "Text", "ImagePath": "", "MessageJP": "少し、お時間よろしいでしょうか？", "MessageTW": "可以占用你一點時間嗎？", "MessageEN": "Do you happen to be free right now?"}, {"MessageId": 230050030, "Flag": 2, "Type": 3, "NextId": 230050046, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫だよ、どうしたの？", "MessageTW": "沒問題唷，怎麼了？", "MessageEN": "Sure. What's going on?"}, {"MessageId": 230050030, "Flag": 2, "Type": 3, "NextId": 230050053, "MessageType": "Text", "ImagePath": "", "MessageJP": "それなりに忙しいけど、アヤネのためなら全然大丈夫！", "MessageTW": "雖然很忙，但為了綾音的話完全沒問題！", "MessageEN": "I'm very busy, but I can make time for you."}, {"MessageId": 230050046, "Flag": 2, "Type": 0, "NextId": 230050067, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます、実は……", "MessageTW": "非常感謝，事實上……", "MessageEN": "Okay, it's nothing serious, but..."}, {"MessageId": 230050053, "Flag": 1, "Type": 0, "NextId": 230050050, "MessageType": "Text", "ImagePath": "", "MessageJP": "え、えっと、あはは……。あ、ありがとうございます", "MessageTW": "那，那個，啊哈哈……非，非常感謝", "MessageEN": "Oh... Thank you."}, {"MessageId": 230050050, "Flag": 2, "Type": 0, "NextId": 230050067, "MessageType": "Text", "ImagePath": "", "MessageJP": "実は……", "MessageTW": "其實……", "MessageEN": "It's nothing serious, but..."}, {"MessageId": 230050067, "Flag": 1, "Type": 0, "NextId": 230050064, "MessageType": "Text", "ImagePath": "", "MessageJP": "ご相談と言いますか、聞いていただきたいことがあると言いますか……", "MessageTW": "該說是商量，還是說想詢問一些事呢……", "MessageEN": "...there's something I need to discuss."}, {"MessageId": 230050064, "Flag": 2, "Type": 0, "NextId": 230050071, "MessageType": "Text", "ImagePath": "", "MessageJP": "少し、お話したいことがありまして", "MessageTW": "有一些想說的話", "MessageEN": "Can we talk?"}, {"MessageId": 230050071, "Flag": 1, "Type": 0, "NextId": 230050080, "MessageType": "Text", "ImagePath": "", "MessageJP": "突然のことですみません！", "MessageTW": "對不起這麼突然！", "MessageEN": "I'm sorry to bother you!"}, {"MessageId": 230050080, "Flag": 2, "Type": 3, "NextId": 230050092, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、分かった", "MessageTW": "嗯，我知道了", "MessageEN": "Understood."}, {"MessageId": 230050080, "Flag": 2, "Type": 3, "NextId": 230050092, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、全然大丈夫", "MessageTW": "不會，完全沒問題", "MessageEN": "It's no problem."}, {"MessageId": 230050092, "Flag": 2, "Type": 0, "NextId": 230040100, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、ありがとうございます！では次の休日、学校でお会いしましょう！", "MessageTW": "非，非常感謝！那下個休息日，我們在學校見面吧！", "MessageEN": "Thank you! I'll see you at Abydos on your day off!"}, {"MessageId": 230040100, "Flag": 2, "Type": 2, "NextId": 230050109, "MessageType": "Text", "ImagePath": "", "MessageJP": "アヤネの絆イベントへ", "MessageTW": "前往綾音的羈絆劇情", "MessageEN": "Go to Ayane's Event"}, {"MessageId": 230050109, "Flag": 2, "Type": 0, "NextId": 230050116, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、あの本なのですが", "MessageTW": "老師，關於那本書", "MessageEN": "You know about that book, Sensei? "}, {"MessageId": 230050116, "Flag": 1, "Type": 0, "NextId": 230050113, "MessageType": "Text", "ImagePath": "", "MessageJP": "商店街のアンティークショップに依頼してみたところ、", "MessageTW": "我委託了商店街的古董店看了一下", "MessageEN": "I asked the antique shop in the shopping district, and..."}, {"MessageId": 230050113, "Flag": 2, "Type": 0, "NextId": 230050120, "MessageType": "Text", "ImagePath": "", "MessageJP": "あまり価値のない、古い大衆小説だそうです……", "MessageTW": "說只是一本沒什麼價值的舊大眾小說……", "MessageEN": "They said it's just an old novel that's not worth much..."}, {"MessageId": 230050120, "Flag": 2, "Type": 3, "NextId": 230050134, "MessageType": "Text", "ImagePath": "", "MessageJP": "あらら……", "MessageTW": "哎呀……", "MessageEN": "Ah..."}, {"MessageId": 230050120, "Flag": 2, "Type": 3, "NextId": 230050134, "MessageType": "Text", "ImagePath": "", "MessageJP": "そっか……", "MessageTW": "這樣啊……", "MessageEN": "I see."}, {"MessageId": 230050134, "Flag": 2, "Type": 0, "NextId": 230050131, "MessageType": "Text", "ImagePath": "", "MessageJP": "あはは……ですが、1200円も頂いてしまいました", "MessageTW": "啊哈哈……但就算是這樣，我也收到了1200元", "MessageEN": "But...I still got 1,200 yen for it. "}, {"MessageId": 230050131, "Flag": 2, "Type": 0, "NextId": 230050140, "MessageType": "Text", "ImagePath": "", "MessageJP": "価値は無いと言っても、これぐらいにはなったみたいです", "MessageTW": "雖然不是特別有價值，但這點程度還是有的", "MessageEN": "I guess it wasn't completely worthless."}, {"MessageId": 230050140, "Flag": 2, "Type": 3, "NextId": 230050152, "MessageType": "Text", "ImagePath": "", "MessageJP": "時間の無駄ではなかったね", "MessageTW": "花的時間值得了呢", "MessageEN": "It wasn't a complete waste of time."}, {"MessageId": 230050140, "Flag": 2, "Type": 3, "NextId": 230050152, "MessageType": "Text", "ImagePath": "", "MessageJP": "良かったね", "MessageTW": "太好了呢", "MessageEN": "I'm glad."}, {"MessageId": 230050152, "Flag": 2, "Type": 0, "NextId": 230050159, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい、そうですね", "MessageTW": "是，我也這麼認為", "MessageEN": "Hahaha... Yeah."}, {"MessageId": 230050159, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "また何か変なご相談をするかもしれませんが、よろしくお願いします。先生", "MessageTW": "也許之後還會商量什麼奇怪的事，再拜託了。老師", "MessageEN": "I hope I can talk to you again soon, Sensei."}]