[{"ImagePath": "https://schale.gg/images/student/collection/10045.webp", "MessageJP": "ホシノ（水着）", "MessageTW": "星野(泳裝)", "MessageEN": "Hoshino (Swimsuit)"}, {"MessageId": 100440010, "Flag": 2, "Type": 4, "NextId": 100450010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100450010, "Flag": 2, "Type": 0, "NextId": 100450020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、ちょっとお願いがあるんだけど", "MessageTW": "老師，我有個請求", "MessageEN": "Hmm..."}, {"MessageId": 100450020, "Flag": 1, "Type": 0, "NextId": 100450030, "MessageType": "Text", "ImagePath": "", "MessageJP": "もしみんなが私を探してても「知らない」って答えてもらえるかな？", "MessageTW": "大家問老師我在哪裡的話，能請老師回答「不知道」嗎？", "MessageEN": "I noticed someone was looking for me again."}, {"MessageId": 100450030, "Flag": 1, "Type": 0, "NextId": 100450031, "MessageType": "Text", "ImagePath": "", "MessageJP": "いま大事なことをしてて", "MessageTW": "我現在在做很重要的事", "MessageEN": "I'm in the middle of something very important."}, {"MessageId": 100450031, "Flag": 2, "Type": 0, "NextId": 100450040, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生も何も聞かなかったフリしてね。頼んだよ", "MessageTW": "老師就裝做什麼都不知道。拜託了唷", "MessageEN": "So, if someone asks, just tell them you don't know where I am."}, {"MessageId": 100450040, "Flag": 2, "Type": 3, "NextId": 100450050, "MessageType": "Text", "ImagePath": "", "MessageJP": "いいけど……客室にいるの？", "MessageTW": "是沒什麼關係……你是在客房嗎？", "MessageEN": "Sure... Are you gonna sleep in the cabin?"}, {"MessageId": 100450040, "Flag": 2, "Type": 3, "NextId": 100450050, "MessageType": "Text", "ImagePath": "", "MessageJP": "客室で寝る、とか？", "MessageTW": "在客房睡覺之類的嗎？", "MessageEN": "You're gonna nap again?"}, {"MessageId": 100450050, "Flag": 2, "Type": 0, "NextId": 100450060, "MessageType": "Text", "ImagePath": "", "MessageJP": "うへ～まぁ、そんな感じ", "MessageTW": "唔嘿～類似啦", "MessageEN": "Heh. Maybe."}, {"MessageId": 100450060, "Flag": 1, "Type": 0, "NextId": 100450061, "MessageType": "Text", "ImagePath": "", "MessageJP": "寝てるわけじゃないけど……客室にはいるよ～", "MessageTW": "雖然不是在睡覺……不過是在客房～", "MessageEN": "You understand the assignment though, right?"}, {"MessageId": 100450061, "Flag": 2, "Type": 0, "NextId": 100440070, "MessageType": "Text", "ImagePath": "", "MessageJP": "とりあえず……聞かれたらそう答えておいてね、先生", "MessageTW": "總之……被問到的話，就這麼回答囉，老師", "MessageEN": "Anyway, I'll be counting on you, Sensei."}, {"MessageId": 100440070, "Flag": 2, "Type": 2, "NextId": 100450070, "MessageType": "Text", "ImagePath": "", "MessageJP": "ホシノ（水着）の絆イベントへ", "MessageTW": "前往星野(泳裝)的羈絆劇情", "MessageEN": "Go to Hoshino (Swimsuit)'s Event"}, {"MessageId": 100450070, "Flag": 2, "Type": 0, "NextId": 100450080, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今日はありがとね", "MessageTW": "老師，今天謝啦", "MessageEN": "Sensei."}, {"MessageId": 100450080, "Flag": 1, "Type": 0, "NextId": 100450081, "MessageType": "Text", "ImagePath": "", "MessageJP": "一緒にお昼寝するの楽しかったよ", "MessageTW": "一起睡午覺很開心唷", "MessageEN": "You took a nap too."}, {"MessageId": 100450081, "Flag": 2, "Type": 0, "NextId": 100450090, "MessageType": "Text", "ImagePath": "", "MessageJP": "これでもう私たちは共犯者だね", "MessageTW": "我們現在是共犯了", "MessageEN": "We're partners in crime."}, {"MessageId": 100450090, "Flag": 1, "Type": 0, "NextId": 100450100, "MessageType": "Text", "ImagePath": "", "MessageJP": "サボりの共犯……ってことで", "MessageTW": "偷懶的共犯……就是這樣", "MessageEN": "You get it?"}, {"MessageId": 100450100, "Flag": 2, "Type": 3, "NextId": 100450110, "MessageType": "Text", "ImagePath": "", "MessageJP": "……！？", "MessageTW": "……！？", "MessageEN": "...?!"}, {"MessageId": 100450100, "Flag": 2, "Type": 3, "NextId": 100450110, "MessageType": "Text", "ImagePath": "", "MessageJP": "やられた……最初からその目的で！？", "MessageTW": "上當了……從一開始就是為了這個目的！？", "MessageEN": "You got me. That was your motive from the start!"}, {"MessageId": 100450110, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "えへへ", "MessageTW": "唔嘿嘿", "MessageEN": "Hehehe."}]