[{"ImagePath": "https://schale.gg/images/student/collection/10046.webp", "MessageJP": "イズナ（水着）", "MessageTW": "伊樹菜(泳裝)", "MessageEN": "Izuna (Swimsuit)"}, {"MessageId": 100450170, "Flag": 2, "Type": 4, "NextId": 100460170, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100460170, "Flag": 2, "Type": 0, "NextId": 100460180, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日も朝から暑いですね、主殿！", "MessageTW": "今天從早上開始就非常熱呢，主公！", "MessageEN": "My lord! It's been so hot since this morning!"}, {"MessageId": 100460180, "Flag": 1, "Type": 0, "NextId": 100460181, "MessageType": "Text", "ImagePath": "", "MessageJP": "あまりの暑さにイズナ、朝起きた時から汗だくでした……", "MessageTW": "實在太熱了，伊樹菜從早上起床後就汗流浹背……", "MessageEN": "It was so hot that..."}, {"MessageId": 100460181, "Flag": 2, "Type": 0, "NextId": 100460190, "MessageType": "Text", "ImagePath": "", "MessageJP": "特に尻尾が……", "MessageTW": "特別是尾巴的部份……", "MessageEN": "when I woke up, my tail was soaking wet!"}, {"MessageId": 100460190, "Flag": 2, "Type": 3, "NextId": 100460200, "MessageType": "Text", "ImagePath": "", "MessageJP": "この季節は大変そうだね……", "MessageTW": "這個季節真是辛苦你了……", "MessageEN": "Whoa, that's not pleasant."}, {"MessageId": 100460200, "Flag": 2, "Type": 0, "NextId": 100460201, "MessageType": "Text", "ImagePath": "", "MessageJP": "しかし、たとえどんなに暑かろうとも！", "MessageTW": "但是，不管有多熱！", "MessageEN": "But even in this heat..."}, {"MessageId": 100460201, "Flag": 2, "Type": 0, "NextId": 100460210, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナは今日も主殿と一緒に、修行に臨みたい所存です！", "MessageTW": "伊樹菜今天也想和主公一起修行！", "MessageEN": "I want to train hard with you, my lord!"}, {"MessageId": 100460210, "Flag": 1, "Type": 0, "NextId": 100460220, "MessageType": "Text", "ImagePath": "", "MessageJP": "というわけで主殿！", "MessageTW": "所以說，主公！", "MessageEN": "So on that note...!"}, {"MessageId": 100460220, "Flag": 2, "Type": 3, "NextId": 100460230, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、じゃあ準備するね", "MessageTW": "嗯，我準備好了", "MessageEN": "Yeah, Izuna. See you there, then?"}, {"MessageId": 100460230, "Flag": 2, "Type": 0, "NextId": 100460231, "MessageType": "Text", "ImagePath": "", "MessageJP": "えへへっ！はい！", "MessageTW": "耶嘿嘿！好！", "MessageEN": "Heeheehee. Yes!"}, {"MessageId": 100460231, "Flag": 2, "Type": 0, "NextId": 100450240, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナも、すぐに準備していきます！", "MessageTW": "伊樹菜也馬上準備！", "MessageEN": "I'll get ready and leave right away!"}, {"MessageId": 100450240, "Flag": 2, "Type": 2, "NextId": 100460240, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナ（水着）の絆イベントへ", "MessageTW": "前往伊樹菜(泳裝)的羈絆劇情", "MessageEN": "Go to Izuna (Swimsuit)'s Event"}, {"MessageId": 100460240, "Flag": 2, "Type": 0, "NextId": 100460250, "MessageType": "Text", "ImagePath": "", "MessageJP": "その", "MessageTW": "那個", "MessageEN": "Oh, um... So..."}, {"MessageId": 100460250, "Flag": 1, "Type": 0, "NextId": 100460251, "MessageType": "Text", "ImagePath": "", "MessageJP": "すみませんでした、主殿！", "MessageTW": "對不起，主公！", "MessageEN": "I'm sorry, my lord!"}, {"MessageId": 100460251, "Flag": 2, "Type": 0, "NextId": 100460260, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナから修行したいと言っておいて、途中で寝てしまうなんて……！", "MessageTW": "明明是伊樹菜說要修行的，居然在中途就睡著了……！", "MessageEN": "I was the one who suggested training...but I fell asleep..."}, {"MessageId": 100460260, "Flag": 2, "Type": 3, "NextId": 100460270, "MessageType": "Text", "ImagePath": "", "MessageJP": "気にしないで。砂の中、気持ち良いよね", "MessageTW": "不用放在心上。在沙子裡很舒服對吧", "MessageEN": "Don't worry. It can happen when you're buried in the sand."}, {"MessageId": 100460270, "Flag": 2, "Type": 0, "NextId": 100460280, "MessageType": "Text", "ImagePath": "", "MessageJP": "主殿……！", "MessageTW": "主公……！", "MessageEN": "My lord...! "}, {"MessageId": 100460280, "Flag": 2, "Type": 3, "NextId": 100460290, "MessageType": "Text", "ImagePath": "", "MessageJP": "まだまだ時間はあるから、大丈夫", "MessageTW": "還有時間，不要緊的", "MessageEN": "We have time. We can train sometime later."}, {"MessageId": 100460290, "Flag": 2, "Type": 0, "NextId": 100460300, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい！", "MessageTW": "是！", "MessageEN": "Oh, thank you!"}, {"MessageId": 100460300, "Flag": 1, "Type": 0, "NextId": 100460301, "MessageType": "Text", "ImagePath": "", "MessageJP": "それではまた！", "MessageTW": "那下次見！", "MessageEN": "Heehee! Next time! "}, {"MessageId": 100460301, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "引き続き一緒に遊びましょうね、主殿！", "MessageTW": "下次再繼續一起玩吧，主公！", "MessageEN": "I'll see you then, my lord!"}]