[{"ImagePath": "https://schale.gg/images/student/collection/16006.webp", "MessageJP": "イズミ（水着）", "MessageTW": "泉(泳裝)", "MessageEN": "Izumi (Swimsuit)"}, {"MessageId": 160050130, "Flag": 2, "Type": 4, "NextId": 160060130, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160060130, "Flag": 2, "Type": 0, "NextId": 160060140, "MessageType": "Text", "ImagePath": "", "MessageJP": "ねえ先生。せっかくの夏だし、私……", "MessageTW": "嘿，老師。難得的夏天，我……", "MessageEN": "We gotta go back to the beach, Sensei!"}, {"MessageId": 160060140, "Flag": 1, "Type": 0, "NextId": 160060150, "MessageType": "Text", "ImagePath": "", "MessageJP": "もっともっと夏の珍味を食べたい！", "MessageTW": "想吃更多夏天的美食！", "MessageEN": "I want more summer cuisine!"}, {"MessageId": 160060150, "Flag": 2, "Type": 3, "NextId": 160060160, "MessageType": "Text", "ImagePath": "", "MessageJP": "たしかに、季節ものはそのシーズンの内に食べておきたいよね", "MessageTW": "確實，季節性的東西就想在那個季節吃呢", "MessageEN": "Summer food is pretty good."}, {"MessageId": 160060160, "Flag": 2, "Type": 0, "NextId": 160060161, "MessageType": "Text", "ImagePath": "", "MessageJP": "でも気づいたの、先生", "MessageTW": "但我注意到了，老師", "MessageEN": "But, I realized, Sensei..."}, {"MessageId": 160060161, "Flag": 2, "Type": 0, "NextId": 160060170, "MessageType": "Text", "ImagePath": "", "MessageJP": "珍味は待ってても、来てくれないって！", "MessageTW": "美食等再久也不會來！", "MessageEN": "It's not going to come to ME!"}, {"MessageId": 160060170, "Flag": 2, "Type": 3, "NextId": 160060180, "MessageType": "Text", "ImagePath": "", "MessageJP": "すごく美食研究会っぽい台詞！", "MessageTW": "非常像是美食研究會會說的台詞！", "MessageEN": "That's the Gourmet Research Society spirit!"}, {"MessageId": 160060170, "Flag": 2, "Type": 3, "NextId": 160060180, "MessageType": "Text", "ImagePath": "", "MessageJP": "確かに、足が生えたりしないもんね。……普通は", "MessageTW": "確實，美食不會自己長腳上門呢……一般來說", "MessageEN": "You're hitting your stride!"}, {"MessageId": 160060180, "Flag": 2, "Type": 0, "NextId": 160060181, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん！ただぼーっと口を開けて待ってても、食べ物はやってこないからね！", "MessageTW": "嗯！即使是呆呆地張開嘴巴等著，食物也不會來！", "MessageEN": "Yep! Staying still like an idiot isn't going to fill me."}, {"MessageId": 160060181, "Flag": 2, "Type": 0, "NextId": 160060190, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生がすぐ横で、ポテチを食べさせてくれない限りは！", "MessageTW": "除非是老師就在我旁邊吃著洋芋片！", "MessageEN": "Well, except for all of your chips you'd share with me."}, {"MessageId": 160060190, "Flag": 2, "Type": 3, "NextId": 160060200, "MessageType": "Text", "ImagePath": "", "MessageJP": "え、それは暗にやってってこと……？", "MessageTW": "耶，那是在暗地裡做的事嗎……？", "MessageEN": "When did I agree to do that?!"}, {"MessageId": 160060200, "Flag": 2, "Type": 0, "NextId": 160060201, "MessageType": "Text", "ImagePath": "", "MessageJP": "ポテチはまた今度として……さあ、夏の珍味を探しに行こう！", "MessageTW": "洋芋片下次再吃……好了，去尋找夏天的美食吧！", "MessageEN": "Then summer food, here we come!"}, {"MessageId": 160060201, "Flag": 2, "Type": 0, "NextId": 160060210, "MessageType": "Text", "ImagePath": "", "MessageJP": "私たち、常夏グルメコンビの出番だよ！", "MessageTW": "我們常夏美食家雙人組該出場了！", "MessageEN": "The Summer Summer Food Tour is a go!"}, {"MessageId": 160060210, "Flag": 2, "Type": 3, "NextId": 160060220, "MessageType": "Text", "ImagePath": "", "MessageJP": "常夏グルメコンビ……？", "MessageTW": "常夏美食家雙人組……？", "MessageEN": "Summer Summer...?"}, {"MessageId": 160060210, "Flag": 2, "Type": 3, "NextId": 160060220, "MessageType": "Text", "ImagePath": "", "MessageJP": "……って、何？", "MessageTW": "……什麼東西？", "MessageEN": "Food Tour...?"}, {"MessageId": 160060220, "Flag": 2, "Type": 0, "NextId": 160050220, "MessageType": "Text", "ImagePath": "", "MessageJP": "あの場所で待ってるね！", "MessageTW": "我在那裡等老師囉！", "MessageEN": "I'll be waiting!"}, {"MessageId": 160050220, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズミ（水着）の絆イベントへ", "MessageTW": "前往泉(泳裝)的羈絆劇情", "MessageEN": "Go to Izumi (Swimsuit)'s Event"}]