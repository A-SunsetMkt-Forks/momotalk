[{"ImagePath": "https://schale.gg/images/student/collection/10029.webp", "MessageJP": "ナツ", "MessageTW": "夏", "MessageEN": "Natsu"}, {"MessageId": 100280080, "Flag": 2, "Type": 4, "NextId": 100290080, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100290080, "Flag": 2, "Type": 0, "NextId": 100290090, "MessageType": "Text", "ImagePath": "", "MessageJP": "世の中は厳しい", "MessageTW": "世道艱難", "MessageEN": "The world is so harsh."}, {"MessageId": 100290090, "Flag": 1, "Type": 0, "NextId": 100290100, "MessageType": "Text", "ImagePath": "", "MessageJP": "自分と異なる思想を受け入れることは難しく、お互いを受け入れることがなかなかできない……", "MessageTW": "難以接受與自己不同的思想，更別提互相包容……", "MessageEN": "People don't understand each other, and they aren't accepting of each other's ideas."}, {"MessageId": 100290100, "Flag": 1, "Type": 0, "NextId": 100290101, "MessageType": "Text", "ImagePath": "", "MessageJP": "そこを乗り越えれば、新しい場所にたどり着けるはずなのに", "MessageTW": "如果能跨越這點，明明就能到達新的境界", "MessageEN": "If people can look beyond that, they will definitely be able to break new ground."}, {"MessageId": 100290101, "Flag": 2, "Type": 0, "NextId": 100290110, "MessageType": "Text", "ImagePath": "", "MessageJP": "嘆かわしい、実に嘆かわしい……先生はそう思わない？", "MessageTW": "可悲，真可悲……老師不這麼認為嗎？", "MessageEN": "It is truly tragic. What do you think of this sad reality, Sensei?"}, {"MessageId": 100290110, "Flag": 2, "Type": 3, "NextId": 100290120, "MessageType": "Text", "ImagePath": "", "MessageJP": "重い話だね……どうしたの？", "MessageTW": "好沉重的話題啊……怎麼了嗎？", "MessageEN": "This is a pretty heavy discussion."}, {"MessageId": 100290110, "Flag": 2, "Type": 3, "NextId": 100290120, "MessageType": "Text", "ImagePath": "", "MessageJP": "何か辛いことでもあった……？", "MessageTW": "有什麼痛苦的事嗎……？", "MessageEN": "Huh? Is there something wrong?"}, {"MessageId": 100290120, "Flag": 2, "Type": 0, "NextId": 100290121, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、別に―", "MessageTW": "不，沒什麼──", "MessageEN": "No, not at all."}, {"MessageId": 100290121, "Flag": 2, "Type": 0, "NextId": 100290130, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ、ふとそう思っただけ", "MessageTW": "只是突然這麼想而已", "MessageEN": "It's just a thought that came to mind out of nowhere."}, {"MessageId": 100290130, "Flag": 1, "Type": 0, "NextId": 100290131, "MessageType": "Text", "ImagePath": "", "MessageJP": "でも", "MessageTW": "不過", "MessageEN": "But..."}, {"MessageId": 100290131, "Flag": 2, "Type": 0, "NextId": 100290140, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、先生なら大丈夫かも", "MessageTW": "嗯，如果是老師的話應該沒問題", "MessageEN": "Yeah, that's right. It might be okay if it's Sensei."}, {"MessageId": 100290140, "Flag": 1, "Type": 0, "NextId": 100290141, "MessageType": "Text", "ImagePath": "", "MessageJP": "待ってて、先生", "MessageTW": "等著，老師", "MessageEN": "Sensei."}, {"MessageId": 100290141, "Flag": 2, "Type": 0, "NextId": 100280150, "MessageType": "Text", "ImagePath": "", "MessageJP": "すぐ行く", "MessageTW": "馬上過去", "MessageEN": "I'm coming to see you right now."}, {"MessageId": 100280150, "Flag": 2, "Type": 2, "NextId": 100290150, "MessageType": "Text", "ImagePath": "", "MessageJP": "ナツの絆イベントへ", "MessageTW": "前往夏的羈絆劇情", "MessageEN": "Go to Natsu's Event"}, {"MessageId": 100290150, "Flag": 2, "Type": 0, "NextId": 100290160, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日はありがとねー、先生", "MessageTW": "今天謝啦──，老師", "MessageEN": "Thank you for today, Sensei."}, {"MessageId": 100290160, "Flag": 1, "Type": 0, "NextId": 100290170, "MessageType": "Text", "ImagePath": "", "MessageJP": "この前もそうだったけど、何だか先生と話すとスッキリする", "MessageTW": "之前也是，總覺得和老師聊天後就暢快多了", "MessageEN": "Just like last time, my heart feels lighter after talking with you, Sensei."}, {"MessageId": 100290170, "Flag": 1, "Type": 0, "NextId": 100290171, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生さえ良ければだけど", "MessageTW": "要是老師不嫌棄的話", "MessageEN": "Sometime, if it's okay with you, Sensei..."}, {"MessageId": 100290171, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "また、こんな感じでよろしくね", "MessageTW": "今後也拜託老師了", "MessageEN": "I'd like to request this kind of favor again. Thank you, Sensei."}]