[{"ImagePath": "https://schale.gg/images/student/collection/10024.webp", "MessageJP": "シロコ（ライディング）", "MessageTW": "白子(單車)", "MessageEN": "Shiroko (Cycling)"}, {"MessageId": 100230260, "Flag": 2, "Type": 4, "NextId": 100240260, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100240260, "Flag": 2, "Type": 0, "NextId": 100240270, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういえば", "MessageTW": "話說回來", "MessageEN": "Oh, right. Sensei."}, {"MessageId": 100240270, "Flag": 1, "Type": 0, "NextId": 100240271, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生この前、行き先の予定を教えてほしいって言ってたけど", "MessageTW": "老師之前，說了要我告訴你預定的目的地", "MessageEN": "You asked me to share the riding schedule and map before."}, {"MessageId": 100240271, "Flag": 2, "Type": 0, "NextId": 100240280, "MessageType": "Text", "ImagePath": "", "MessageJP": "この情報は、何か先生の役に立つの？", "MessageTW": "這個情報，對老師有什麼幫助嗎？", "MessageEN": "Why did you need that?"}, {"MessageId": 100240280, "Flag": 2, "Type": 3, "NextId": 100240290, "MessageType": "Text", "ImagePath": "", "MessageJP": "また補給品とか持っていこうかなって", "MessageTW": "我想說再幫你帶點補給品什麼的過去", "MessageEN": "So I can meet you wherever you are taking a break."}, {"MessageId": 100240280, "Flag": 2, "Type": 3, "NextId": 100240290, "MessageType": "Text", "ImagePath": "", "MessageJP": "サポーターとして色々手伝えるかなと思って", "MessageTW": "我想作為一名支持者幫上各種忙", "MessageEN": "I wanted to be a good supporter."}, {"MessageId": 100240290, "Flag": 2, "Type": 0, "NextId": 100240291, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ……", "MessageTW": "啊……", "MessageEN": "Oh..."}, {"MessageId": 100240291, "Flag": 2, "Type": 0, "NextId": 100240292, "MessageType": "Text", "ImagePath": "", "MessageJP": "ん、そっか", "MessageTW": "嗯，這樣啊", "MessageEN": "Um..."}, {"MessageId": 100240292, "Flag": 2, "Type": 0, "NextId": 100240300, "MessageType": "Text", "ImagePath": "", "MessageJP": "でも、大丈夫？仕事も忙しいのに……", "MessageTW": "但是，沒關係嗎？老師還要忙工作……", "MessageEN": "Are you okay? You must be busy with your work..."}, {"MessageId": 100240300, "Flag": 1, "Type": 0, "NextId": 100240301, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや、その", "MessageTW": "不，那個", "MessageEN": "No"}, {"MessageId": 100240301, "Flag": 2, "Type": 0, "NextId": 100240310, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん、嫌なわけじゃない", "MessageTW": "當然，我不討厭這樣", "MessageEN": "It's not that I mind. Really."}, {"MessageId": 100240310, "Flag": 1, "Type": 0, "NextId": 100240311, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、私はすごく嬉しいけど……", "MessageTW": "應該說，我很開心……", "MessageEN": "I'm fine as long as you're okay."}, {"MessageId": 100240311, "Flag": 2, "Type": 0, "NextId": 100240320, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、無理しない程度にお願い", "MessageTW": "那就麻煩老師了，但也請不要勉強", "MessageEN": "Yes."}, {"MessageId": 100240320, "Flag": 1, "Type": 0, "NextId": 100230320, "MessageType": "Text", "ImagePath": "", "MessageJP": "次のポイントに着くの、楽しみにしてるね", "MessageTW": "我會期待抵達下一個地點的哦", "MessageEN": "Then see you at the next point."}, {"MessageId": 100230320, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "シロコ（ライディング）の絆イベントへ", "MessageTW": "前往白子(單車)的羈絆劇情", "MessageEN": "Go to Shiroko (Cycling)'s Event"}]