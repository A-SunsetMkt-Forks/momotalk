[{"ImagePath": "https://schale.gg/images/student/collection/10046.webp", "MessageJP": "イズナ（水着）", "MessageTW": "伊樹菜(泳裝)", "MessageEN": "Izuna (Swimsuit)"}, {"MessageId": 100450010, "Flag": 2, "Type": 4, "NextId": 100460010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100460010, "Flag": 2, "Type": 0, "NextId": 100460020, "MessageType": "Text", "ImagePath": "", "MessageJP": "主殿！！！！", "MessageTW": "主公！！！", "MessageEN": "My lord!"}, {"MessageId": 100460020, "Flag": 1, "Type": 0, "NextId": 100460030, "MessageType": "Text", "ImagePath": "", "MessageJP": "夏ですよ、夏！", "MessageTW": "夏天到了，夏天！", "MessageEN": "It's summer. Summertime!"}, {"MessageId": 100460030, "Flag": 2, "Type": 3, "NextId": 100460040, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうだね、夏だね", "MessageTW": "是啊，夏天到了", "MessageEN": "Yeah, it's already here."}, {"MessageId": 100460040, "Flag": 2, "Type": 0, "NextId": 100460041, "MessageType": "Text", "ImagePath": "", "MessageJP": "夏なので、陽炎があちこちに見られます！", "MessageTW": "因為是夏天，到處都熱氣沖天！", "MessageEN": "The haze was everywhere today!"}, {"MessageId": 100460041, "Flag": 2, "Type": 0, "NextId": 100460050, "MessageType": "Text", "ImagePath": "", "MessageJP": "まるであの、柴犬ワン蔵監督の26作目！『かげろうの血闘』のように！", "MessageTW": "簡直就像柴犬汪藏導演的第26部作品！《陽炎之血鬥》一樣！", "MessageEN": "It looked like a scene from Bloody Battle of the Haze, the twenty-sixth film directed by Shiba Dog Wang Wang..."}, {"MessageId": 100460050, "Flag": 1, "Type": 0, "NextId": 100460060, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当にすごかったです！", "MessageTW": "真的是太驚人了！", "MessageEN": "It was amazing!"}, {"MessageId": 100460060, "Flag": 2, "Type": 3, "NextId": 100460070, "MessageType": "Text", "ImagePath": "", "MessageJP": "よく分からないけど、イズナが楽しそうでよかった", "MessageTW": "雖然不是很懂，但伊樹菜看起來很開心，真是太好了", "MessageEN": "I'm not sure what that means, but I'm glad you enjoyed it. "}, {"MessageId": 100460060, "Flag": 2, "Type": 3, "NextId": 100460070, "MessageType": "Text", "ImagePath": "", "MessageJP": "なんだかイズナ、いつも以上に元気だね", "MessageTW": "總覺得伊樹菜比平常更有精神呢", "MessageEN": "You seem more energetic than usual."}, {"MessageId": 100460070, "Flag": 2, "Type": 0, "NextId": 100460080, "MessageType": "Text", "ImagePath": "", "MessageJP": "もしかしたら、夏のお日様エネルギーのおかげかもしれません！", "MessageTW": "也許，是多虧了夏天的陽光能量！", "MessageEN": "Do you think it's because of the energy of the summer sun?"}, {"MessageId": 100460080, "Flag": 1, "Type": 0, "NextId": 100460081, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、こうしている場合ではありませんね。主殿、一緒に海に行きましょう！", "MessageTW": "啊，現在不是說這個的時候。主公！一起去海邊吧！", "MessageEN": "Oh, no time to dilly-dally! My lord, let's go to the beach together!"}, {"MessageId": 100460081, "Flag": 2, "Type": 0, "NextId": 100460090, "MessageType": "Text", "ImagePath": "", "MessageJP": "夏、そして海に適応した忍びになるため、修行開始です！", "MessageTW": "這個夏天，為了成為能適應大海的忍者，要開始修行了！", "MessageEN": "I need to train to be a ninja who's well adjusted to summer and the beach."}, {"MessageId": 100460090, "Flag": 2, "Type": 3, "NextId": 100460100, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、行こっか", "MessageTW": "嗯，走吧", "MessageEN": "If you say so!"}, {"MessageId": 100460100, "Flag": 2, "Type": 0, "NextId": 100450100, "MessageType": "Text", "ImagePath": "", "MessageJP": "はいっ、お待ちしてますね！", "MessageTW": "好的，我會等著唷！", "MessageEN": "Heehee! I'll be waiting!"}, {"MessageId": 100450100, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナ（水着）の絆イベントへ", "MessageTW": "前往伊樹菜(泳裝)的羈絆劇情", "MessageEN": "Go to Izuna (Swimsuit)'s Event"}]