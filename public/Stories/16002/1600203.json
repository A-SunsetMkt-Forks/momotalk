[{"ImagePath": "https://schale.gg/images/student/collection/16002.webp", "MessageJP": "コトリ", "MessageTW": "亞都梨", "MessageEN": "Kotori"}, {"MessageId": 160010130, "Flag": 2, "Type": 4, "NextId": 160020132, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160020132, "Flag": 2, "Type": 0, "NextId": 160020140, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei"}, {"MessageId": 160020140, "Flag": 1, "Type": 0, "NextId": 160020137, "MessageType": "Text", "ImagePath": "", "MessageJP": "今お時間ありますでしょうか？", "MessageTW": "現在有時間嗎？", "MessageEN": "Are you free right now?"}, {"MessageId": 160020137, "Flag": 2, "Type": 0, "NextId": 160020143, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、大したことではないのですが", "MessageTW": "啊，沒什麼大不了的", "MessageEN": "Oh, it's nothing."}, {"MessageId": 160020143, "Flag": 1, "Type": 0, "NextId": 160020141, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえ、個人的には大事なことなのですが……！", "MessageTW": "不，對我來說是很重要的事……！", "MessageEN": "I mean, it is pretty important!"}, {"MessageId": 160020141, "Flag": 2, "Type": 0, "NextId": 160020147, "MessageType": "Text", "ImagePath": "", "MessageJP": "と、とにかくですね……", "MessageTW": "總，總之呢……", "MessageEN": "Anyway..."}, {"MessageId": 160020147, "Flag": 2, "Type": 0, "NextId": 160020144, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、", "MessageTW": "那個", "MessageEN": "I just..."}, {"MessageId": 160020144, "Flag": 2, "Type": 0, "NextId": 160020142, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと、質問と言いますか", "MessageTW": "有點……算是疑問嗎", "MessageEN": "Um..."}, {"MessageId": 160020142, "Flag": 2, "Type": 0, "NextId": 160020150, "MessageType": "Text", "ImagePath": "", "MessageJP": "お聞きしたいことがありまして……！", "MessageTW": "我有件事想問你……！", "MessageEN": "I have something to ask you!"}, {"MessageId": 160020150, "Flag": 2, "Type": 3, "NextId": 160020162, "MessageType": "Text", "ImagePath": "", "MessageJP": "