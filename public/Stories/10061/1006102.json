[{"ImagePath": "https://schale.gg/images/student/collection/10061.webp", "MessageJP": "サクラコ", "MessageTW": "櫻子", "MessageEN": "Sakurako"}, {"MessageId": 100600100, "Flag": 2, "Type": 4, "NextId": 100610100, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100610100, "Flag": 2, "Type": 0, "NextId": 100610110, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちわ～先生☆", "MessageTW": "嗨嗨～老師☆", "MessageEN": "Yay! Sensei! ☆"}, {"MessageId": 100610110, "Flag": 1, "Type": 0, "NextId": 100610111, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日もいかがお過ごしですか？", "MessageTW": "今天過得好嗎？", "MessageEN": "How are you doing today?"}, {"MessageId": 100610111, "Flag": 2, "Type": 0, "NextId": 100610120, "MessageType": "Text", "ImagePath": "", "MessageJP": "サクラコは、いつも先生の一日が平和であるよう、願っています！", "MessageTW": "櫻子一直都在祈求老師的平安哦！", "MessageEN": "I'm always praying for your peace, Sensei!"}, {"MessageId": 100610120, "Flag": 1, "Type": 0, "NextId": 100610130, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日も良い一日を～！！", "MessageTW": "希望今天也是美好的一天～！！", "MessageEN": "Have a great day!"}, {"MessageId": 100610130, "Flag": 1, "Type": 0, "NextId": 100610140, "MessageType": "Text", "ImagePath": "", "MessageJP": "それじゃ、またね☆", "MessageTW": "那，掰掰囉☆", "MessageEN": "Baiii! ☆"}, {"MessageId": 100610140, "Flag": 2, "Type": 3, "NextId": 100610150, "MessageType": "Text", "ImagePath": "", "MessageJP": "……サクラコ？？？", "MessageTW": "……櫻子？？？", "MessageEN": "...Sakurako???"}, {"MessageId": 100610140, "Flag": 2, "Type": 3, "NextId": 100610150, "MessageType": "Text", "ImagePath": "", "MessageJP": "本人……だよね？", "MessageTW": "是本人……嗎？", "MessageEN": "Is that you?"}, {"MessageId": 100610150, "Flag": 2, "Type": 0, "NextId": 100610160, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "......."}, {"MessageId": 100610160, "Flag": 1, "Type": 0, "NextId": 100610170, "MessageType": "Text", "ImagePath": "", "MessageJP": "ええと、", "MessageTW": "那個……", "MessageEN": "Hmm."}, {"MessageId": 100610170, "Flag": 1, "Type": 0, "NextId": 100610171, "MessageType": "Text", "ImagePath": "", "MessageJP": "申し訳ありません先生。これはどうやら違うみたいですね", "MessageTW": "不好意思，老師。看來似乎有點搞錯了呢", "MessageEN": "I'm sorry. I guess I didn't get it right."}, {"MessageId": 100610171, "Flag": 2, "Type": 0, "NextId": 100610180, "MessageType": "Text", "ImagePath": "", "MessageJP": "「親しみやすい人」と、インターネットで調べてみたのですが", "MessageTW": "我在網路上搜尋了「讓人容易親近的人」", "MessageEN": "I researched \"how to look friendly\" on the internet."}, {"MessageId": 100610180, "Flag": 1, "Type": 0, "NextId": 100610181, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……何か失礼がありましたら……", "MessageTW": "那個……如果有什麼失禮之處……", "MessageEN": "If I conducted myself badly..."}, {"MessageId": 100610181, "Flag": 2, "Type": 0, "NextId": 100610190, "MessageType": "Text", "ImagePath": "", "MessageJP": "申し訳ありません", "MessageTW": "我非常抱歉", "MessageEN": "I'm sorry."}, {"MessageId": 100610190, "Flag": 1, "Type": 0, "NextId": 100600200, "MessageType": "Text", "ImagePath": "", "MessageJP": "では、また……", "MessageTW": "那麼，再會……", "MessageEN": "Until next time..."}, {"MessageId": 100600200, "Flag": 2, "Type": 2, "NextId": 100610200, "MessageType": "Text", "ImagePath": "", "MessageJP": "サクラコの絆イベントへ", "MessageTW": "前往櫻子的羈絆劇情", "MessageEN": "Go to Sakurako's Event"}, {"MessageId": 100610200, "Flag": 2, "Type": 0, "NextId": 100610210, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、お疲れ様です", "MessageTW": "老師，您辛苦了", "MessageEN": "Sensei."}, {"MessageId": 100610210, "Flag": 1, "Type": 0, "NextId": 100610211, "MessageType": "Text", "ImagePath": "", "MessageJP": "お別れの後に連絡を差し上げるのは初めてですね", "MessageTW": "這是我第一次在分別後聯絡您呢", "MessageEN": "This is my first time contacting you after work."}, {"MessageId": 100610211, "Flag": 2, "Type": 0, "NextId": 100610220, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は、本当にありがとうございました", "MessageTW": "今天真的非常謝謝您", "MessageEN": "Thank you so much for today, Sensei."}, {"MessageId": 100610220, "Flag": 1, "Type": 0, "NextId": 100610230, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……厚かましいお願いですが……また何かあった際には、よろしくお願いします", "MessageTW": "那個……雖然是個厚顏無恥的請求……如果又發生什麼事的話，再麻煩老師幫忙了", "MessageEN": "I know it's a lot to ask...but please help if something like this happens again."}, {"MessageId": 100610230, "Flag": 1, "Type": 0, "NextId": 100610231, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、先生", "MessageTW": "那麼，老師", "MessageEN": "Well, then..."}, {"MessageId": 100610231, "Flag": 2, "Type": 0, "NextId": 100610240, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうか明日も、平穏な一日でありますよう", "MessageTW": "希望明天也是和平安穩的一天", "MessageEN": "May you always be at peace and blessed with luck."}, {"MessageId": 100610240, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは……わっぴ～！", "MessageTW": "那麼……啾咪～！", "MessageEN": "Baiii~!"}]