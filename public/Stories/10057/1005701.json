[{"ImagePath": "https://schale.gg/images/student/collection/10057.webp", "MessageJP": "ハルナ（正月）", "MessageTW": "羽留奈(正月)", "MessageEN": "Haruna (New Year)"}, {"MessageId": 100560010, "Flag": 2, "Type": 4, "NextId": 100570010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100570010, "Flag": 2, "Type": 0, "NextId": 100570020, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生", "MessageTW": "您好，老師", "MessageEN": "Hello, Sensei."}, {"MessageId": 100570020, "Flag": 1, "Type": 0, "NextId": 100570030, "MessageType": "Text", "ImagePath": "", "MessageJP": "新年を迎えるにあたって、いかがお過ごしでしょうか？", "MessageTW": "新的一年即將到來，您過得如何呢？", "MessageEN": "Are you ready for the New Year?"}, {"MessageId": 100570030, "Flag": 2, "Type": 3, "NextId": 100570040, "MessageType": "Text", "ImagePath": "", "MessageJP": "割といつも通りかな。ハルナの方は？", "MessageTW": "跟平時差不多吧。羽留奈呢？", "MessageEN": "I'm looking forward to it. How about you, Haruna?"}, {"MessageId": 100570040, "Flag": 2, "Type": 0, "NextId": 100570041, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふっ", "MessageTW": "呵呵", "MessageEN": "Heehee."}, {"MessageId": 100570041, "Flag": 2, "Type": 0, "NextId": 100570042, "MessageType": "Text", "ImagePath": "", "MessageJP": "私の方も、年末も美食の探究をしていることに変わりはありませんが……", "MessageTW": "我在年末也依然進行著對美食的探究……", "MessageEN": "I'm planning to continue my gastronomical quest in the New Year, and..."}, {"MessageId": 100570042, "Flag": 2, "Type": 0, "NextId": 100570050, "MessageType": "Text", "ImagePath": "", "MessageJP": "美食において、季節や雰囲気も重要な要素の一つ", "MessageTW": "但是對美食來說，季節和氣氛也是重要的要素之一", "MessageEN": "Naturally, timing and atmosphere are also important elements of gastronomy."}, {"MessageId": 100570050, "Flag": 1, "Type": 0, "NextId": 100570051, "MessageType": "Text", "ImagePath": "", "MessageJP": "つまり年末年始という今の時期こそ", "MessageTW": "也就是說，年始年末的這個時期", "MessageEN": "This New Year..."}, {"MessageId": 100570051, "Flag": 2, "Type": 0, "NextId": 100570060, "MessageType": "Text", "ImagePath": "", "MessageJP": "普段とは一味違う美食に挑戦する良い機会ですわ", "MessageTW": "正是挑戰別具特色美食的好機會", "MessageEN": "It'll be the time to challenge ourselves with unique, revolutionary gourmet food."}, {"MessageId": 100570060, "Flag": 2, "Type": 3, "NextId": 100570070, "MessageType": "Text", "ImagePath": "", "MessageJP": "な、なるほど……", "MessageTW": "這，這樣啊……", "MessageEN": "This sounds complicated..."}, {"MessageId": 100570070, "Flag": 2, "Type": 0, "NextId": 100570080, "MessageType": "Text", "ImagePath": "", "MessageJP": "さほど難しく考えることはありませんよ", "MessageTW": "沒必要想得那麼困難唷", "MessageEN": "It only seems that way."}, {"MessageId": 100570080, "Flag": 1, "Type": 0, "NextId": 100570081, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういうわけで、先生", "MessageTW": "基於這個理由，老師", "MessageEN": "Heehee."}, {"MessageId": 100570081, "Flag": 2, "Type": 0, "NextId": 100570090, "MessageType": "Text", "ImagePath": "", "MessageJP": "少し付き合っていただけませんか？", "MessageTW": "能請您陪我一下嗎？", "MessageEN": "That's the reason you should hang out with me."}, {"MessageId": 100570090, "Flag": 2, "Type": 3, "NextId": 100560090, "MessageType": "Text", "ImagePath": "", "MessageJP": "ど、どういうわけで……！？", "MessageTW": "什，什麼理由……！？", "MessageEN": "What's the reason?!"}, {"MessageId": 100560090, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハルナ（正月）の絆イベントへ", "MessageTW": "前往羽留奈(正月)的羈絆劇情", "MessageEN": "Go to Haruna (New Year)'s Event"}]