[{"ImagePath": "https://schale.gg/images/student/collection/26004.webp", "MessageJP": "シミコ", "MessageTW": "志美子", "MessageEN": "Shimiko"}, {"MessageId": 260030230, "Flag": 2, "Type": 4, "NextId": 260040230, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 260040230, "Flag": 2, "Type": 0, "NextId": 260040240, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生は最近、本読まれてますか？何かあればいつでも相談してください！", "MessageTW": "老師最近有看書嗎？有什麼事的話隨時歡迎和我討論！", "MessageEN": "Please let me know if you ever need a book!"}, {"MessageId": 260040240, "Flag": 2, "Type": 3, "NextId": 260040250, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、ナイスタイミング", "MessageTW": "啊，時機正好", "MessageEN": "Speaking of books..."}, {"MessageId": 260040240, "Flag": 2, "Type": 3, "NextId": 260040250, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとう、ちょうど探してるものがあって", "MessageTW": "謝謝，正好有想找的東西", "MessageEN": "I've been looking for something."}, {"MessageId": 260040250, "Flag": 2, "Type": 0, "NextId": 260040260, "MessageType": "Text", "ImagePath": "", "MessageJP": "？", "MessageTW": "？", "MessageEN": "...?"}, {"MessageId": 260040260, "Flag": 1, "Type": 0, "NextId": 260040261, "MessageType": "Text", "ImagePath": "", "MessageJP": "え", "MessageTW": "咦", "MessageEN": "Mm"}, {"MessageId": 260040261, "Flag": 2, "Type": 0, "NextId": 260040270, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当ですか？", "MessageTW": "真的嗎？", "MessageEN": "Really?"}, {"MessageId": 260040270, "Flag": 2, "Type": 3, "NextId": 260040280, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、本当", "MessageTW": "嗯，真的", "MessageEN": "Yup."}, {"MessageId": 260040270, "Flag": 2, "Type": 3, "NextId": 260040280, "MessageType": "Text", "ImagePath": "", "MessageJP": "今から図書館に行っても良い？", "MessageTW": "可以現在過去圖書館嗎？", "MessageEN": "I'll be going now."}, {"MessageId": 260040280, "Flag": 2, "Type": 0, "NextId": 260040281, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ", "MessageTW": "啊", "MessageEN": "Ah."}, {"MessageId": 260040281, "Flag": 2, "Type": 0, "NextId": 260040290, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい", "MessageTW": "好的", "MessageEN": "Okay."}, {"MessageId": 260040290, "Flag": 1, "Type": 0, "NextId": 260030300, "MessageType": "Text", "ImagePath": "", "MessageJP": "はいっ！お待ちしてます！！:D", "MessageTW": "沒問題！！我在這裡等你！:D", "MessageEN": "Yes!! I'll be waiting!!!"}, {"MessageId": 260030300, "Flag": 2, "Type": 2, "NextId": 260040300, "MessageType": "Text", "ImagePath": "", "MessageJP": "シミコの絆イベントへ", "MessageTW": "前往志美子的羈絆劇情", "MessageEN": "Go to Shimiko's Event"}, {"MessageId": 260040300, "Flag": 2, "Type": 0, "NextId": 260040310, "MessageType": "Text", "ImagePath": "", "MessageJP": "本も先生も、無事に帰られましたか？", "MessageTW": "老師和書都安全回到家了嗎？", "MessageEN": "Did you get home safely, Sensei?"}, {"MessageId": 260040310, "Flag": 1, "Type": 0, "NextId": 260040320, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生が来てくれたおかげで、とっても楽しかったです", "MessageTW": "托老師過來的福，我非常開心", "MessageEN": "Thanks to you, the library was even more fun than usual today."}, {"MessageId": 260040320, "Flag": 1, "Type": 0, "NextId": 260040321, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、本は期間内に返却してくださいね", "MessageTW": "啊，書請在時限內歸還哦", "MessageEN": "Please remember to return the book you borrowed on time!"}, {"MessageId": 260040321, "Flag": 2, "Type": 0, "NextId": 260040330, "MessageType": "Text", "ImagePath": "", "MessageJP": "それから", "MessageTW": "另外", "MessageEN": "And..."}, {"MessageId": 260040330, "Flag": 1, "Type": 0, "NextId": 260040331, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしければ、またその時", "MessageTW": "如果可以的話，那個時候", "MessageEN": "If it's okay..."}, {"MessageId": 260040331, "Flag": 2, "Type": 0, "NextId": 260040340, "MessageType": "Text", "ImagePath": "", "MessageJP": "ぜひ、新しい本を借りていってください。おススメもお教えしますので", "MessageTW": "請務必再借新的書。我也會推薦書給你的", "MessageEN": "I hope you borrow a new book when you do."}, {"MessageId": 260040340, "Flag": 1, "Type": 0, "NextId": 260040350, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうすれば", "MessageTW": "那樣才可以", "MessageEN": "That way..."}, {"MessageId": 260040350, "Flag": 1, "Type": 0, "NextId": 260040351, "MessageType": "Text", "ImagePath": "", "MessageJP": "その", "MessageTW": "那個", "MessageEN": "Um..."}, {"MessageId": 260040351, "Flag": 2, "Type": 0, "NextId": 260040360, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "You'll..."}, {"MessageId": 260040360, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "……また、来てくれますよね？", "MessageTW": "……你還會，過來的對吧？", "MessageEN": "Visit again, even if it's just to return the book."}]