[{"ImagePath": "https://schale.gg/images/student/collection/13007.webp", "MessageJP": "ジュンコ", "MessageTW": "淳子", "MessageEN": "Junko"}, {"MessageId": 130060220, "Flag": 2, "Type": 4, "NextId": 130070222, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 130070222, "Flag": 2, "Type": 0, "NextId": 130070239, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふ、ねえ先生。良いこと教えてあげよっか？", "MessageTW": "呵呵，嘿，老師。能告訴你一件好事嗎？", "MessageEN": "Heehee! Sensei, you're gonna be so jealous."}, {"MessageId": 130070239, "Flag": 1, "Type": 0, "NextId": 130070240, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日の美食研究会の夕飯はなんと……お肉！", "MessageTW": "今天美食研究會的晚餐竟然是……肉！", "MessageEN": "For tonight's dinner, the Gourmet Research Society is having...meat!"}, {"MessageId": 130070240, "Flag": 2, "Type": 3, "NextId": 130070253, "MessageType": "Text", "ImagePath": "", "MessageJP": "お肉！", "MessageTW": "肉！", "MessageEN": "...Meat!"}, {"MessageId": 130070253, "Flag": 2, "Type": 0, "NextId": 130070260, "MessageType": "Text", "ImagePath": "", "MessageJP": "それも、高級な和牛！", "MessageTW": "而且還是高級和牛！", "MessageEN": "And not just any meat! We'll be enjoying premium wagyu!"}, {"MessageId": 130070260, "Flag": 2, "Type": 3, "NextId": 130070277, "MessageType": "Text", "ImagePath": "", "MessageJP": "和牛！？", "MessageTW": "和牛！？", "MessageEN": "...Wagyu!"}, {"MessageId": 130070277, "Flag": 2, "Type": 0, "NextId": 130070274, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふ。羨ましいでしょ、先生？", "MessageTW": "呵呵。羨慕吧，老師？", "MessageEN": "Heehee. Are you jealous, Sensei?"}, {"MessageId": 130070274, "Flag": 2, "Type": 0, "NextId": 130070280, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は、存分に私のことを羨ましがっていいよ！", "MessageTW": "今天就儘管羨慕我吧！", "MessageEN": "You should be green with envy today!"}, {"MessageId": 130070280, "Flag": 2, "Type": 3, "NextId": 130070295, "MessageType": "Text", "ImagePath": "", "MessageJP": "私も食べたい……", "MessageTW": "我也想吃……