[{"ImagePath": "https://schale.gg/images/student/collection/20001.webp", "MessageJP": "カリン", "MessageTW": "花凛", "MessageEN": "Karin"}, {"MessageId": 200000080, "Flag": 2, "Type": 4, "NextId": 200010080, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200010080, "Flag": 2, "Type": 0, "NextId": 200010097, "MessageType": "Text", "ImagePath": "", "MessageJP": "私にもっと力があれば、", "MessageTW": "如果我有更多的力量", "MessageEN": "If only I were a bit stronger..."}, {"MessageId": 200010097, "Flag": 1, "Type": 0, "NextId": 200010100, "MessageType": "Text", "ImagePath": "", "MessageJP": "この世から「数学」という科目を消し去るのに……", "MessageTW": "就能將「數學」這門科目從這個世界上消除了……", "MessageEN": "If there's one thing I'd love to purge from this world, it's math."}, {"MessageId": 200010100, "Flag": 2, "Type": 3, "NextId": 200010111, "MessageType": "Text", "ImagePath": "", "MessageJP": "急にどうしたの？", "MessageTW": "怎麼突然這麼說？", "MessageEN": "What are you talking about?"}, {"MessageId": 200010111, "Flag": 2, "Type": 0, "NextId": 200010118, "MessageType": "Text", "ImagePath": "", "MessageJP": "急な愚痴で申し訳ない……", "MessageTW": "一時昏頭了不好意思……", "MessageEN": "Oh, I'm just venting."}, {"MessageId": 200010118, "Flag": 2, "Type": 0, "NextId": 200000120, "MessageType": "Text", "ImagePath": "", "MessageJP": "図書館へ行く途中にふと、先生に聞いてほしくなって", "MessageTW": "在去圖書館的路上，突然想跟老師說", "MessageEN": "I was on the way to the library...and I wanted to text you."}, {"MessageId": 200000120, "Flag": 2, "Type": 2, "NextId": 200010125, "MessageType": "Text", "ImagePath": "", "MessageJP": "カリンの絆イベントへ", "MessageTW": "前往花凛的羈絆劇情", "MessageEN": "Go to Karin's Event"}, {"MessageId": 200010125, "Flag": 2, "Type": 0, "NextId": 200010122, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、二つ目の夢は……叶った時にちゃんと教える", "MessageTW": "那個，第二個夢想……實現的時候會好好告訴你的", "MessageEN": "My next dream is... Well, I'll tell you once it comes true."}, {"MessageId": 200010122, "Flag": 2, "Type": 0, "NextId": 200010139, "MessageType": "Text", "ImagePath": "", "MessageJP": "誰にも言ったことない、けど", "MessageTW": "雖然我沒有跟任何人說過", "MessageEN": "I've never told anyone."}, {"MessageId": 200010139, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "どっちにせよ、一番最初に知るのは先生だろうし……", "MessageTW": "但不管怎樣，老師八成會是第一個知道的……", "MessageEN": "I have a feeling you'll be the first to know."}]