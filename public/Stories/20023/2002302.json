[{"ImagePath": "https://schale.gg/images/student/collection/20023.webp", "MessageJP": "カンナ", "MessageTW": "環奈", "MessageEN": "Kanna"}, {"MessageId": 200220090, "Flag": 2, "Type": 4, "NextId": 200230090, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200230090, "Flag": 2, "Type": 0, "NextId": 200230100, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、夜分遅くに失礼いたします", "MessageTW": "老師，很抱歉這麼晚打擾您", "MessageEN": "I'm so sorry to contact you so late, Sensei."}, {"MessageId": 200230100, "Flag": 1, "Type": 0, "NextId": 200230110, "MessageType": "Text", "ImagePath": "", "MessageJP": "ヴァルキューレ警察学校の尾刃カンナです", "MessageTW": "我是女武神警察學園的尾刃環奈", "MessageEN": "This is Ogata Kanna of the Valkyrie Police School."}, {"MessageId": 200230110, "Flag": 2, "Type": 3, "NextId": 200230120, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了嗎？", "MessageEN": "Is something wrong?"}, {"MessageId": 200230120, "Flag": 2, "Type": 0, "NextId": 200230121, "MessageType": "Text", "ImagePath": "", "MessageJP": "こちらの勝手で申し訳ありませんが……", "MessageTW": "很抱歉，因為我個人的一些原因……", "MessageEN": "If you don't mind,"}, {"MessageId": 200230121, "Flag": 2, "Type": 0, "NextId": 200230130, "MessageType": "Text", "ImagePath": "", "MessageJP": "来週の当番、日程を変更しても良いですか？", "MessageTW": "下週值日生的安排，可以稍做更改嗎？", "MessageEN": "I'd like to reschedule for next week."}, {"MessageId": 200230130, "Flag": 2, "Type": 3, "NextId": 200230140, "MessageType": "Text", "ImagePath": "", "MessageJP": "それくらい問題ないよ", "MessageTW": "這種小事沒問題唷", "MessageEN": "That's not a problem."}, {"MessageId": 200230130, "Flag": 2, "Type": 3, "NextId": 200230140, "MessageType": "Text", "ImagePath": "", "MessageJP": "何かトラブル？", "MessageTW": "發生什麼狀況了嗎？", "MessageEN": "Is something wrong?"}, {"MessageId": 200230140, "Flag": 2, "Type": 0, "NextId": 200230150, "MessageType": "Text", "ImagePath": "", "MessageJP": "また警備局のバカ共が……", "MessageTW": "那些警備局的笨蛋們又……", "MessageEN": "It's the jerks at the Security Bureau."}, {"MessageId": 200230150, "Flag": 1, "Type": 0, "NextId": 200230160, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえ", "MessageTW": "不", "MessageEN": "Actually..."}, {"MessageId": 200230160, "Flag": 1, "Type": 0, "NextId": 200230170, "MessageType": "Text", "ImagePath": "", "MessageJP": "忘れてください", "MessageTW": "請忘了吧", "MessageEN": "Excuse my language."}, {"MessageId": 200230170, "Flag": 1, "Type": 0, "NextId": 200230180, "MessageType": "Text", "ImagePath": "", "MessageJP": "緊急の要請が来たため、私も作戦に加わることになりまして", "MessageTW": "因為傳來了緊急要求，我必須參加作戰", "MessageEN": "There's an urgent problem to deal with at the Police School, so I'll be on patrol duty."}, {"MessageId": 200230180, "Flag": 2, "Type": 3, "NextId": 200230190, "MessageType": "Text", "ImagePath": "", "MessageJP": "大変だね……", "MessageTW": "真辛苦呢……", "MessageEN": "You have a lot on your plate..."}, {"MessageId": 200230190, "Flag": 2, "Type": 0, "NextId": 200230200, "MessageType": "Text", "ImagePath": "", "MessageJP": "これも公安局局長としての務めですので", "MessageTW": "這是身為公安局局長應盡的義務", "MessageEN": "It's just my duty as an officer of the Valkyrie Police School."}, {"MessageId": 200230200, "Flag": 1, "Type": 0, "NextId": 200230201, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、当番の日程は、別途メールで連絡いたします", "MessageTW": "那麼，關於值日生的日程我會另外發訊息聯絡您", "MessageEN": "I'll email you about my new schedule."}, {"MessageId": 200230201, "Flag": 2, "Type": 0, "NextId": 200220200, "MessageType": "Text", "ImagePath": "", "MessageJP": "事件のことは気にせず、先生はお休みになってください", "MessageTW": "請老師別在意事件，好好休息", "MessageEN": "That's all I have for now. Have a relaxing day."}, {"MessageId": 200220200, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "カンナの絆イベントへ", "MessageTW": "前往環奈的羈絆劇情", "MessageEN": "Go to Kanna's Event"}]