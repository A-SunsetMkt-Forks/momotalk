[{"ImagePath": "https://schale.gg/images/student/collection/20001.webp", "MessageJP": "カリン", "MessageTW": "花凛", "MessageEN": "Karin"}, {"MessageId": 200000250, "Flag": 2, "Type": 4, "NextId": 200010255, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200010255, "Flag": 2, "Type": 0, "NextId": 200010262, "MessageType": "Text", "ImagePath": "", "MessageJP": "友人に急用ができたらしくて、代わりにバイトに行くことになった", "MessageTW": "朋友有急事，所以我代替她去打工", "MessageEN": "My friend who works part-time at a cafe had an emergency, so I took her shift."}, {"MessageId": 200010262, "Flag": 1, "Type": 0, "NextId": 200010270, "MessageType": "Text", "ImagePath": "", "MessageJP": "可愛い制服で有名なお店みたいだから、楽しみだ", "MessageTW": "那間店的制服出名的可愛，非常期待", "MessageEN": "This cafe is famous for their cute uniforms, so I'm kind of excited."}, {"MessageId": 200010270, "Flag": 2, "Type": 3, "NextId": 200010283, "MessageType": "Text", "ImagePath": "", "MessageJP": "可愛いユニフォームを着たカリン、見たいな", "MessageTW": "穿著可愛制服的花凛，真想看啊", "MessageEN": "I'm sure you'll look great in uniform, Karin."}, {"MessageId": 200010270, "Flag": 2, "Type": 3, "NextId": 200010297, "MessageType": "Text", "ImagePath": "", "MessageJP": "……そのお店、大丈夫？", "MessageTW": "……那間店，沒問題吧？", "MessageEN": "...Isn't it strange?"}, {"MessageId": 200010283, "Flag": 2, "Type": 0, "NextId": 200010280, "MessageType": "Text", "ImagePath": "", "MessageJP": "そ、そう言ってくれるのは、嬉しい、けど", "MessageTW": "聽你這麼說，很開心", "MessageEN": "That's nice of you to say."}, {"MessageId": 200010280, "Flag": 2, "Type": 0, "NextId": 200010305, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ちょっと、恥ずかしい", "MessageTW": "……但也有點害羞", "MessageEN": "But I'm kind of embarrassed now."}, {"MessageId": 200010297, "Flag": 1, "Type": 0, "NextId": 200010294, "MessageType": "Text", "ImagePath": "", "MessageJP": "ん？どういうこと？", "MessageTW": "嗯？什麼意思？", "MessageEN": "Huh? What's strange?"}, {"MessageId": 200010294, "Flag": 2, "Type": 0, "NextId": 200010291, "MessageType": "Text", "ImagePath": "", "MessageJP": "たとえどこであっても、", "MessageTW": "不管是什麼地方", "MessageEN": "Even if it is strange..."}, {"MessageId": 200010291, "Flag": 2, "Type": 0, "NextId": 200010298, "MessageType": "Text", "ImagePath": "", "MessageJP": "C&Cのメイドとして必要とされてるのであれば、", "MessageTW": "只要需要C&C的女僕", "MessageEN": "If there's someone who needs me as a C&C maid,"}, {"MessageId": 200010298, "Flag": 2, "Type": 0, "NextId": 200010305, "MessageType": "Text", "ImagePath": "", "MessageJP": "私は、助けになりたい", "MessageTW": "我就會給予幫助", "MessageEN": "I want to help them."}, {"MessageId": 200010305, "Flag": 1, "Type": 0, "NextId": 200010302, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、そろそろ行ってくる", "MessageTW": "那麼，我該走了", "MessageEN": "I'm off to work now."}, {"MessageId": 200010302, "Flag": 2, "Type": 0, "NextId": 200000310, "MessageType": "Text", "ImagePath": "", "MessageJP": "慣れてきたら、先生のことも招待するから", "MessageTW": "等我習慣了之後也會邀請老師來的", "MessageEN": "I'll invite you once I improve at this job."}, {"MessageId": 200000310, "Flag": 2, "Type": 2, "NextId": 200010319, "MessageType": "Text", "ImagePath": "", "MessageJP": "カリンの絆イベントへ", "MessageTW": "前往花凛的羈絆劇情", "MessageEN": "Go to Karin's Event"}, {"MessageId": 200010319, "Flag": 2, "Type": 0, "NextId": 200010316, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういえば、先生……", "MessageTW": "這麼說來，老師……", "MessageEN": "Sensei..."}, {"MessageId": 200010316, "Flag": 2, "Type": 0, "NextId": 200010323, "MessageType": "Text", "ImagePath": "", "MessageJP": "私がその……あのポーズをした時、タブレットを構えてた気がするけど……", "MessageTW": "我在那個……擺姿勢的時候，好像有看到你架起平板……", "MessageEN": "I saw you holding a tablet while I was doing the \"moe moe, kyun\" thing..."}, {"MessageId": 200010323, "Flag": 1, "Type": 0, "NextId": 200010330, "MessageType": "Text", "ImagePath": "", "MessageJP": "まさか、撮ってないよね？", "MessageTW": "你該不會有拍下來吧？", "MessageEN": "Were you taking photos of me?"}, {"MessageId": 200010330, "Flag": 2, "Type": 3, "NextId": 200010344, "MessageType": "Text", "ImagePath": "", "MessageJP": "カリンは撮ってないよ。笑顔が眩しいキャサリンは撮ったけど", "MessageTW": "我沒有拍花凛。我拍的是笑容耀眼的凱瑟琳", "MessageEN": "I didn't take any photos of you. I did take pictures of Catherine, though."}, {"MessageId": 200010330, "Flag": 2, "Type": 3, "NextId": 200010351, "MessageType": "Text", "ImagePath": "", "MessageJP": "黙秘権を行使します", "MessageTW": "我行使緘默權", "MessageEN": "I refuse to incriminate myself!"}, {"MessageId": 200010344, "Flag": 2, "Type": 0, "NextId": 200010368, "MessageType": "Text", "ImagePath": "", "MessageJP": "今すぐ消して！", "MessageTW": "立刻刪掉！", "MessageEN": "Delete them now!"}, {"MessageId": 200010351, "Flag": 1, "Type": 0, "NextId": 200010368, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと、先生！？既読無視しないで！", "MessageTW": "喂，老師！？不要已讀不回啊！", "MessageEN": "Answer me!"}, {"MessageId": 200010368, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei!"}]