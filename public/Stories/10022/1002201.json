[{"ImagePath": "https://schale.gg/images/student/collection/10022.webp", "MessageJP": "ヒナ（水着）", "MessageTW": "陽奈(泳裝)", "MessageEN": "Hina (Swimsuit)"}, {"MessageId": 100210010, "Flag": 2, "Type": 4, "NextId": 100220010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100220010, "Flag": 2, "Type": 0, "NextId": 100220020, "MessageType": "Text", "ImagePath": "", "MessageJP": "……もう季節は夏、か", "MessageTW": "……季節已經是夏天了嗎", "MessageEN": "It's summertime, Sensei."}, {"MessageId": 100220020, "Flag": 2, "Type": 3, "NextId": 100220030, "MessageType": "Text", "ImagePath": "", "MessageJP": "夏だね！", "MessageTW": "是夏天呢！", "MessageEN": "Yeah, summertime!"}, {"MessageId": 100220030, "Flag": 2, "Type": 3, "NextId": 100220040, "MessageType": "Text", "ImagePath": "", "MessageJP": "そして夏と言えば、海だね！", "MessageTW": "說到夏天，就是大海呢！", "MessageEN": "And when you think of the summer, you think of beaches!"}, {"MessageId": 100220040, "Flag": 2, "Type": 0, "NextId": 100220041, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、海ね", "MessageTW": "嗯，大海呢", "MessageEN": "Yup. Summer. Beaches."}, {"MessageId": 100220041, "Flag": 2, "Type": 0, "NextId": 100220050, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 100220050, "Flag": 1, "Type": 0, "NextId": 100220060, "MessageType": "Text", "ImagePath": "", "MessageJP": "……先生？ちゃんとそこにいる？", "MessageTW": "……老師？還在嗎？", "MessageEN": "Are you paying attention?"}, {"MessageId": 100220060, "Flag": 2, "Type": 3, "NextId": 100220070, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん！", "MessageTW": "當然！", "MessageEN": "Of course!"}, {"MessageId": 100220060, "Flag": 2, "Type": 3, "NextId": 100220070, "MessageType": "Text", "ImagePath": "", "MessageJP": "全力でモモトーク見てるよ！", "MessageTW": "正全力關注MomoTalk唷！", "MessageEN": "I always pay attention to you."}, {"MessageId": 100220070, "Flag": 2, "Type": 0, "NextId": 100220071, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふぅ……", "MessageTW": "呼……", "MessageEN": "*sigh*"}, {"MessageId": 100220071, "Flag": 2, "Type": 0, "NextId": 100220080, "MessageType": "Text", "ImagePath": "", "MessageJP": "夏だから、心が浮足立つのは分かるけれど……", "MessageTW": "因為是夏天，所以我可以理解會讓人興奮……", "MessageEN": "I know it's the summer..."}, {"MessageId": 100220080, "Flag": 1, "Type": 0, "NextId": 100220081, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生までそんなテンションだと、何だかやる事が増えそうで困る", "MessageTW": "但連老師也這麼情緒高昂的話，總覺得要做的事會增加，很困擾", "MessageEN": "But if you start slacking off, that affects everyone."}, {"MessageId": 100220081, "Flag": 2, "Type": 0, "NextId": 100220090, "MessageType": "Text", "ImagePath": "", "MessageJP": "仕事っていうのは、常に増えていくもの", "MessageTW": "所謂的工作，就是會不斷的增加", "MessageEN": "Work is work, all the time."}, {"MessageId": 100220090, "Flag": 1, "Type": 0, "NextId": 100220091, "MessageType": "Text", "ImagePath": "", "MessageJP": "こういう時期も例外じゃない", "MessageTW": "這種時期也不例外", "MessageEN": "Even at times like this."}, {"MessageId": 100220091, "Flag": 2, "Type": 0, "NextId": 100220100, "MessageType": "Text", "ImagePath": "", "MessageJP": "風紀を乱す生徒たちは、いつどこにでもいる。夏だから尚更", "MessageTW": "擾亂風紀的學生到處都有。尤其是夏天", "MessageEN": "There will always be immoral criminals out there."}, {"MessageId": 100220100, "Flag": 1, "Type": 0, "NextId": 100220110, "MessageType": "Text", "ImagePath": "", "MessageJP": "……だから、ちょっと手伝ってほしい", "MessageTW": "……所以，我需要一點幫助", "MessageEN": "That's why I need your help, Sensei."}, {"MessageId": 100220110, "Flag": 2, "Type": 3, "NextId": 100220120, "MessageType": "Text", "ImagePath": "", "MessageJP": "さすが、夏でもヒナちゃんは真面目だね！", "MessageTW": "不愧是陽奈，即使是夏天也很認真呢！", "MessageEN": "You're so diligent, even in your swimsuit, Hina!"}, {"MessageId": 100220110, "Flag": 2, "Type": 3, "NextId": 100220120, "MessageType": "Text", "ImagePath": "", "MessageJP": "夏の風紀委員長も隙が無いね！", "MessageTW": "風紀委員長到了夏天也毫不鬆懈！", "MessageEN": "The head prefect is even better in her swimsuit!"}, {"MessageId": 100220120, "Flag": 2, "Type": 0, "NextId": 100220130, "MessageType": "Text", "ImagePath": "", "MessageJP": "……まずは、風紀を乱しそうな先生から処理した方が良い？", "MessageTW": "……首先，先從感覺會帶亂風紀的老師開始處理似乎比較好？", "MessageEN": "Maybe I should arrest a certain immoral sensei, then."}, {"MessageId": 100220130, "Flag": 2, "Type": 3, "NextId": 100220140, "MessageType": "Text", "ImagePath": "", "MessageJP": "すみませんでした", "MessageTW": "對不起", "MessageEN": "I'm sorry. What do you need?"}, {"MessageId": 100220140, "Flag": 2, "Type": 0, "NextId": 100220141, "MessageType": "Text", "ImagePath": "", "MessageJP": "はあ、まったく……", "MessageTW": "唉，真是的……", "MessageEN": "*sigh* Honestly..."}, {"MessageId": 100220141, "Flag": 2, "Type": 0, "NextId": 100210150, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、場所はこの後送るから。待ってる", "MessageTW": "那麼，地點之後會發送給你。我等著", "MessageEN": "Come to the location I told you of before. I'll be waiting."}, {"MessageId": 100210150, "Flag": 2, "Type": 2, "NextId": 100220150, "MessageType": "Text", "ImagePath": "", "MessageJP": "ヒナ（水着）の絆イベントへ", "MessageTW": "前往陽奈(泳裝)的羈絆劇情", "MessageEN": "Go to Hina (Swimsuit)'s Event"}, {"MessageId": 100220150, "Flag": 2, "Type": 0, "NextId": 100220151, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日はありがとう、先生。合宿の参考になった", "MessageTW": "今天謝謝你，老師。這對合宿很有幫助", "MessageEN": "Thank you for helping me out with today's task, Sensei."}, {"MessageId": 100220151, "Flag": 2, "Type": 0, "NextId": 100220160, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、今度の夏の合宿でも、もし余裕があれば一緒に……", "MessageTW": "另外，這次的夏天合宿，如果時間充裕的話，希望能一起……", "MessageEN": "Then, for real this time for the summer camp..."}, {"MessageId": 100220160, "Flag": 1, "Type": 0, "NextId": 100220161, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや", "MessageTW": "不", "MessageEN": "..."}, {"MessageId": 100220161, "Flag": 2, "Type": 0, "NextId": 100220170, "MessageType": "Text", "ImagePath": "", "MessageJP": "何でもない", "MessageTW": "沒什麼", "MessageEN": "N-Never mind."}, {"MessageId": 100220170, "Flag": 2, "Type": 3, "NextId": 100220180, "MessageType": "Text", "ImagePath": "", "MessageJP": "時間を見つけて、一緒に遊べるといいね", "MessageTW": "能找到時間一起玩的話就好了呢", "MessageEN": "Right, I hope we can spend time together."}, {"MessageId": 100220180, "Flag": 2, "Type": 0, "NextId": 100220181, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 100220181, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん", "MessageTW": "嗯", "MessageEN": "Yes."}]