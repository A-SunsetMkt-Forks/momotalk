[{"ImagePath": "https://schale.gg/images/student/collection/26005.webp", "MessageJP": "ヨシミ", "MessageTW": "喜美", "MessageEN": "Yoshimi"}, {"MessageId": 260040270, "Flag": 2, "Type": 4, "NextId": 260050270, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 260050270, "Flag": 2, "Type": 0, "NextId": 260050280, "MessageType": "Text", "ImagePath": "", "MessageJP": "授業出たくない……", "MessageTW": "不想去上課……", "MessageEN": "I really don't wanna go to class today..."}, {"MessageId": 260050280, "Flag": 1, "Type": 0, "NextId": 260050290, "MessageType": "Text", "ImagePath": "", "MessageJP": "ねえ先生、何か上手い感じにサボる方法とか無いの？", "MessageTW": "嘿，老師，有什麼偷懶的好方法嗎？", "MessageEN": "Is there any way for me to get out of this, Sensei?"}, {"MessageId": 260050290, "Flag": 2, "Type": 3, "NextId": 260050300, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと……", "MessageTW": "那個……", "MessageEN": "Today...? Is there something wrong?"}, {"MessageId": 260050290, "Flag": 2, "Type": 3, "NextId": 260050300, "MessageType": "Text", "ImagePath": "", "MessageJP": "それ、一応先生である私に聞く！？", "MessageTW": "問身為老師的我！？", "MessageEN": "You're asking me, your teacher? Haha"}, {"MessageId": 260050300, "Flag": 2, "Type": 0, "NextId": 260050301, "MessageType": "Text", "ImagePath": "", "MessageJP": "分かってる、私も分かってるから。先生にこういうこと聞くのはおかしいって", "MessageTW": "我知道，我知道啦。問老師這種事情很奇怪", "MessageEN": "I know, I know. I shouldn't say things like that to you, Sensei."}, {"MessageId": 260050301, "Flag": 2, "Type": 0, "NextId": 260050302, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ、今日は「カフェ・ミルフィーユ」の新作が出る日で……", "MessageTW": "但是，今天是「Café．Mille-feuille」推出新品的日子……", "MessageEN": "It's just that Cafe Mille-Feuille is supposed to unveil their newest item today..."}, {"MessageId": 260050302, "Flag": 2, "Type": 0, "NextId": 260050310, "MessageType": "Text", "ImagePath": "", "MessageJP": "授業中にオープンしちゃうから……理由はただそれだけ。大したことじゃないから、気にしないで", "MessageTW": "因為會在上課時開始販售……理由只是這樣。沒什麼大不了的，別放在心上", "MessageEN": "The reveal happens during class, but it's not a big deal. Don't worry about it."}, {"MessageId": 260050310, "Flag": 2, "Type": 3, "NextId": 260050320, "MessageType": "Text", "ImagePath": "", "MessageJP": "気持ちは分かるけど、とりあえず授業優先かな", "MessageTW": "我懂你的心情，但還是以上課為優先吧", "MessageEN": "Let's just focus on class for now."}, {"MessageId": 260050310, "Flag": 2, "Type": 3, "NextId": 260050330, "MessageType": "Text", "ImagePath": "", "MessageJP": "OK、任せて", "MessageTW": "OK，交給我", "MessageEN": "Okay. I understand."}, {"MessageId": 260050320, "Flag": 2, "Type": 0, "NextId": 260050332, "MessageType": "Text", "ImagePath": "", "MessageJP": "うっ、まあそりゃそうよね……", "MessageTW": "嗚，是沒錯啦……", "MessageEN": "Yeah, I'm sorry. You're right..."}, {"MessageId": 260050330, "Flag": 1, "Type": 0, "NextId": 260050331, "MessageType": "Text", "ImagePath": "", "MessageJP": "……？", "MessageTW": "……？", "MessageEN": "...?"}, {"MessageId": 260050331, "Flag": 2, "Type": 0, "NextId": 260050332, "MessageType": "Text", "ImagePath": "", "MessageJP": "文脈理解してる……？まあいいや、仕方ない。ちゃんと授業には出るわよ", "MessageTW": "有理解上下文嗎……？算了，這也沒辦法。我會好好去上課的", "MessageEN": "Don't worry about it, it's just my typical nonsense... Anyway, I got it."}, {"MessageId": 260050332, "Flag": 1, "Type": 0, "NextId": 260040340, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、気が向いたらまた連絡するから(￣︶￣*)", "MessageTW": "那就先這樣，想連絡的時候會再聯絡你(￣︶￣*)", "MessageEN": "Ttyl! //(￣︶￣*))"}, {"MessageId": 260040340, "Flag": 2, "Type": 2, "NextId": 260050340, "MessageType": "Text", "ImagePath": "", "MessageJP": "ヨシミの絆イベントへ", "MessageTW": "前往喜美的羈絆劇情", "MessageEN": "Go to Yoshimi's Event"}, {"MessageId": 260050340, "Flag": 2, "Type": 0, "NextId": 260050350, "MessageType": "Text", "ImagePath": "", "MessageJP": "あー美味しかった！", "MessageTW": "啊――真好吃！", "MessageEN": "That was the best!"}, {"MessageId": 260050350, "Flag": 1, "Type": 0, "NextId": 260050360, "MessageType": "Text", "ImagePath": "", "MessageJP": "やっぱりあそこのスイーツは外れが無い！あのとろける食感と、甘酸っぱい味……！", "MessageTW": "果然那裡的甜點從來不會讓人失望！那融化的口感和酸甜的味道……！", "MessageEN": "Mille-Feuille never disappoints!"}, {"MessageId": 260050360, "Flag": 1, "Type": 0, "NextId": 260050361, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生もああいうの、好き？", "MessageTW": "老師也喜歡嗎？", "MessageEN": "What did you think, Sensei? Did you like it? The Sunshine Mousse, I mean."}, {"MessageId": 260050361, "Flag": 2, "Type": 0, "NextId": 260050362, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ", "MessageTW": "啊", "MessageEN": "Oh..."}, {"MessageId": 260050362, "Flag": 2, "Type": 0, "NextId": 260050370, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや", "MessageTW": "不是", "MessageEN": "Never mind."}, {"MessageId": 260050370, "Flag": 1, "Type": 0, "NextId": 260050371, "MessageType": "Text", "ImagePath": "", "MessageJP": "違う、今のは無し、何も言わないで", "MessageTW": "沒事，當我沒說，你什麼都別說", "MessageEN": "Wait, don't say anything. Not a word."}, {"MessageId": 260050371, "Flag": 2, "Type": 0, "NextId": 260050380, "MessageType": "Text", "ImagePath": "", "MessageJP": "こういうのはダメ、そうじゃなくて", "MessageTW": "這樣不行，不是那樣", "MessageEN": "You're probably gonna say something weird again and ruin the moment."}, {"MessageId": 260050380, "Flag": 1, "Type": 0, "NextId": 260050381, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 260050381, "Flag": 2, "Type": 0, "NextId": 260050382, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふっ、分かんないなら良いよ", "MessageTW": "呵呵，不懂的話也好", "MessageEN": "Hehe"}, {"MessageId": 260050382, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "それじゃあまたね、先生", "MessageTW": "那就之後再見囉，老師", "MessageEN": "See ya later, Sensei!"}]