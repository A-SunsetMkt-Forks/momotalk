[{"ImagePath": "https://schale.gg/images/student/collection/10027.webp", "MessageJP": "カリン（バニーガール）", "MessageTW": "花凛(兔女郎)", "MessageEN": "Karin (Bunny)"}, {"MessageId": 100260220, "Flag": 2, "Type": 4, "NextId": 100270220, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100270220, "Flag": 2, "Type": 0, "NextId": 100270230, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今忙しい？", "MessageTW": "老師，現在忙嗎？", "MessageEN": "Do you think you'll be free later, Sensei?"}, {"MessageId": 100270230, "Flag": 1, "Type": 0, "NextId": 100270240, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと真面目な話というか……", "MessageTW": "有些嚴肅的話想說……", "MessageEN": "If it's okay with you, I'd like to talk about something kind of serious."}, {"MessageId": 100270240, "Flag": 2, "Type": 3, "NextId": 100270250, "MessageType": "Text", "ImagePath": "", "MessageJP": "……もしかして、この間の話の続き？", "MessageTW": "……該不會是要繼續之前的談話？", "MessageEN": "Is this about the bunny girl outfit?"}, {"MessageId": 100270240, "Flag": 2, "Type": 3, "NextId": 100270250, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごくり……", "MessageTW": "（吞口水……）", "MessageEN": "The time has come."}, {"MessageId": 100270250, "Flag": 2, "Type": 0, "NextId": 100270251, "MessageType": "Text", "ImagePath": "", "MessageJP": "なんか、緊張してる……？", "MessageTW": "總覺得，很緊張呢……？", "MessageEN": "Um."}, {"MessageId": 100270251, "Flag": 2, "Type": 0, "NextId": 100270260, "MessageType": "Text", "ImagePath": "", "MessageJP": "あれだったら、別の日にしておく？", "MessageTW": "那樣的話，改天再說吧？", "MessageEN": "Actually...never mind."}, {"MessageId": 100270260, "Flag": 1, "Type": 0, "NextId": 100270261, "MessageType": "Text", "ImagePath": "", "MessageJP": "全然今日じゃなくても", "MessageTW": "就算不是今天也沒關係", "MessageEN": "Sorry. Forget it."}, {"MessageId": 100270261, "Flag": 2, "Type": 0, "NextId": 100270270, "MessageType": "Text", "ImagePath": "", "MessageJP": "急ぐことじゃないし……", "MessageTW": "不是什麼緊急的事情……", "MessageEN": "I think I might have been too hasty."}, {"MessageId": 100270270, "Flag": 2, "Type": 3, "NextId": 100270280, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、今日で全然大丈夫", "MessageTW": "不，今天完全沒問題", "MessageEN": "Actually, it's better this way."}, {"MessageId": 100270270, "Flag": 2, "Type": 3, "NextId": 100270280, "MessageType": "Text", "ImagePath": "", "MessageJP": "確かめておきたいっていうか、はっきりさせたいというか……", "MessageTW": "我想確認一下，或者說想澄清一下……", "MessageEN": "Let's take this chance to clarify things."}, {"MessageId": 100270280, "Flag": 2, "Type": 0, "NextId": 100270290, "MessageType": "Text", "ImagePath": "", "MessageJP": "そう？", "MessageTW": "是嗎？", "MessageEN": "Really?"}, {"MessageId": 100270290, "Flag": 1, "Type": 0, "NextId": 100270291, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、お願い", "MessageTW": "那麼，拜託了", "MessageEN": "I don't know how."}, {"MessageId": 100270291, "Flag": 2, "Type": 0, "NextId": 100270300, "MessageType": "Text", "ImagePath": "", "MessageJP": "場所は……", "MessageTW": "地點……", "MessageEN": "Okay, Sensei."}, {"MessageId": 100270300, "Flag": 1, "Type": 0, "NextId": 100270301, "MessageType": "Text", "ImagePath": "", "MessageJP": "誰にもバレないようにするためには、やっぱりあそこかな", "MessageTW": "為了不讓任何人發現，果然還是那裡吧", "MessageEN": "Then I'll find a safe, discreet meeting spot for us."}, {"MessageId": 100270301, "Flag": 2, "Type": 0, "NextId": 100260310, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、お願い。また後で連絡する", "MessageTW": "那麼，拜託了。稍後再聯絡", "MessageEN": "See you in a bit."}, {"MessageId": 100260310, "Flag": 2, "Type": 2, "NextId": 100270310, "MessageType": "Text", "ImagePath": "", "MessageJP": "カリン（バニーガール）の絆イベントへ", "MessageTW": "前往花凛(兔女郎)的羈絆劇情", "MessageEN": "Go to Karin (Bunny)'s Event"}, {"MessageId": 100270310, "Flag": 2, "Type": 0, "NextId": 100270320, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 100270320, "Flag": 1, "Type": 0, "NextId": 100270321, "MessageType": "Text", "ImagePath": "", "MessageJP": "その", "MessageTW": "那個", "MessageEN": "Um."}, {"MessageId": 100270321, "Flag": 2, "Type": 0, "NextId": 100270330, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は来てくれてありがとう", "MessageTW": "謝謝你今天能來", "MessageEN": "As for the outfit I wore today, I don't think I'll get tired of it easily."}, {"MessageId": 100270330, "Flag": 1, "Type": 0, "NextId": 100270331, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生のおかげで、この服はより特別な服になった", "MessageTW": "多虧了老師，這件衣服變得更特別了", "MessageEN": "I've become attached to this outfit because of what happened."}, {"MessageId": 100270331, "Flag": 2, "Type": 0, "NextId": 100270332, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、ありがとう", "MessageTW": "老師，謝謝", "MessageEN": "It made me realize something important..."}, {"MessageId": 100270332, "Flag": 2, "Type": 0, "NextId": 100270340, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつも先生は、色々なことを教えてくれる", "MessageTW": "老師總是教會我各式各樣的事", "MessageEN": "And I have you to thank, Sensei."}, {"MessageId": 100270340, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、また連絡するから", "MessageTW": "那麼，再聯絡", "MessageEN": "I'll talk to you again later."}]