[{"ImagePath": "https://schale.gg/images/student/collection/16011.webp", "MessageJP": "ハスミ（体操服）", "MessageTW": "蓮實(體育服)", "MessageEN": "Hasumi (Track)"}, {"MessageId": 160100180, "Flag": 2, "Type": 4, "NextId": 160110180, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160110180, "Flag": 2, "Type": 0, "NextId": 160110190, "MessageType": "Text", "ImagePath": "", "MessageJP": "余談なのですが……", "MessageTW": "雖然是題外話……", "MessageEN": "Somewhat of a tangent, but..."}, {"MessageId": 160110190, "Flag": 1, "Type": 0, "NextId": 160110200, "MessageType": "Text", "ImagePath": "", "MessageJP": "普段はちゃんと自制できているものも、状況が変わると難しくなるものですね……", "MessageTW": "就算平常能夠好好克制自己，但情況改變的話就會變得十分困難呢……", "MessageEN": "...Do you ever feel like things you're usually good at suddenly become much more difficult?"}, {"MessageId": 160110200, "Flag": 2, "Type": 3, "NextId": 160110210, "MessageType": "Text", "ImagePath": "", "MessageJP": "急にどうしたの？", "MessageTW": "怎麼突然這麼說呢？", "MessageEN": "What are you talking about?"}, {"MessageId": 160110200, "Flag": 2, "Type": 3, "NextId": 160110210, "MessageType": "Text", "ImagePath": "", "MessageJP": "……もしかして、ダイエットの話？", "MessageTW": "……難道是指減肥嗎？", "MessageEN": "...Is this about your diet?"}, {"MessageId": 160110210, "Flag": 2, "Type": 0, "NextId": 160110220, "MessageType": "Text", "ImagePath": "", "MessageJP": "周囲の空気も浮き足立ってますし、格好も……", "MessageTW": "周圍的氣氛也有點浮躁，還有外表也……", "MessageEN": "It's just... It's like there's something in the air... and all of these sights around me..."}, {"MessageId": 160110220, "Flag": 1, "Type": 0, "NextId": 160110221, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえ、何でもないです……気にしないでください", "MessageTW": "不，沒什麼……請別在意", "MessageEN": "...Never mind. Please, don't mind me."}, {"MessageId": 160110221, "Flag": 2, "Type": 0, "NextId": 160100230, "MessageType": "Text", "ImagePath": "", "MessageJP": "変な話をしてすみませんでした", "MessageTW": "抱歉說了莫名其妙的話", "MessageEN": "I'm sorry I mentioned it, Sensei."}, {"MessageId": 160100230, "Flag": 2, "Type": 2, "NextId": 160110230, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハスミ（体操服）の絆イベントへ", "MessageTW": "前往蓮實(體育服)的羈絆劇情", "MessageEN": "Go to Hasumi (Track)'s Event"}, {"MessageId": 160110230, "Flag": 2, "Type": 0, "NextId": 160110240, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめんなさい、先生。プレゼントしたのに全部私が食べてしまうなんて……", "MessageTW": "對不起，老師。明明是要給老師的禮物卻被我吃光了……", "MessageEN": "I'm sorry, Sensei. I can't believe I ate the entire thing. And after all the trouble I went through to give it to you..."}, {"MessageId": 160110240, "Flag": 1, "Type": 0, "NextId": 160110250, "MessageType": "Text", "ImagePath": "", "MessageJP": "後でまたお作りしますね", "MessageTW": "之後再做給您哦", "MessageEN": "I'll bring you some more later."}, {"MessageId": 160110250, "Flag": 2, "Type": 3, "NextId": 160110260, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫。今度また一緒に食べようね", "MessageTW": "沒關係。下次再一起吃吧", "MessageEN": "It's okay. We can share some together next time."}, {"MessageId": 160110260, "Flag": 2, "Type": 0, "NextId": 160110270, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと……その", "MessageTW": "呃……那個", "MessageEN": "Oh."}, {"MessageId": 160110270, "Flag": 1, "Type": 0, "NextId": 160110280, "MessageType": "Text", "ImagePath": "", "MessageJP": "一緒に食べるのは……", "MessageTW": "一起吃的話……", "MessageEN": "Together? Next time?"}, {"MessageId": 160110280, "Flag": 1, "Type": 0, "NextId": 160110290, "MessageType": "Text", "ImagePath": "", "MessageJP": "カロリーが……ちょっと……", "MessageTW": "卡路里會……有點……", "MessageEN": "But the calories..."}, {"MessageId": 160110290, "Flag": 1, "Type": 0, "NextId": 160110300, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえ、はい……", "MessageTW": "不，嗯……", "MessageEN": "I can't... But okay."}, {"MessageId": 160110300, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます……", "MessageTW": "謝謝……", "MessageEN": "Thank you..."}]