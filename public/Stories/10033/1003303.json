[{"ImagePath": "https://schale.gg/images/student/collection/10033.webp", "MessageJP": "ワカモ", "MessageTW": "若藻", "MessageEN": "Wakamo"}, {"MessageId": 100320210, "Flag": 2, "Type": 4, "NextId": 100330210, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100330210, "Flag": 2, "Type": 0, "NextId": 100330220, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、お元気でしょうか？", "MessageTW": "老師，請問可好？", "MessageEN": "Hello, Sensei."}, {"MessageId": 100330220, "Flag": 1, "Type": 0, "NextId": 100330221, "MessageType": "Text", "ImagePath": "", "MessageJP": "まさか、お忘れではありませんよね？", "MessageTW": "應該，沒有忘記吧？", "MessageEN": "I hope you didn't forget!"}, {"MessageId": 100330221, "Flag": 2, "Type": 0, "NextId": 100330222, "MessageType": "Text", "ImagePath": "", "MessageJP": "本日は、あなた様のお約束を頂戴した日……", "MessageTW": "今日是，收到閣下約定的日子……", "MessageEN": "Today's finally the day! You promised to spend an entire day with me!"}, {"MessageId": 100330222, "Flag": 2, "Type": 0, "NextId": 100330230, "MessageType": "Text", "ImagePath": "", "MessageJP": "この日を楽しみにしつつ、幾つの眠れない夜を明かしてきたことでしょう……", "MessageTW": "我期待著這一天，不知迎來了多少夜未眠的清晨呢……", "MessageEN": "You have no idea how many sleepless nights I've spent waiting for today..."}, {"MessageId": 100330230, "Flag": 1, "Type": 0, "NextId": 100330240, "MessageType": "Text", "ImagePath": "", "MessageJP": "そんなときめきをどうにか抑えつつも、デートプランは完璧に立てておきました！", "MessageTW": "即使想辦法壓抑著那份怦然心動，我依然制定了完美的約會計劃！", "MessageEN": "I've planned the perfect date for us. I can barely contain my excitement!"}, {"MessageId": 100330240, "Flag": 2, "Type": 3, "NextId": 100330250, "MessageType": "Text", "ImagePath": "", "MessageJP": "そ、そっか", "MessageTW": "是，是嗎", "MessageEN": "Sure..."}, {"MessageId": 100330250, "Flag": 2, "Type": 0, "NextId": 100330260, "MessageType": "Text", "ImagePath": "", "MessageJP": "では、約束の場所でお待ちしておりますね。ふふふっ……", "MessageTW": "那麼，我在約定的場所恭候您哦。哼哼哼……", "MessageEN": "I'll be waiting for you at the meeting place. Heehee."}, {"MessageId": 100330260, "Flag": 2, "Type": 3, "NextId": 100320260, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん。こっちもすぐ向かうね", "MessageTW": "嗯，我也會馬上過去哦", "MessageEN": "Okay, I'll be right there."}, {"MessageId": 100320260, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ワカモの絆イベントへ", "MessageTW": "前往若藻的羈絆劇情", "MessageEN": "Go to Wakamo's Event"}]