[{"ImagePath": "https://schale.gg/images/student/collection/26004.webp", "MessageJP": "シミコ", "MessageTW": "志美子", "MessageEN": "Shimiko"}, {"MessageId": 260030080, "Flag": 2, "Type": 4, "NextId": 260040083, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 260040083, "Flag": 2, "Type": 0, "NextId": 260040090, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういえば先生も、夜遅くまで起きて本を読んだりしますか？", "MessageTW": "這麼說來，老師也會熬夜看書嗎？", "MessageEN": "Do you happen to read late at night?"}, {"MessageId": 260040090, "Flag": 1, "Type": 0, "NextId": 260040097, "MessageType": "Text", "ImagePath": "", "MessageJP": "あっ、先生の場合は本ではなく……", "MessageTW": "啊，如果是老師的話應該不是書……", "MessageEN": "Oh, I don't really read..."}, {"MessageId": 260040097, "Flag": 2, "Type": 0, "NextId": 260040100, "MessageType": "Text", "ImagePath": "", "MessageJP": "お仕事の書類、でしょうか？", "MessageTW": "是工作的文件，對吧？", "MessageEN": "Then, do you work?"}, {"MessageId": 260040100, "Flag": 2, "Type": 3, "NextId": 260040118, "MessageType": "Text", "ImagePath": "", "MessageJP": "社会人だからね……", "MessageTW": "畢竟是社會人士嘛……", "MessageEN": "Working isn't all I do..."}, {"MessageId": 260040100, "Flag": 2, "Type": 3, "NextId": 260040118, "MessageType": "Text", "ImagePath": "", "MessageJP": "シミコ、結構夜型なんだね", "MessageTW": "志美子是夜貓子啊", "MessageEN": "Oh? You too?"}, {"MessageId": 260040118, "Flag": 2, "Type": 0, "NextId": 260040115, "MessageType": "Text", "ImagePath": "", "MessageJP": "笑", "MessageTW": "哈哈", "MessageEN": "Heeheehee."}, {"MessageId": 260040115, "Flag": 2, "Type": 0, "NextId": 260040112, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうですね", "MessageTW": "也是呢", "MessageEN": "I see."}, {"MessageId": 260040112, "Flag": 2, "Type": 0, "NextId": 260040119, "MessageType": "Text", "ImagePath": "", "MessageJP": "結構遅くまで起きてて、本を読んでいることが多いです", "MessageTW": "我經常熬夜看書到很晚", "MessageEN": "I guess..."}, {"MessageId": 260040119, "Flag": 2, "Type": 0, "NextId": 260040126, "MessageType": "Text", "ImagePath": "", "MessageJP": "私たち、活動する時間帯が近いかもしれません(・∀・*)", "MessageTW": "說不定，我們活動的時間很接近喔(．∀．*)", "MessageEN": "We have a very similar lifestyle! "}, {"MessageId": 260040126, "Flag": 1, "Type": 0, "NextId": 260040123, "MessageType": "Text", "ImagePath": "", "MessageJP": "深夜でも全然大丈夫ですので、いつでも暇な時には遊びに来てくださいね！", "MessageTW": "就算是深夜也完全沒關係，有空時都能來玩唷！", "MessageEN": "If you get bored when everyone's sleeping, come visit me!"}, {"MessageId": 260040123, "Flag": 2, "Type": 0, "NextId": 260030130, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生が来てくださるのであれば、24時間いつでも開けておきますので！", "MessageTW": "只要老師能來，24小時都能開放！", "MessageEN": "This place is open to you twenty-four hours a day!"}, {"MessageId": 260030130, "Flag": 2, "Type": 2, "NextId": 260040130, "MessageType": "Text", "ImagePath": "", "MessageJP": "シミコの絆イベントへ", "MessageTW": "前往志美子的羈絆劇情", "MessageEN": "Go to Shimiko's Event"}, {"MessageId": 260040130, "Flag": 2, "Type": 0, "NextId": 260040137, "MessageType": "Text", "ImagePath": "", "MessageJP": "無事にシャーレに着きましたか？", "MessageTW": "平安到達夏萊了嗎？", "MessageEN": "Did you get home safely?"}, {"MessageId": 260040137, "Flag": 2, "Type": 0, "NextId": 260040134, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日のことは秘密ですよ", "MessageTW": "今天的事是秘密唷", "MessageEN": "Let's keep today a secret."}, {"MessageId": 260040134, "Flag": 2, "Type": 0, "NextId": 260040141, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、噂になってしまうかもしれませんし……", "MessageTW": "那個，因為可能會被說閒話……", "MessageEN": "People might start talking, you know."}, {"MessageId": 260040141, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "私も秘密にしておきますね！(・∀・*)", "MessageTW": "我也會保密的！(・∀・*)", "MessageEN": "I won't say a word to anyone!"}]