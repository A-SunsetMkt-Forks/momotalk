[{"ImagePath": "https://schale.gg/images/student/collection/10015.webp", "MessageJP": "アリス", "MessageTW": "愛麗絲", "MessageEN": "Aris"}, {"MessageId": 1001490320, "Flag": 2, "Type": 4, "NextId": 1001500320, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 1001500320, "Flag": 2, "Type": 0, "NextId": 1001500330, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は、部室にアリス一人でして", "MessageTW": "今天，社團教室只有愛麗絲一個人", "MessageEN": "I am alone at the club today."}, {"MessageId": 1001500330, "Flag": 1, "Type": 0, "NextId": 1001500331, "MessageType": "Text", "ImagePath": "", "MessageJP": "なので、先生と一緒にゲームがしたいです", "MessageTW": "所以想跟老師一起玩遊戲", "MessageEN": "I am bored, so you should come play a game with me."}, {"MessageId": 1001500331, "Flag": 2, "Type": 0, "NextId": 1001500340, "MessageType": "Text", "ImagePath": "", "MessageJP": "良ければ遊びに来てください", "MessageTW": "如果可以的話請來玩", "MessageEN": "Please come visit."}, {"MessageId": 1001500340, "Flag": 2, "Type": 3, "NextId": 1001500350, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと時間かかるかもだけど大丈夫？", "MessageTW": "可能需要一點時間，沒關係嗎？", "MessageEN": "I'll stop by if I have time."}, {"MessageId": 1001500340, "Flag": 2, "Type": 3, "NextId": 1001500350, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、ちょっと待っててね", "MessageTW": "嗯，等一下唷", "MessageEN": "All right, I'll check in on you."}, {"MessageId": 1001500350, "Flag": 2, "Type": 0, "NextId": 1001500351, "MessageType": "Text", "ImagePath": "", "MessageJP": "はい、大丈夫です。ありがとうございます", "MessageTW": "好的，沒關係。謝謝老師", "MessageEN": "Okay. Thank you."}, {"MessageId": 1001500351, "Flag": 2, "Type": 0, "NextId": 1001490360, "MessageType": "Text", "ImagePath": "", "MessageJP": "待ってますね", "MessageTW": "愛麗絲等著哦", "MessageEN": "I will be waiting for you."}, {"MessageId": 1001490360, "Flag": 2, "Type": 2, "NextId": 1001500360, "MessageType": "Text", "ImagePath": "", "MessageJP": "アリスの絆イベントへ", "MessageTW": "前往愛麗絲的羈絆劇情", "MessageEN": "Go to Aris's Event"}, {"MessageId": 1001500360, "Flag": 2, "Type": 0, "NextId": 1001500361, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしてダンジョンには宝箱があって、", "MessageTW": "為什麼地下城內有寶箱", "MessageEN": "How do the treasure chests get into the dungeons?"}, {"MessageId": 1001500361, "Flag": 2, "Type": 0, "NextId": 1001500370, "MessageType": "Text", "ImagePath": "", "MessageJP": "その宝箱の中にはアイテムがあるのでしょうか？", "MessageTW": "那個寶箱裡有道具呢？", "MessageEN": "And why do they have items inside?"}, {"MessageId": 1001500370, "Flag": 1, "Type": 0, "NextId": 1001500371, "MessageType": "Text", "ImagePath": "", "MessageJP": "誰かが入れておいてくれたのでしょうか……？", "MessageTW": "是誰放進去的呢……？", "MessageEN": "Did someone put them there?"}, {"MessageId": 1001500371, "Flag": 2, "Type": 0, "NextId": 1001500380, "MessageType": "Text", "ImagePath": "", "MessageJP": "どなたかは知りませんが、その人はとても優しい人ですね", "MessageTW": "雖然不知道是誰，但那個人是非常溫柔的人呢", "MessageEN": "I don't know who did it, but they must be very nice."}, {"MessageId": 1001500380, "Flag": 1, "Type": 0, "NextId": 1001500390, "MessageType": "Text", "ImagePath": "", "MessageJP": "アリスも、そんな人になりたいです", "MessageTW": "愛麗絲也想成為那樣的人", "MessageEN": "I want to be someone who fills treasure chests too."}, {"MessageId": 1001500390, "Flag": 2, "Type": 3, "NextId": 1001500400, "MessageType": "Text", "ImagePath": "", "MessageJP": "応援してるね", "MessageTW": "我支持你唷", "MessageEN": "Well, you have my support, Aris."}, {"MessageId": 1001500390, "Flag": 2, "Type": 3, "NextId": 1001500400, "MessageType": "Text", "ImagePath": "", "MessageJP": "アリスならきっとなれるよ", "MessageTW": "愛麗絲一定能辦到", "MessageEN": "You can do it!"}, {"MessageId": 1001500400, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます、先生", "MessageTW": "非常感謝你，老師", "MessageEN": "Thank you, Sensei."}]