[{"ImagePath": "https://schale.gg/images/student/collection/10032.webp", "MessageJP": "ムツキ（正月）", "MessageTW": "無月(正月)", "MessageEN": "Mutsuki (New Year)"}, {"MessageId": 100310100, "Flag": 2, "Type": 4, "NextId": 100320100, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100320100, "Flag": 2, "Type": 0, "NextId": 100320110, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生って、新年のお願いごともう決めた？", "MessageTW": "老師，新年願望已經決定好了嗎？", "MessageEN": "Do you have any wishes, Sensei?"}, {"MessageId": 100320110, "Flag": 2, "Type": 3, "NextId": 100320120, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん！", "MessageTW": "當然！", "MessageEN": "Of course I do!"}, {"MessageId": 100320110, "Flag": 2, "Type": 3, "NextId": 100320130, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういえばまだ……", "MessageTW": "這麼說來，還沒……", "MessageEN": "Um, not really."}, {"MessageId": 100320120, "Flag": 2, "Type": 0, "NextId": 100320121, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふーん、そうなんだ", "MessageTW": "呵──，這樣啊", "MessageEN": "Aha! I knew it."}, {"MessageId": 100320121, "Flag": 2, "Type": 0, "NextId": 100320140, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生のお願いごと、気になるな―", "MessageTW": "老師的願望，好在意吶──", "MessageEN": "Yeah, if you think about it, everyone has one or two wishes!"}, {"MessageId": 100320130, "Flag": 1, "Type": 0, "NextId": 100320131, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、そうなんだ", "MessageTW": "啊，這樣啊", "MessageEN": "Oh! Really?"}, {"MessageId": 100320131, "Flag": 2, "Type": 0, "NextId": 100320140, "MessageType": "Text", "ImagePath": "", "MessageJP": "まあ、今からゆっくり決めれば良いんじゃない？", "MessageTW": "嘛，從現在開始慢慢決定不就行了嗎？", "MessageEN": "Hmm, it doesn't really matter!"}, {"MessageId": 100320140, "Flag": 1, "Type": 0, "NextId": 100320150, "MessageType": "Text", "ImagePath": "", "MessageJP": "とりあえず先生、行こう！", "MessageTW": "總之，老師，走吧！", "MessageEN": "Then let's go, Sensei! To fulfill our wishes!"}, {"MessageId": 100320150, "Flag": 2, "Type": 3, "NextId": 100320160, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、どこに？", "MessageTW": "那個，去哪兒？", "MessageEN": "Huh? What do you mean?"}, {"MessageId": 100320160, "Flag": 2, "Type": 0, "NextId": 100320161, "MessageType": "Text", "ImagePath": "", "MessageJP": "せっかくお正月なんだから決まってるでしょ？", "MessageTW": "難得過新年，所以很明顯吧？", "MessageEN": "Well, it is New Year's, right?"}, {"MessageId": 100320161, "Flag": 2, "Type": 0, "NextId": 100320170, "MessageType": "Text", "ImagePath": "", "MessageJP": "正月なんだから、正月らしいところに行かないと！", "MessageTW": "新年就得去有新年氣氛的地方！", "MessageEN": "We should enjoy the New Year's events the best we can."}, {"MessageId": 100320170, "Flag": 1, "Type": 0, "NextId": 100310170, "MessageType": "Text", "ImagePath": "", "MessageJP": "ほらほら、早く準備して！先生！", "MessageTW": "好啦好啦，快點準備！老師！", "MessageEN": "So let's get together, Sensei!"}, {"MessageId": 100310170, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ムツキ（正月）の絆イベントへ", "MessageTW": "前往無月(正月)的羈絆劇情", "MessageEN": "Go to Mutsuki (New Year)'s Event"}]