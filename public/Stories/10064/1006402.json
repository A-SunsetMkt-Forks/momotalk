[{"ImagePath": "https://schale.gg/images/student/collection/10064.webp", "MessageJP": "カヨコ（正月）", "MessageTW": "佳世子(正月)", "MessageEN": "Kayoko (New Year)"}, {"MessageId": 100630070, "Flag": 2, "Type": 4, "NextId": 100640070, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100640070, "Flag": 2, "Type": 0, "NextId": 100640080, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生は普段の年越しって、何して過ごしてるの？", "MessageTW": "老師過年時一般都會做什麼呢？", "MessageEN": "Sensei, what do you usually do on New Year's Eve?"}, {"MessageId": 100640080, "Flag": 2, "Type": 3, "NextId": 100640090, "MessageType": "Text", "ImagePath": "", "MessageJP": "年越しそばを食べてるかな", "MessageTW": "吃跨年蕎麥麵吧", "MessageEN": "I eat toshikoshi soba."}, {"MessageId": 100640080, "Flag": 2, "Type": 3, "NextId": 100640090, "MessageType": "Text", "ImagePath": "", "MessageJP": "年末の特番を見てるかも", "MessageTW": "看年終的特別節目吧", "MessageEN": "I watch year-end specials."}, {"MessageId": 100640090, "Flag": 2, "Type": 0, "NextId": 100640100, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふ、それも悪くないね", "MessageTW": "呵呵，那樣也不錯呢", "MessageEN": "I guess that doesn't sound too bad, haha."}, {"MessageId": 100640100, "Flag": 1, "Type": 0, "NextId": 100640101, "MessageType": "Text", "ImagePath": "", "MessageJP": "私はお寺で除夜の鐘を聞いてるよ", "MessageTW": "我會去寺廟裡聽除夕的撞鐘", "MessageEN": "I always go to the temple to listen to the New Year's bell-ringing."}, {"MessageId": 100640101, "Flag": 2, "Type": 0, "NextId": 100640110, "MessageType": "Text", "ImagePath": "", "MessageJP": "その日しか聞けない、ってところが好きで", "MessageTW": "只有那天能夠聽見，這點我很喜歡", "MessageEN": "I like it, cause you can only hear them once a year."}, {"MessageId": 100640110, "Flag": 2, "Type": 3, "NextId": 100640120, "MessageType": "Text", "ImagePath": "", "MessageJP": "ロマンがあるよね", "MessageTW": "真浪漫呢", "MessageEN": "So it's special. It feels...romantic."}, {"MessageId": 100640120, "Flag": 2, "Type": 0, "NextId": 100640130, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでだけど、先生", "MessageTW": "所以啊，老師", "MessageEN": "So, I wanna ask, Sensei."}, {"MessageId": 100640130, "Flag": 1, "Type": 0, "NextId": 100640131, "MessageType": "Text", "ImagePath": "", "MessageJP": "今年の年越しは、一緒に除夜の鐘を聞いて過ごさない？", "MessageTW": "今年跨年要不要一起去聽除夕的撞鐘？", "MessageEN": "Do you wanna listen to the New Year's bell-ringing with me this year?"}, {"MessageId": 100640131, "Flag": 2, "Type": 0, "NextId": 100640140, "MessageType": "Text", "ImagePath": "", "MessageJP": "隠れた名所があるらしくて", "MessageTW": "好像有個隱藏的好地方", "MessageEN": "I found a great place. It's not super well-known, but the bells are supposed to be really beautiful."}, {"MessageId": 100640140, "Flag": 2, "Type": 3, "NextId": 100640150, "MessageType": "Text", "ImagePath": "", "MessageJP": "良いね、行こうか", "MessageTW": "感覺不錯，一起去吧", "MessageEN": "That sounds good. Let's go!"}, {"MessageId": 100640150, "Flag": 2, "Type": 0, "NextId": 100630150, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、当日は駅前集合で", "MessageTW": "那當天在車站前集合", "MessageEN": "OK. Meet me in front of the station that evening."}, {"MessageId": 100630150, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "カヨコ（正月）の絆イベントへ", "MessageTW": "前往佳世子(正月)的羈絆劇情", "MessageEN": "Go to Kayoko (New Year)'s Event"}]