[{"ImagePath": "https://schale.gg/images/student/collection/26000.webp", "MessageJP": "チナツ", "MessageTW": "千夏", "MessageEN": "Chinatsu"}, {"MessageId": 259990160, "Flag": 2, "Type": 4, "NextId": 260000163, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 260000163, "Flag": 2, "Type": 0, "NextId": 260000170, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生", "MessageTW": "您好，老師", "MessageEN": "Hello, Sensei."}, {"MessageId": 260000170, "Flag": 1, "Type": 0, "NextId": 260000177, "MessageType": "Text", "ImagePath": "", "MessageJP": "もしよかったら、先生のご都合が良いお時間帯を教えていただけませんか？", "MessageTW": "如果可以的話，能告訴我老師方便的時間嗎？", "MessageEN": "Can you let me know when you're free?"}, {"MessageId": 260000177, "Flag": 2, "Type": 0, "NextId": 260000180, "MessageType": "Text", "ImagePath": "", "MessageJP": "個人的に相談したいことがありまして", "MessageTW": "我有事想私下商量", "MessageEN": "I have something personal to discuss with you."}, {"MessageId": 260000180, "Flag": 2, "Type": 3, "NextId": 260000198, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日の仕事が終わった頃とかどう？", "MessageTW": "今天工作結束的時候如何？", "MessageEN": "I'm free after work."}, {"MessageId": 260000180, "Flag": 2, "Type": 3, "NextId": 260000198, "MessageType": "Text", "ImagePath": "", "MessageJP": "今すぐでも大丈夫！", "MessageTW": "現在馬上也沒問題！", "MessageEN": "I'm free now!"}, {"MessageId": 260000198, "Flag": 2, "Type": 0, "NextId": 260000195, "MessageType": "Text", "ImagePath": "", "MessageJP": "承知しました、ではそれでお願いします", "MessageTW": "知道了，那就再拜託了", "MessageEN": "Yeah."}, {"MessageId": 260000195, "Flag": 2, "Type": 0, "NextId": 260000202, "MessageType": "Text", "ImagePath": "", "MessageJP": "お時間いただきありがとうございます", "MessageTW": "非常感謝老師給我時間", "MessageEN": "Thank you so much for making yourself available."}, {"MessageId": 260000202, "Flag": 1, "Type": 0, "NextId": 259990210, "MessageType": "Text", "ImagePath": "", "MessageJP": "お伺いしますね　～チナツ～", "MessageTW": "我會去拜訪老師哦　～千夏～", "MessageEN": "I'll come to you this time. - Chinatsu"}, {"MessageId": 259990210, "Flag": 2, "Type": 2, "NextId": 260000219, "MessageType": "Text", "ImagePath": "", "MessageJP": "チナツの絆イベントへ", "MessageTW": "前往千夏的羈絆劇情", "MessageEN": "Go to Chinatsu's Event"}, {"MessageId": 260000219, "Flag": 2, "Type": 0, "NextId": 260000216, "MessageType": "Text", "ImagePath": "", "MessageJP": "疲れたと感じた時には、いつでも連絡してください", "MessageTW": "感到疲累的時候，請隨時聯絡我", "MessageEN": "Just let me know if you ever need anything, Sensei."}, {"MessageId": 260000216, "Flag": 2, "Type": 0, "NextId": 260000213, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえ、", "MessageTW": "不", "MessageEN": "Nah."}, {"MessageId": 260000213, "Flag": 2, "Type": 0, "NextId": 260000210, "MessageType": "Text", "ImagePath": "", "MessageJP": "これは疲れそうだなと思うようなことがありましたら、私を呼んでください", "MessageTW": "如果覺得有事情會讓老師感到疲憊，請通知我一聲", "MessageEN": "If you think something exhausting is coming up, just let me know in advance."}, {"MessageId": 260000210, "Flag": 2, "Type": 0, "NextId": 260000227, "MessageType": "Text", "ImagePath": "", "MessageJP": "また疲労が溜まってしまわないよう、私もお手伝いしますので", "MessageTW": "為了不再讓您累積疲勞，我會去幫忙的", "MessageEN": "I'll help you so that you don't ever feel tired."}, {"MessageId": 260000227, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "それではまた　～チナツ～", "MessageTW": "那麼，下次見　～千夏～", "MessageEN": "Have a nice day. - Chinatsu"}]