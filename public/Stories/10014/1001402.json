[{"ImagePath": "https://schale.gg/images/student/collection/10014.webp", "MessageJP": "イズナ", "MessageTW": "伊樹菜", "MessageEN": "Izuna"}, {"MessageId": 100130100, "Flag": 2, "Type": 4, "NextId": 100140107, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100140107, "Flag": 2, "Type": 0, "NextId": 100140110, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナ、先日の主殿からの忠告、心に深く刻みました", "MessageTW": "伊樹菜會將主公前幾日的忠告深深刻在心中", "MessageEN": "I took your advice to heart, my lord."}, {"MessageId": 100140110, "Flag": 2, "Type": 3, "NextId": 100140121, "MessageType": "Text", "ImagePath": "", "MessageJP": "忠告？何の話だっけ……？", "MessageTW": "忠告？我說了什麼來著……？", "MessageEN": "What advice?"}, {"MessageId": 100140121, "Flag": 2, "Type": 0, "NextId": 100140128, "MessageType": "Text", "ImagePath": "", "MessageJP": "この前、主殿が仰っていた……", "MessageTW": "上次您說的……", "MessageEN": "What you told me last time... "}, {"MessageId": 100140128, "Flag": 2, "Type": 0, "NextId": 100140130, "MessageType": "Text", "ImagePath": "", "MessageJP": "無断での尾行は良くないというお話です！", "MessageTW": "未經允許的跟蹤是不好的！", "MessageEN": "That following people is bad!"}, {"MessageId": 100140130, "Flag": 2, "Type": 3, "NextId": 100140149, "MessageType": "Text", "ImagePath": "", "MessageJP": "ごめん、覚えてないかも……？", "MessageTW": "抱歉，我可能不記得了……？", "MessageEN": "Did I say that?"}, {"MessageId": 100140130, "Flag": 2, "Type": 3, "NextId": 100140149, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういえばそんなこともあったね……", "MessageTW": "這麼說來好像有這麼回事……", "MessageEN": "Yes, I probably did say that..."}, {"MessageId": 100140149, "Flag": 2, "Type": 0, "NextId": 100140146, "MessageType": "Text", "ImagePath": "", "MessageJP": "あの時主殿よりしかと頂いたお言葉を、あらためて勘考しました", "MessageTW": "伊樹菜會聽從當時的話", "MessageEN": "Take my word for it, Izuna. "}, {"MessageId": 100140146, "Flag": 2, "Type": 0, "NextId": 100140150, "MessageType": "Text", "ImagePath": "", "MessageJP": "イズナ、今後は主殿をこっそり護衛することはやめます！", "MessageTW": "伊樹菜今後