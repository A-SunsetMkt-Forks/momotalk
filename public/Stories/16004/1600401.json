[{"ImagePath": "https://schale.gg/images/student/collection/16004.webp", "MessageJP": "フィーナ", "MessageTW": "菲娜", "MessageEN": "Pina"}, {"MessageId": 160030010, "Flag": 2, "Type": 4, "NextId": 160040017, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160040017, "Flag": 2, "Type": 0, "NextId": 160040024, "MessageType": "Text", "ImagePath": "", "MessageJP": "ご機嫌いかがですか、お頭！", "MessageTW": "一切安好嗎，頭兒！", "MessageEN": "Hello! I just wanted to say hello, boss!"}, {"MessageId": 160040024, "Flag": 1, "Type": 0, "NextId": 160040021, "MessageType": "Text", "ImagePath": "", "MessageJP": "私は百鬼夜行連合学院、「お祭り運営委員会」組のフィーナと申しやす！", "MessageTW": "我是百鬼夜行聯合學園「節慶營運管理部」組的菲娜！", "MessageEN": "I'm Pina of the Festival Department at Hyakkiyako Academy."}, {"MessageId": 160040021, "Flag": 2, "Type": 0, "NextId": 160040030, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしくお願いしやす！", "MessageTW": "請多指教！", "MessageEN": "Pleasure to meet you!"}, {"MessageId": 160040030, "Flag": 2, "Type": 3, "NextId": 160040042, "MessageType": "Text", "ImagePath": "", "MessageJP": "お頭……？", "MessageTW": "頭兒……？", "MessageEN": "Why are you calling me \"boss\"?"}, {"MessageId": 160040030, "Flag": 2, "Type": 3, "NextId": 160040042, "MessageType": "Text", "ImagePath": "", "MessageJP": "お祭り委員会「組」……？", "MessageTW": "營運管理部「組」……？", "MessageEN": "The Festival Department?"}, {"MessageId": 160040042, "Flag": 2, "Type": 0, "NextId": 160040049, "MessageType": "Text", "ImagePath": "", "MessageJP": "あっ", "MessageTW": "啊", "MessageEN": "Oh!"}, {"MessageId": 160040049, "Flag": 2, "Type": 0, "NextId": 160040046, "MessageType": "Text", "ImagePath": "", "MessageJP": "失礼しました", "MessageTW": "失禮了", "MessageEN": "I'm sorry."}, {"MessageId": 160040046, "Flag": 2, "Type": 0, "NextId": 160040043, "MessageType": "Text", "ImagePath": "", "MessageJP": "唐突な自己紹介でした", "MessageTW": "突然就自我介紹", "MessageEN": "I guess I never really introduced myself."}, {"MessageId": 160040043, "Flag": 2, "Type": 0, "NextId": 160040050, "MessageType": "Text", "ImagePath": "", "MessageJP": "まだ色々と慣れていなくて", "MessageTW": "我也還不太習慣", "MessageEN": "I'm not quite used to speaking of myself that way."}, {"MessageId": 160040050, "Flag": 1, "Type": 0, "NextId": 160040057, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと練習してから、また今度もう一度やらせてください", "MessageTW": "讓我再練習一下，下次請讓我再試一次", "MessageEN": "I should practice a bit more."}, {"MessageId": 160040057, "Flag": 2, "Type": 0, "NextId": 160030060, "MessageType": "Text", "ImagePath": "", "MessageJP": "とにかく、今度遊びに来てくださいね！！", "MessageTW": "總之，下次請來玩唷！！", "MessageEN": "Anyway, please drop by sometime!"}, {"MessageId": 160030060, "Flag": 2, "Type": 2, "NextId": 160040064, "MessageType": "Text", "ImagePath": "", "MessageJP": "フィーナの絆イベントへ", "MessageTW": "前往菲娜的羈絆劇情", "MessageEN": "Go to Pina's Event"}, {"MessageId": 160040064, "Flag": 2, "Type": 0, "NextId": 160040061, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日はすごく楽しかったです、先生！", "MessageTW": "今天非常開心，老師！", "MessageEN": "Today was great, Sensei!"}, {"MessageId": 160040061, "Flag": 2, "Type": 0, "NextId": 160040078, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生の言った「フィーナの任侠」が何なのか、まだ分かりませんが", "MessageTW": "老師說的「菲娜的任俠」究竟是什麼，雖然我還不太明白", "MessageEN": "I'm still not sure what you mean by \"ninkyo\". You mean my chivalry?"}, {"MessageId": 160040078, "Flag": 1, "Type": 0, "NextId": 160040075, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生が言ってくれたんだから、間違いありません！", "MessageTW": "但因為是老師說的，所以一定沒錯！", "MessageEN": "But I'm sure you mean well!"}, {"MessageId": 160040075, "Flag": 2, "Type": 0, "NextId": 160040072, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつかは私も分かるはずです！", "MessageTW": "總有一天我會明白的！", "MessageEN": "Maybe I'll get it eventually."}, {"MessageId": 160040072, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "それではまた会いましょう！！絶対に！！", "MessageTW": "那就再會囉！絕對要唷！", "MessageEN": "I'll see you again soon! "}]