[{"ImagePath": "https://schale.gg/images/student/collection/10030.webp", "MessageJP": "チナツ（温泉）", "MessageTW": "千夏(溫泉)", "MessageEN": "Chinatsu (Hot Spring)"}, {"MessageId": 100290120, "Flag": 2, "Type": 4, "NextId": 100300120, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100300120, "Flag": 2, "Type": 0, "NextId": 100300130, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、お疲れ様です", "MessageTW": "老師，辛苦了", "MessageEN": "Hello, Sensei. This is Chinatsu."}, {"MessageId": 100300130, "Flag": 1, "Type": 0, "NextId": 100300140, "MessageType": "Text", "ImagePath": "", "MessageJP": "温泉に入られて、何かご不便などはありませんでしたか？", "MessageTW": "泡了溫泉，有感到什麼不適的地方嗎？", "MessageEN": "Is there anything uncomfortable with your stay at the hot springs?"}, {"MessageId": 100300140, "Flag": 2, "Type": 3, "NextId": 100300150, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、大丈夫", "MessageTW": "嗯，沒問題", "MessageEN": "No, not at all."}, {"MessageId": 100300140, "Flag": 2, "Type": 3, "NextId": 100300150, "MessageType": "Text", "ImagePath": "", "MessageJP": "チナツが色々配慮してくれたおかげでバッチリ", "MessageTW": "多虧有千夏關照，非常完美", "MessageEN": "All thanks to you, Chinatsu."}, {"MessageId": 100300150, "Flag": 2, "Type": 0, "NextId": 100300160, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは良かったです", "MessageTW": "那太好了", "MessageEN": "I'm glad to hear that."}, {"MessageId": 100300160, "Flag": 1, "Type": 0, "NextId": 100300161, "MessageType": "Text", "ImagePath": "", "MessageJP": "それで、本題なのですが……今から先生のところに、お伺いしてもよろしいでしょうか？", "MessageTW": "然後，進入正題……現在方便去老師那邊打擾一下嗎？", "MessageEN": "If it's okay with you, can I stop by your room right now?"}, {"MessageId": 100300161, "Flag": 2, "Type": 0, "NextId": 100300170, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、もちろん変な意味ではなく……！", "MessageTW": "那個，當然不是奇怪的意思……！", "MessageEN": "Not for any weird reason of course!"}, {"MessageId": 100300170, "Flag": 1, "Type": 0, "NextId": 100300171, "MessageType": "Text", "ImagePath": "", "MessageJP": "疲労に効くお茶を手に入れたので、よろしければ一緒にお茶でもと思いまして", "MessageTW": "是因為得到了恢復疲勞很有效的茶，如果可以的話，想一起喝茶", "MessageEN": "It's just that I bought some teas that are good for relieving fatigue..."}, {"MessageId": 100300171, "Flag": 2, "Type": 0, "NextId": 100300180, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……いかがでしょうか？", "MessageTW": "那個……如何？", "MessageEN": "So I was wondering if you would like to have a cup of tea together."}, {"MessageId": 100300180, "Flag": 2, "Type": 3, "NextId": 100300190, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん。ありがとう、待ってるね", "MessageTW": "當然。謝謝，我等著唷", "MessageEN": "Sure."}, {"MessageId": 100300190, "Flag": 2, "Type": 0, "NextId": 100300191, "MessageType": "Text", "ImagePath": "", "MessageJP": "いえそんな、こちらこそありがとうございます", "MessageTW": "不會，我才要感謝老師", "MessageEN": "Yes."}, {"MessageId": 100300191, "Flag": 2, "Type": 0, "NextId": 100300200, "MessageType": "Text", "ImagePath": "", "MessageJP": "では、すぐにお伺いしますね", "MessageTW": "那麼，我馬上就過去拜訪您", "MessageEN": "Then I will see you soon."}, {"MessageId": 100300200, "Flag": 1, "Type": 0, "NextId": 100290200, "MessageType": "Text", "ImagePath": "", "MessageJP": "～チナツ～", "MessageTW": "～千夏～", "MessageEN": "- Chinatsu"}, {"MessageId": 100290200, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "チナツ（温泉）の絆イベントへ", "MessageTW": "前往千夏(溫泉)的羈絆劇情", "MessageEN": "Go to Chinatsu (Hot Spring)'s Event"}]