[{"ImagePath": "https://schale.gg/images/student/collection/20024.webp", "MessageJP": "ナギサ", "MessageTW": "渚", "MessageEN": "Nagisa"}, {"MessageId": 200230310, "Flag": 2, "Type": 4, "NextId": 200240310, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200240310, "Flag": 2, "Type": 0, "NextId": 200240320, "MessageType": "Text", "ImagePath": "", "MessageJP": "おはようございます、先生", "MessageTW": "早安，老師", "MessageEN": "Good morning, Sensei."}, {"MessageId": 200240320, "Flag": 1, "Type": 0, "NextId": 200240330, "MessageType": "Text", "ImagePath": "", "MessageJP": "一日の始まりは朝食からと言いますが……先生はもうすでに食べられましたか？", "MessageTW": "有句話說早餐是一天的開始……老師已經用過早餐了嗎？", "MessageEN": "Did you eat breakfast before you went to work?"}, {"MessageId": 200240330, "Flag": 2, "Type": 3, "NextId": 200240340, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、ちょうどさっき。ナギサの方は？", "MessageTW": "嗯，我剛吃完。渚呢？", "MessageEN": "Yeah. How about you, Nagisa?"}, {"MessageId": 200240340, "Flag": 2, "Type": 0, "NextId": 200240350, "MessageType": "Text", "ImagePath": "", "MessageJP": "私は先ほど、お味噌汁をいただきました", "MessageTW": "我稍早之前喝過了味噌湯", "MessageEN": "Yes. I had miso soup a while ago."}, {"MessageId": 200240350, "Flag": 2, "Type": 3, "NextId": 200240360, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ちょっと意外かも", "MessageTW": "……有點意外", "MessageEN": "...I didn't expect that."}, {"MessageId": 200240360, "Flag": 2, "Type": 3, "NextId": 200240370, "MessageType": "Text", "ImagePath": "", "MessageJP": "ティーパーティーって何か、スイーツしか食べないみたいなイメージが……", "MessageTW": "茶會好像給人一種只吃甜點的印象……", "MessageEN": "I thought Tea Party students ate pastries three meals a day."}, {"MessageId": 200240370, "Flag": 2, "Type": 0, "NextId": 200240380, "MessageType": "Text", "ImagePath": "", "MessageJP": "何を仰るのですか……", "MessageTW": "您說笑了……", "MessageEN": "That's simply not possible."}, {"MessageId": 200240380, "Flag": 1, "Type": 0, "NextId": 200240381, "MessageType": "Text", "ImagePath": "", "MessageJP": "いくら何でも、甘いものばかり食べていては栄養バランスが良くありません", "MessageTW": "再怎麼說，只吃甜食對營養的平衡會造成不良的影響", "MessageEN": "We may enjoy sweets at our gatherings, but it wouldn't be healthy to eat that and nothing else."}, {"MessageId": 200240381, "Flag": 2, "Type": 0, "NextId": 200230380, "MessageType": "Text", "ImagePath": "", "MessageJP": "私たちだって、いつもケーキやお菓子ばかり食べているわけではないんです", "MessageTW": "即使是我們，也不是一直都在吃蛋糕和點心的", "MessageEN": "I certainly don't eat only cakes and cookies."}, {"MessageId": 200230380, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ナギサの絆イベントへ", "MessageTW": "前往渚的羈絆劇情", "MessageEN": "Go to Nagisa's Event"}]