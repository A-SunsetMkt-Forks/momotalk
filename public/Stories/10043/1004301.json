[{"ImagePath": "https://schale.gg/images/student/collection/10043.webp", "MessageJP": "ワカモ（水着）", "MessageTW": "若藻(泳裝)", "MessageEN": "Wakamo (Swimsuit)"}, {"MessageId": 100420010, "Flag": 2, "Type": 4, "NextId": 100430010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100430010, "Flag": 2, "Type": 0, "NextId": 100430020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、ご無沙汰しております", "MessageTW": "老師，久未問候了", "MessageEN": "Sensei. Today is especially hot."}, {"MessageId": 100430020, "Flag": 1, "Type": 0, "NextId": 100430030, "MessageType": "Text", "ImagePath": "", "MessageJP": "本日は真夏日のようですが、お変わりはありませんか？", "MessageTW": "今日似乎是盛夏日，有發生什麼異常嗎？", "MessageEN": "How has your health been?"}, {"MessageId": 100430030, "Flag": 2, "Type": 3, "NextId": 100430040, "MessageType": "Text", "ImagePath": "", "MessageJP": "心配してくれてありがとう、ワカモ。私は大丈夫だよ", "MessageTW": "謝謝妳擔心我，若藻。我沒事唷", "MessageEN": "Oh, thanks for the concern, Wakamo. I'm okay."}, {"MessageId": 100430030, "Flag": 2, "Type": 3, "NextId": 100430040, "MessageType": "Text", "ImagePath": "", "MessageJP": "ワカモこそ、体調大丈夫？", "MessageTW": "若藻才是，身體還好嗎？", "MessageEN": "How are you doing?"}, {"MessageId": 100430040, "Flag": 2, "Type": 0, "NextId": 100430050, "MessageType": "Text", "ImagePath": "", "MessageJP": "あぁ……いつ何時も変わらないこの優しいお返事……", "MessageTW": "啊……無論何時都不會變的這溫柔的答覆……", "MessageEN": "Oh, you're always so kind."}, {"MessageId": 100430050, "Flag": 1, "Type": 0, "NextId": 100430060, "MessageType": "Text", "ImagePath": "", "MessageJP": "ええ、ご安心ください。このワカモ、健康だけは自信がありますので", "MessageTW": "是的，請安心。若藻我，唯獨對健康可是很有自信的", "MessageEN": "I'm doing great. I'm very confident in my health."}, {"MessageId": 100430060, "Flag": 2, "Type": 3, "NextId": 100430070, "MessageType": "Text", "ImagePath": "", "MessageJP": "元気なのが一番だね", "MessageTW": "有精神是最重要的呢", "MessageEN": "Nothing is more important than health."}, {"MessageId": 100430070, "Flag": 2, "Type": 0, "NextId": 100430071, "MessageType": "Text", "ImagePath": "", "MessageJP": "うふふ……時に、先生", "MessageTW": "唔哼哼……話說，老師", "MessageEN": "Oh, by the way, Sensei..."}, {"MessageId": 100430071, "Flag": 2, "Type": 0, "NextId": 100430080, "MessageType": "Text", "ImagePath": "", "MessageJP": "あなた様がご多忙を極めていらっしゃる事は、このワカモもよく存じておりますが", "MessageTW": "對於閣下十分忙碌這件事，若藻我也十分清楚", "MessageEN": "I know that you're always busy with work, but..."}, {"MessageId": 100430080, "Flag": 1, "Type": 0, "NextId": 100430090, "MessageType": "Text", "ImagePath": "", "MessageJP": "もしかして、本日も……？", "MessageTW": "難不成，今日也……？", "MessageEN": "Are you going to...?"}, {"MessageId": 100430090, "Flag": 2, "Type": 3, "NextId": 100430100, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、今日も一日中シャーレにいる予定だけど……どうしたの？", "MessageTW": "嗯，今天也預計一整天都會待在夏萊……怎麼了嗎？", "MessageEN": "I'm going to be at Schale all day today. What's up?"}, {"MessageId": 100430100, "Flag": 2, "Type": 0, "NextId": 100430101, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうでしたか……無慈悲な太陽の熱を避けられるのは良いことですが……", "MessageTW": "是這樣啊……雖然可以躲避無慈悲的太陽的熱能是件好事……", "MessageEN": "I see... I'm glad you're out of the heat during this merciless summer, but..."}, {"MessageId": 100430101, "Flag": 2, "Type": 0, "NextId": 100430110, "MessageType": "Text", "ImagePath": "", "MessageJP": "つまりあなた様にとって今日は、全く夏らしくない一日になってしまうのですね……", "MessageTW": "也就是今天對閣下來說，會是個完全不像夏季的一天呢……", "MessageEN": "...then you won't get to see the beautiful day."}, {"MessageId": 100430110, "Flag": 1, "Type": 0, "NextId": 100430120, "MessageType": "Text", "ImagePath": "", "MessageJP": "ええ……それは、あまりにも……", "MessageTW": "沒錯……那樣，也太……", "MessageEN": "Yes, indeed."}, {"MessageId": 100430120, "Flag": 2, "Type": 3, "NextId": 100430130, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと……ワカモ？", "MessageTW": "那個……若藻？", "MessageEN": "Huh? What do you mean?"}, {"MessageId": 100430130, "Flag": 2, "Type": 0, "NextId": 100430131, "MessageType": "Text", "ImagePath": "", "MessageJP": "よくわかりました", "MessageTW": "我明白了", "MessageEN": "I've got it!"}, {"MessageId": 100430131, "Flag": 2, "Type": 0, "NextId": 100430140, "MessageType": "Text", "ImagePath": "", "MessageJP": "あなた様のためなら、このワカモ一肌脱いでご覧にいれましょう", "MessageTW": "如果是為了閣下的話，若藻我就奮力相助吧", "MessageEN": "I'll do everything and anything for you."}, {"MessageId": 100430140, "Flag": 1, "Type": 0, "NextId": 100430150, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうぞご期待なさってくださいませ♥", "MessageTW": "敬請期待♥", "MessageEN": "Please stay tuned."}, {"MessageId": 100430150, "Flag": 2, "Type": 3, "NextId": 100430160, "MessageType": "Text", "ImagePath": "", "MessageJP": "え……？", "MessageTW": "咦……？", "MessageEN": "Huh...? Um... Wakamo?"}, {"MessageId": 100430160, "Flag": 2, "Type": 3, "NextId": 100420160, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ど、どういう意味？", "MessageTW": "……什，什麼意思？", "MessageEN": "...Hey?"}, {"MessageId": 100420160, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ワカモ（水着）の絆イベントへ", "MessageTW": "前往若藻(泳裝)的羈絆劇情", "MessageEN": "Go to Wakamo (Swimsuit)'s Event"}]