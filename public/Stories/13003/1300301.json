[{"ImagePath": "https://schale.gg/images/student/collection/13003.webp", "MessageJP": "ハスミ", "MessageTW": "蓮實", "MessageEN": "Hasumi"}, {"MessageId": 130020010, "Flag": 2, "Type": 4, "NextId": 130030016, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 130030016, "Flag": 2, "Type": 0, "NextId": 130030023, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは、先生", "MessageTW": "您好，老師", "MessageEN": "Sensei."}, {"MessageId": 130030023, "Flag": 1, "Type": 0, "NextId": 130030020, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっと個人的に聞いていただきたいたいことがありまして……", "MessageTW": "我有點私事想要詢問您……", "MessageEN": "There's something I'd like to ask you personally."}, {"MessageId": 130030020, "Flag": 2, "Type": 0, "NextId": 130030030, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしければ、相談に乗っていただけませんでしょうか？", "MessageTW": "如果可以的話，能和您商量一下嗎？", "MessageEN": "Is it okay if I consult you for advice?"}, {"MessageId": 130030030, "Flag": 2, "Type": 3, "NextId": 130030041, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん", "MessageTW": "當然", "MessageEN": "Sure. Whatever you need, Hasumi."}, {"MessageId": 130030030, "Flag": 2, "Type": 3, "NextId": 130030041, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつでも呼んで", "MessageTW": "隨時都能找我", "MessageEN": "You can call me anytime."}, {"MessageId": 130030041, "Flag": 2, "Type": 0, "NextId": 130030048, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、ありがとうございます！", "MessageTW": "非，非常感謝！", "MessageEN": "Oh."}, {"MessageId": 130030048, "Flag": 2, "Type": 0, "NextId": 130030045, "MessageType": "Text", "ImagePath": "", "MessageJP": "す、すみません、つい……", "MessageTW": "不，不好意思，忍不住就……", "MessageEN": "Great!"}, {"MessageId": 130030045, "Flag": 2, "Type": 0, "NextId": 130030042, "MessageType": "Text", "ImagePath": "", "MessageJP": "その、断られてしまったらどうしようとばかり考えていましたので……", "MessageTW": "那個，一直想說如果被拒絕的話怎麼辦……", "MessageEN": "Thank you. I was worried you would say no