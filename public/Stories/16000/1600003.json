[{"ImagePath": "https://schale.gg/images/student/collection/16000.webp", "MessageJP": "ハルカ", "MessageTW": "遙香", "MessageEN": "Haruka"}, {"MessageId": 159990160, "Flag": 2, "Type": 4, "NextId": 160000168, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 160000168, "Flag": 2, "Type": 0, "NextId": 160000175, "MessageType": "Text", "ImagePath": "", "MessageJP": "こんにちは先生、何度もすみません私ハルカでｓ", "MessageTW": "老師好，又打擾您非常抱歉我是遙香ㄤ", "MessageEN": "Hello, Sensei. I know I'm probably out of line, and shouldn't be asking you for something like this, but... My name is Haruka, by the way..."}, {"MessageId": 160000175, "Flag": 1, "Type": 0, "NextId": 160000182, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 160000182, "Flag": 1, "Type": 0, "NextId": 160000190, "MessageType": "Text", "ImagePath": "", "MessageJP": "何でもありません、すみません", "MessageTW": "沒什麼，對不起", "MessageEN": "Actually, it's nothing. Never mind."}, {"MessageId": 160000190, "Flag": 2, "Type": 3, "NextId": 160000203, "MessageType": "Text", "ImagePath": "", "MessageJP": "おはよう、ハルカ", "MessageTW": "早安，遙香", "MessageEN": "Hello, Haruka."}, {"MessageId": 160000190, "Flag": 2, "Type": 3, "NextId": 160000203, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？続けて？", "MessageTW": "怎麼了？繼續說吧？", "MessageEN": "It's okay. What do you need?"}, {"MessageId": 160000203, "Flag": 2, "Type": 0, "NextId": 160000200, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 160000200, "Flag": 2, "Type": 0, "NextId": 160000207, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます", "MessageTW": "非常感謝", "MessageEN": "Thank you."}, {"MessageId": 160000207, "Flag": 2, "Type": 0, "NextId": 160000214, "MessageType": "Text", "ImagePath": "", "MessageJP": "もし", "MessageTW": "如果", "MessageEN": "By any chance..."}, {"MessageId": 160000214, "Flag": 1, "Type": 0, "NextId": 160000211, "MessageType": "Text", "ImagePath": "", "MessageJP": "もし、私にもう一度チャンスをくださるのであれば、", "MessageTW": "如果能再給我一次機會的話", "MessageEN": "If you could please give me another opportunity,"}, {"MessageId": 160000211, "Flag": 2, "Type": 0, "NextId": 160000218, "MessageType": "Text", "ImagePath": "", "MessageJP": "私、今度こそ頑張ります", "MessageTW": "我這次會努力的", "MessageEN": "I'll do my best this time."}, {"MessageId": 160000218, "Flag": 2, "Type": 0, "NextId": 160000215, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつでも時間の空いた時にご連絡を頂ければ、すぐに駆け付けます", "MessageTW": "有空的時候隨時能聯絡我，我會馬上趕去", "MessageEN": "Please let me know when you're free."}, {"MessageId": 160000215, "Flag": 2, "Type": 0, "NextId": 159990220, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしくお願いいたします", "MessageTW": "再麻煩您了", "MessageEN": "Thank you."}, {"MessageId": 159990220, "Flag": 2, "Type": 2, "NextId": 160000222, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハルカの絆イベントへ", "MessageTW": "前往遙香的羈絆劇情", "MessageEN": "Go to Haruka's Event"}, {"MessageId": 160000222, "Flag": 2, "Type": 0, "NextId": 160000229, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は本当にありがとうございました", "MessageTW": "今天真的非常感謝", "MessageEN": "Thanks so much for today, Sensei!"}, {"MessageId": 160000229, "Flag": 2, "Type": 0, "NextId": 160000226, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、いえ", "MessageTW": "啊，不", "MessageEN": "Never mind."}, {"MessageId": 160000226, "Flag": 2, "Type": 0, "NextId": 160000223, "MessageType": "Text", "ImagePath": "", "MessageJP": "その", "MessageTW": "那個", "MessageEN": "Um..."}, {"MessageId": 160000223, "Flag": 2, "Type": 0, "NextId": 160000220, "MessageType": "Text", "ImagePath": "", "MessageJP": "「今日は」ということを強調したかったわけではなくて……", "MessageTW": "我沒有想強調「今天」的意思……", "MessageEN": "That's not to say I'm only grateful for today!"}, {"MessageId": 160000220, "Flag": 2, "Type": 0, "NextId": 160000237, "MessageType": "Text", "ImagePath": "", "MessageJP": "いつもです", "MessageTW": "是一直", 