[{"ImagePath": "https://schale.gg/images/student/collection/10027.webp", "MessageJP": "カリン（バニーガール）", "MessageTW": "花凛(兔女郎)", "MessageEN": "Karin (Bunny)"}, {"MessageId": 100260110, "Flag": 2, "Type": 4, "NextId": 100270110, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100270110, "Flag": 2, "Type": 0, "NextId": 100270120, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今ちょっと良い？", "MessageTW": "老師，現在方便嗎？", "MessageEN": "Sensei, are you free right now?"}, {"MessageId": 100270120, "Flag": 1, "Type": 0, "NextId": 100270121, "MessageType": "Text", "ImagePath": "", "MessageJP": "この間の件だけど", "MessageTW": "關於上次的事", "MessageEN": "It's nothing serious. I just don't think saying thanks is enough."}, {"MessageId": 100270121, "Flag": 2, "Type": 0, "NextId": 100270130, "MessageType": "Text", "ImagePath": "", "MessageJP": "きちんとお返しをしようと思って", "MessageTW": "我想好好地報答你", "MessageEN": "I'd like to repay you for all the help you've given me."}, {"MessageId": 100270130, "Flag": 1, "Type": 0, "NextId": 100270131, "MessageType": "Text", "ImagePath": "", "MessageJP": "潜入捜査中だから、事態が動いたらお返しどころじゃなくなるかもだけど……今は落ち着いてるから、多分大丈夫", "MessageTW": "因為正在進行潛入調查，如果情勢改變的話，也許就無法回禮了……但是目前還好，大概沒問題", "MessageEN": "I'm currently on an undercover investigation, and it happens to be customer service related."}, {"MessageId": 100270131, "Flag": 2, "Type": 0, "NextId": 100270140, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうかな？", "MessageTW": "怎麼樣？", "MessageEN": "I'd like to invite you over if you're not too busy. Would that be okay?"}, {"MessageId": 100270140, "Flag": 2, "Type": 3, "NextId": 100270150, "MessageType": "Text", "ImagePath": "", "MessageJP": "それはもう喜んで", "MessageTW": "那我會非常高興", "MessageEN": "I'd love that. Thanks."}, {"MessageId": 100270140, "Flag": 2, "Type": 3, "NextId": 100270160, "MessageType": "Text", "ImagePath": "", "MessageJP": "この日のために働いてたと言っても過言じゃない！", "MessageTW": "說是為了這一天而工作也不為過！", "MessageEN": "I've been waiting for this day!"}, {"MessageId": 100270150, "Flag": 2, "Type": 0, "NextId": 100270170, "MessageType": "Text", "ImagePath": "", "MessageJP": "良かった", "MessageTW": "太好了", "MessageEN": "Hmm."}, {"MessageId": 100270160, "Flag": 1, "Type": 0, "NextId": 100270170, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 100270170, "Flag": 1, "Type": 0, "NextId": 100270171, "MessageType": "Text", "ImagePath": "", "MessageJP": "まあとにかく", "MessageTW": "嘛，總之", "MessageEN": "Confirmed."}, {"MessageId": 100270171, "Flag": 2, "Type": 0, "NextId": 100260180, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、また後で", "MessageTW": "那麼，晚點見", "MessageEN": "Anyway, I'll see you then."}, {"MessageId": 100260180, "Flag": 2, "Type": 2, "NextId": 100270180, "MessageType": "Text", "ImagePath": "", "MessageJP": "カリン（バニーガール）の絆イベントへ", "MessageTW": "前往花凛(兔女郎)的羈絆劇情", "MessageEN": "Go to Karin (Bunny)'s Event"}, {"MessageId": 100270180, "Flag": 2, "Type": 0, "NextId": 100270190, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "..."}, {"MessageId": 100270190, "Flag": 1, "Type": 0, "NextId": 100270191, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……", "MessageTW": "那個……", "MessageEN": "Um."}, {"MessageId": 100270191, "Flag": 2, "Type": 0, "NextId": 100270200, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日のことなんだけど……", "MessageTW": "關於今天的事……", "MessageEN": "Sensei, about what happened today..."}, {"MessageId": 100270200, "Flag": 1, "Type": 0, "NextId": 100270201, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 100270201, "Flag": 2, "Type": 0, "NextId": 100270210, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや", "MessageTW": "不", "MessageEN": "Hmm."}, {"MessageId": 100270210, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "やっぱり大丈夫、今度また直接", "MessageTW":