[{"ImagePath": "https://schale.gg/images/student/collection/23006.webp", "MessageJP": "シズコ", "MessageTW": "靜子", "MessageEN": "Shizuko"}, {"MessageId": 2300590170, "Flag": 2, "Type": 4, "NextId": 2300600174, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 2300600174, "Flag": 2, "Type": 0, "NextId": 2300600181, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生……", "MessageTW": "老師……", "MessageEN": "Sensei... "}, {"MessageId": 2300600181, "Flag": 1, "Type": 0, "NextId": 2300600188, "MessageType": "Text", "ImagePath": "", "MessageJP": "私、私……", "MessageTW": "我，我……", "MessageEN": "I... I..."}, {"MessageId": 2300600188, "Flag": 2, "Type": 0, "NextId": 2300600190, "MessageType": "Text", "ImagePath": "", "MessageJP": "シズコ、もうこれ以上我慢できません……", "MessageTW": "靜子，再也忍不住了……", "MessageEN": "I can't stand it anymore..."}, {"MessageId": 2300600190, "Flag": 2, "Type": 3, "NextId": 2300600209, "MessageType": "Text", "ImagePath": "", "MessageJP": "なら早めに行った方が良いよ", "MessageTW": "那麼早點去比較好唷", "MessageEN": "You should go before it's too late."}, {"MessageId": 2300600190, "Flag": 2, "Type": 3, "NextId": 2300600209, "MessageType": "Text", "ImagePath": "", "MessageJP": "終わったら手洗いも忘れずにね", "MessageTW": "結束後不要忘記洗手唷", "MessageEN": "Don't forget to wash your hands after you're done."}, {"MessageId": 2300600209, "Flag": 2, "Type": 0, "NextId": 2300600206, "MessageType": "Text", "ImagePath": "", "MessageJP": "今をときめく看板娘相手に何を言ってるんですか！？", "MessageTW": "現在是在對怦然心動的看板娘說些什麼啊！？", "MessageEN": "What? How can you say that to the mascot of Momoyodou?!"}, {"MessageId": 2300600206, "Flag": 2, "Type": 0, "NextId": 2300600213, "MessageType": "Text", "ImagePath": "", "MessageJP": "というかそもそも、私がそんな話をモモトークでするとでも！？", "MessageTW": "更確切地說，你怎麼認為我會在MomoTalk上討論這種事！？", "MessageEN": "Why would such a conversation even happen on MomoTalk?"}, {"MessageId": 2300600213, "Flag": 1, "Type": 0, "NextId": 2300600220, "MessageType": "Text", "ImagePath": "", "MessageJP": "ああもう、全く……", "MessageTW": "哎喲，真是的……", "MessageEN": "Huh? Oh, no..."}, {"MessageId": 2300600220, "Flag": 1, "Type": 0, "NextId": 2300600230, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、それで先生。シズコはですね～", "MessageTW": "那個，老師。我是靜子呀～", "MessageEN": "Hmm...Sensei...I..."}, {"MessageId": 2300600230, "Flag": 2, "Type": 3, "NextId": 2300600244, "MessageType": "Text", "ImagePath": "", "MessageJP": "この流れ、まだ続きそう？", "MessageTW": "還要繼續那套流程？", "MessageEN": "Want to start over?"}, {"MessageId": 2300600244, "Flag": 2, "Type": 0, "NextId": 2300600251, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ううっ、もういいです……", "MessageTW": "……嗚嗚，算了……", "MessageEN": "Ugh... Fine."}, {"MessageId": 2300600251, "Flag": 1, "Type": 0, "NextId": 2300600258, "MessageType": "Text", "ImagePath": "", "MessageJP": "お祭り運営委員会の委員長として、先生に一つお願いがあります", "MessageTW": "身為節慶營運管理部的部長，有件事想拜託老師！", "MessageEN": "As president of the Festival Operations Department, I would like to make a request!"}, {"MessageId": 2300600258, "Flag": 2, "Type": 0, "NextId": 2300600260, "MessageType": "Text", "ImagePath": "", "MessageJP": "明日のお祭りの準備、付き合ってください！", "MessageTW": "請陪我準備明天的祭典！", "MessageEN": "Please come help us prepare for the festival!"}, {"MessageId": 2300600260, "Flag": 2, "Type": 3, "NextId": 2300600279, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは全然いいけど……さっきの話は？", "MessageTW": "嗯，OK", "MessageEN": "That's fine, but... What were you saying earlier?"}, {"MessageId": 2300600260, "Flag": 2, "Type": 3, "NextId": 2300600279, "MessageType": "Text", "ImagePath": "", "MessageJP": "それで結局、何が我慢できなかったの？", "MessageTW": "所以，到底是什麼忍不住？", "MessageEN": "Then why did you say you can't stand it anymore?"}, {"MessageId": 2300600279, "Flag": 2, "Type": 0, "NextId": 2300600276, "MessageType": "Text", "ImagePath": "", "MessageJP": "それはもういいんですっ！", "MessageTW": "那個已經無所謂了！", "MessageEN": "End of conversation!"}, {"MessageId": 2300600276, "Flag": 2, "Type": 0, "NextId": 2300590270, "MessageType": "Text", "ImagePath": "", "MessageJP": "はいもうこの話終わり！蒸し返すの禁止です！！", "MessageTW": "對，該話題到此結束！禁止再提起！！", "MessageEN": "It's too embarrassing! Let's talk about something else!"}, {"MessageId": 2300590270, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "シズコの絆イベントへ", "MessageTW": "前往靜子的羈絆劇情", "MessageEN": "Go to Shizuko's Event"}]