[{"ImagePath": "https://schale.gg/images/student/collection/10029.webp", "MessageJP": "ナツ", "MessageTW": "夏", "MessageEN": "Natsu"}, {"MessageId": 100280260, "Flag": 2, "Type": 4, "NextId": 100290260, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100290260, "Flag": 2, "Type": 0, "NextId": 100290270, "MessageType": "Text", "ImagePath": "", "MessageJP": "暖かい太陽、優しい風", "MessageTW": "溫暖的太陽，溫柔的風", "MessageEN": "The warm sun. The mild wind."}, {"MessageId": 100290270, "Flag": 1, "Type": 0, "NextId": 100290271, "MessageType": "Text", "ImagePath": "", "MessageJP": "空は高くて、世界は青く輝いてて", "MessageTW": "天空很高，世界很藍", "MessageEN": "The sky is high and the whole world shines blue."}, {"MessageId": 100290271, "Flag": 2, "Type": 0, "NextId": 100290280, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ流れていくにはあまりにも惜しい……", "MessageTW": "只是隨波逐流就太可惜了", "MessageEN": "A stage that would be a shame to pass up."}, {"MessageId": 100290280, "Flag": 2, "Type": 3, "NextId": 100290290, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日はどこに出かけてるの？", "MessageTW": "今天去哪兒了？", "MessageEN": "Are you outside right now?"}, {"MessageId": 100290280, "Flag": 2, "Type": 3, "NextId": 100290290, "MessageType": "Text", "ImagePath": "", "MessageJP": "ピクニックの最中だったり？", "MessageTW": "正在野餐嗎？", "MessageEN": "Are you having a picnic?"}, {"MessageId": 100290290, "Flag": 2, "Type": 0, "NextId": 100290291, "MessageType": "Text", "ImagePath": "", "MessageJP": "にひ", "MessageTW": "嘻嘻", "MessageEN": "Heehee."}, {"MessageId": 100290291, "Flag": 2, "Type": 0, "NextId": 100290300, "MessageType": "Text", "ImagePath": "", "MessageJP": "良いね、すごく良い。先生も慣れてきたね", "MessageTW": "很好，非常好。老師也習慣了呢", "MessageEN": "Yes, very good. You're used to it now, Sensei!"}, {"MessageId": 100290300, "Flag": 1, "Type": 0, "NextId": 100290301, "MessageType": "Text", "ImagePath": "", "MessageJP": "ということでこれ以上は省略", "MessageTW": "如此這般，以上省略", "MessageEN": "Therefore, I won't say anything more."}, {"MessageId": 100290301, "Flag": 2, "Type": 0, "NextId": 100280310, "MessageType": "Text", "ImagePath": "", "MessageJP": "待ってるね！", "MessageTW": "我等著唷！", "MessageEN": "I'll be waiting!"}, {"MessageId": 100280310, "Flag": 2, "Type": 2, "NextId": 100290310, "MessageType": "Text", "ImagePath": "", "MessageJP": "ナツの絆イベントへ", "MessageTW": "前往夏的羈絆劇情", "MessageEN": "Go to Natsu's Event"}, {"MessageId": 100290310, "Flag": 2, "Type": 0, "NextId": 100290320, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は本当に楽しかったよ、先生", "MessageTW": "今天真的很開心唷，老師", "MessageEN": "I had so much fun today, Sensei."}, {"MessageId": 100290320, "Flag": 1, "Type": 0, "NextId": 100290321, "MessageType": "Text", "ImagePath": "", "MessageJP": "これからも、何かあったらよろしくー", "MessageTW": "今後也請多關照，有什麼事的話就拜託了──", "MessageEN": "I look forward to more times like this. If there's something going on..."}, {"MessageId": 100290321, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "……何も無くても、ね", "MessageTW": "……即使什麼都沒有也請多關照", "MessageEN": "And, even when there isn't anything going on."}]