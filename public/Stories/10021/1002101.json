[{"ImagePath": "https://schale.gg/images/student/collection/10021.webp", "MessageJP": "アズサ（水着）", "MessageTW": "梓(泳裝)", "MessageEN": "Azusa (Swimsuit)"}, {"MessageId": 10011010, "Flag": 2, "Type": 4, "NextId": 10021010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 10021010, "Flag": 2, "Type": 0, "NextId": 10021020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、急いで確認したいことがある", "MessageTW": "老師，有件事想盡快和你確認一下", "MessageEN": "There's something to I want to check, Sensei."}, {"MessageId": 10021020, "Flag": 1, "Type": 0, "NextId": 10021030, "MessageType": "Text", "ImagePath": "", "MessageJP": "今どこ？", "MessageTW": "你現在在哪裡？", "MessageEN": "What is your location right now?"}, {"MessageId": 10021030, "Flag": 2, "Type": 3, "NextId": 10021040, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "My location? Why?"}, {"MessageId": 10021030, "Flag": 2, "Type": 3, "NextId": 10021040, "MessageType": "Text", "ImagePath": "", "MessageJP": "何かあった？", "MessageTW": "有什麼事嗎？", "MessageEN": "Is there a problem?"}, {"MessageId": 10021040, "Flag": 2, "Type": 0, "NextId": 10021050, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや、特に何か問題があったわけじゃない", "MessageTW": "不，並沒有發生什麼狀況", "MessageEN": "No, it's not a problem."}, {"MessageId": 10021050, "Flag": 1, "Type": 0, "NextId": 10021051, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ、よく考えてみたら、この辺りもそんなに安全ではないから", "MessageTW": "只是仔細想想，這附近並不是那麼安全", "MessageEN": "After further evaluation, I don't believe that place is safe either."}, {"MessageId": 10021051, "Flag": 2, "Type": 0, "NextId": 10021060, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生のことは、私が守らないと", "MessageTW": "我必須保護老師", "MessageEN": "It would be best if I guarded you myself."}, {"MessageId": 10021060, "Flag": 1, "Type": 0, "NextId": 10021070, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生は私の、護衛対象だから", "MessageTW": "因為老師是我的護衛對象