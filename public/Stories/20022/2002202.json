[{"ImagePath": "https://schale.gg/images/student/collection/20022.webp", "MessageJP": "フウカ（正月）", "MessageTW": "風華(正月)", "MessageEN": "Fuuka (New Year)"}, {"MessageId": 200210070, "Flag": 2, "Type": 4, "NextId": 200220070, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200220070, "Flag": 2, "Type": 0, "NextId": 200220080, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと……おはようございます", "MessageTW": "那個……早安", "MessageEN": "Hello, Sensei."}, {"MessageId": 200220080, "Flag": 1, "Type": 0, "NextId": 200220090, "MessageType": "Text", "ImagePath": "", "MessageJP": "起きたら、いつの間にかこんな時間に……", "MessageTW": "起來就發現已經這個時間了……", "MessageEN": "I...overslept this morning."}, {"MessageId": 200220090, "Flag": 2, "Type": 3, "NextId": 200220100, "MessageType": "Text", "ImagePath": "", "MessageJP": "フウカが寝坊、珍しい……！", "MessageTW": "風華居然睡過頭，真是少見……！", "MessageEN": "Really? You, Fuuka?!"}, {"MessageId": 200220100, "Flag": 2, "Type": 0, "NextId": 200220110, "MessageType": "Text", "ImagePath": "", "MessageJP": "お恥ずかしい限りです……", "MessageTW": "真是太丟臉了……", "MessageEN": "I know. I'm ashamed..."}, {"MessageId": 200220110, "Flag": 2, "Type": 3, "NextId": 200220120, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、この時期くらい良いんじゃないかな。可愛いし", "MessageTW": "別在意，這個時期睡過頭很正常的。又很可愛", "MessageEN": "That's as unexpected as it is adorable."}, {"MessageId": 200220120, "Flag": 2, "Type": 0, "NextId": 200220121, "MessageType": "Text", "ImagePath": "", "MessageJP": "も、もう", "MessageTW": "真，真是的", "MessageEN": "Seriously?"}, {"MessageId": 200220121, "Flag": 2, "Type": 0, "NextId": 200220130, "MessageType": "Text", "ImagePath": "", "MessageJP": "からかわないでください", "MessageTW": "請別取笑我了", "MessageEN": "Please stop teasing me..."}, {"MessageId": 200220130, "Flag": 1, "Type": 0, "NextId": 200220131, "MessageType": "Text", "ImagePath": "", "MessageJP": "ところで、その……", "MessageTW": "話說回來，那個", "MessageEN": "Um, Sensei."}, {"MessageId": 200220131, "Flag": 2, "Type": 0, "NextId": 200220132, "MessageType": "Text", "ImagePath": "", "MessageJP": "もうタイミングは過ぎてしまいましたが", "MessageTW": "雖然可能已經錯過時機了", "MessageEN": "It's last minute, and you might be busy..."}, {"MessageId": 200220132, "Flag": 2, "Type": 0, "NextId": 200220140, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしければ、一緒に少し遅い年越しそばを食べませんか？", "MessageTW": "方便的話，要不要一起吃稍微晚了點的跨年蕎麥麵呢？", "MessageEN": "But would you like to have noodles together for New Year's Eve?"}, {"MessageId": 200220140, "Flag": 2, "Type": 3, "NextId": 200220150, "MessageType": "Text", "ImagePath": "", "MessageJP": "フウカの手料理！今すぐ食べたい！", "MessageTW": "風華親手做的料理！我現在就想吃！", "MessageEN": "Of course! I couldn't miss noodles with Fuuka!"}, {"MessageId": 200220150, "Flag": 2, "Type": 0, "NextId": 200220151, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、ありがとうございます", "MessageTW": "謝，謝謝老師", "MessageEN": "Oh! I'm glad to hear that."}, {"MessageId": 200220151, "Flag": 2, "Type": 0, "NextId": 200220160, "MessageType": "Text", "ImagePath": "", "MessageJP": "少し準備があるので、夕方でもよろしいですか？", "MessageTW": "因為還需要做些準備，傍晚左右可以嗎？", "MessageEN": "I'll see you this evening, then."}, {"MessageId": 200220160, "Flag": 1, "Type": 0, "NextId": 200210160, "MessageType": "Text", "ImagePath": "", "MessageJP": "食堂でお待ちしていますね", "MessageTW": "我會在食堂等你的", "MessageEN": "I'll be waiting for you."}, {"MessageId": 200210160, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "フウカ（正月）の絆イベントへ", "MessageTW": "前往風華(正月)的羈絆劇情", "MessageEN": "Go to Fuuka (New Year)'s Event"}]