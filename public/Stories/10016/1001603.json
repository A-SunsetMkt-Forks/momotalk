[{"ImagePath": "https://schale.gg/images/student/collection/10016.webp", "MessageJP": "ミドリ", "MessageTW": "綠", "MessageEN": "Midori"}, {"MessageId": 100150130, "Flag": 2, "Type": 4, "NextId": 100160130, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100160130, "Flag": 2, "Type": 0, "NextId": 100160140, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、速報です！速報！ものすごいニュースですよ！", "MessageTW": "老師，速報！快報！不得了的消息唷！", "MessageEN": "Sensei! I have exciting news! It's crazy!"}, {"MessageId": 100160140, "Flag": 2, "Type": 3, "NextId": 100160150, "MessageType": "Text", "ImagePath": "", "MessageJP": "ミドリ、落ち着いて。一回クールダウン", "MessageTW": "綠，冷靜點。先冷靜一下", "MessageEN": "Calm down, Midori."}, {"MessageId": 100160140, "Flag": 2, "Type": 3, "NextId": 100160170, "MessageType": "Text", "ImagePath": "", "MessageJP": "ものすごいニュース？どうしよう、花火とか用意した方が良いかな！？", "MessageTW": "不得了的消息？怎麼辦，是不是該準備煙火之類的比較好！？", "MessageEN": "Exciting news? Should I get the confetti ready?!"}, {"MessageId": 100160150, "Flag": 2, "Type": 0, "NextId": 100160160, "MessageType": "Text", "ImagePath": "", "MessageJP": "……はい、深呼吸して落ち着きました！", "MessageTW": "……好，深呼吸後冷靜下來了！", "MessageEN": "*deep breath* Okay, I'm calm now!"}, {"MessageId": 100160160, "Flag": 2, "Type": 3, "NextId": 100160180, "MessageType": "Text", "ImagePath": "", "MessageJP": "何があったの？", "MessageTW": "發生了什麼事？", "MessageEN": "So what's up?"}, {"MessageId": 100160170, "Flag": 2, "Type": 0, "NextId": 100160171, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、流石に花火は要らないかと思いますが……！", "MessageTW": "那個，我想應該是不至於需要煙火啦……！", "MessageEN": "Um... I mean it's not that crazy..."}, {"MessageId": 100160171, "Flag": 2, "Type": 0, "NextId": 100160180, "MessageType": "Text", "ImagePath": "", "MessageJP": "と、とにかく！", "MessageTW": "總，總之！", "MessageEN": "Anyway!"}, {"MessageId": 100160180, "Flag": 1, "Type": 0, "NextId": 100160181, "MessageType": "Text", "ImagePath": "", "MessageJP": "今度、伝説の名作レトロゲームの続編が出るそう