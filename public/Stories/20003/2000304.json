[{"ImagePath": "https://schale.gg/images/student/collection/20003.webp", "MessageJP": "マシロ", "MessageTW": "麻白", "MessageEN": "Mashiro"}, {"MessageId": 200020190, "Flag": 2, "Type": 4, "NextId": 200030191, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200030191, "Flag": 2, "Type": 0, "NextId": 200030199, "MessageType": "Text", "ImagePath": "", "MessageJP": "以前お伝えしましたがあらためて", "MessageTW": "雖然之前說過了不過再說一次", "MessageEN": "Because of today's training,"}, {"MessageId": 200030199, "Flag": 1, "Type": 0, "NextId": 200030196, "MessageType": "Text", "ImagePath": "", "MessageJP": "本日訓練のため、当初予定していた当番は別日に変更させていただきました", "MessageTW": "今天因為訓練的關係，當初預定值班的時間將變更為其他日子", "MessageEN": "our duties have been delayed."}, {"MessageId": 200030196, "Flag": 2, "Type": 0, "NextId": 200030200, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしくお願いします", "MessageTW": "再麻煩老師了", "MessageEN": "Please keep that in mind."}, {"MessageId": 200030200, "Flag": 2, "Type": 3, "NextId": 200030219, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、了解。ところで今日の訓練、一緒に行ってもいい？", "MessageTW": "嗯，了解。話說今天的訓練，我可以一起去嗎？", "MessageEN": "Can I go to your training with you?"}, {"MessageId": 200030219, "Flag": 2, "Type": 0, "NextId": 200030220, "MessageType": "Text", "ImagePath": "", "MessageJP": "訓練を手伝ってくださるんですか？", "MessageTW": "要幫助我訓練嗎？", "MessageEN": "You want to help with my training today?"}, {"MessageId": 200030220, "Flag": 2, "Type": 3, "NextId": 200030233, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、良いかな？", "MessageTW": "嗯，可以嗎？", "MessageEN": "Yes, is there a problem?"}, {"MessageId": 200030233, "Flag": 2, "Type": 0, "NextId": 200030240, "MessageType": "Text", "ImagePath": "", "MessageJP": "問題は無いのですが……今日のは少々大変なので、覚悟された方が良いかと", "MessageTW": "雖然沒有問題……但今天的訓練會有點辛苦，最好做好心理準備", "MessageEN": "That's fine, but today's training is supposed to be a bit intense... Please keep that in mind."}, {"MessageId": 200030240, "Flag": 2, "Type": 3, "NextId": 200020250, "MessageType": "Text", "ImagePath": "", "MessageJP": "( ºㅁº )", "MessageTW": "( ºㅁº )", "MessageEN": "( ºㅁº )"}, {"MessageId": 200020250, "Flag": 2, "Type": 2, "NextId": 200030257, "MessageType": "Text", "ImagePath": "", "MessageJP": "マシロの絆イベントへ", "MessageTW": "前往麻白的羈絆劇情", "MessageEN": "Go to Mashiro's Event"}, {"MessageId": 200030257, "Flag": 2, "Type": 0, "NextId": 200030254, "MessageType": "Text", "ImagePath": "", "MessageJP": "あの、先生……大丈夫でしたか？", "MessageTW": "那個，老師……沒事吧？", "MessageEN": "Um... Sensei? Are you okay?"}, {"MessageId": 200030254, "Flag": 2, "Type": 0, "NextId": 200030260, "MessageType": "Text", "ImagePath": "", "MessageJP": "帰り道、どこか魂が抜けたような表情をされていましたが……", "MessageTW": "回去的路上，總覺得表情看起來失魂落魄的……", "MessageEN": "You turned so pale..."}, {"MessageId": 200030260, "Flag": 2, "Type": 3, "NextId": 200030278, "MessageType": "Text", "ImagePath": "", "MessageJP": "( ºㅁº )", "MessageTW": "( ºㅁº )", "MessageEN": "( ºㅁº )"}, {"MessageId": 200030278, "Flag": 2, "Type": 0, "NextId": 200030280, "MessageType": "Text", "ImagePath": "", "MessageJP": "やはり、いきなりあの訓練は少し厳しすぎたでしょうか……", "MessageTW": "突然進行那個訓練果然太嚴格了……", "MessageEN": "I told you the training was intense..."}, {"MessageId": 200030280, "Flag": 2, "Type": 3, "NextId": 200030292, "MessageType": "Text", "ImagePath": "", "MessageJP": "( ºㅁº )", "MessageTW": "( ºㅁº )", "MessageEN": "( ºㅁº )"}, {"MessageId": 200030292, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生！？", "MessageTW": "老師！？", "MessageEN": "Sensei?!"}]