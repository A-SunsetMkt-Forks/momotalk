[{"ImagePath": "https://schale.gg/images/student/collection/10021.webp", "MessageJP": "アズサ（水着）", "MessageTW": "梓(泳裝)", "MessageEN": "Azusa (Swimsuit)"}, {"MessageId": 10011120, "Flag": 2, "Type": 4, "NextId": 10021120, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 10021120, "Flag": 2, "Type": 0, "NextId": 10021130, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Hmm."}, {"MessageId": 10021130, "Flag": 1, "Type": 0, "NextId": 10021340, "MessageType": "Text", "ImagePath": "", "MessageJP": "今、時間ある？", "MessageTW": "現在有時間嗎？", "MessageEN": "Are you free right now, Sensei?"}, {"MessageId": 10021340, "Flag": 1, "Type": 0, "NextId": 10021341, "MessageType": "Text", "ImagePath": "", "MessageJP": "何て言えば良いのか", "MessageTW": "怎麼說才好呢", "MessageEN": "I could use some"}, {"MessageId": 10021341, "Flag": 2, "Type": 0, "NextId": 10021342, "MessageType": "Text", "ImagePath": "", "MessageJP": "実はその", "MessageTW": "其實我現在……", "MessageEN": "support"}, {"MessageId": 10021342, "Flag": 2, "Type": 0, "NextId": 10021350, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生の助けが必要で", "MessageTW": "需要老師的幫助", "MessageEN": "right about now."}, {"MessageId": 10021350, "Flag": 2, "Type": 3, "NextId": 10021360, "MessageType": "Text", "ImagePath": "", "MessageJP": "まさかの非常事態！？", "MessageTW": "難道有緊急狀況！？", "MessageEN": "You need support? Are you in battle?!"}, {"MessageId": 10021350, "Flag": 2, "Type": 3, "NextId": 10021360, "MessageType": "Text", "ImagePath": "", "MessageJP": "すぐ行く、今どの辺り？", "MessageTW": "馬上過去，你現在在哪兒？", "MessageEN": "Are you all right? What's your location?!"}, {"MessageId": 10021360, "Flag": 2, "Type": 0, "NextId": 10021361, "MessageType": "Text", "ImagePath": "", "MessageJP": "この後、位置情報を送る", "MessageTW": "等等我會傳送位置資訊", "MessageEN": "Yes."}, {"MessageId": 10021361, "Flag": 2, "Type": 0, "NextId": 10021370, "MessageType": "Text", "ImagePath": "", "MessageJP": "できるだけ早く来てくれると嬉しい", "MessageTW": "希望你能盡快過來", "MessageEN": "I'll send you the details."}, {"MessageId": 10021370, "Flag": 1, "Type": 0, "NextId": 10021380, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、でも無理はしないで", "MessageTW": "啊，但是不要勉強", "MessageEN": "Earlier the better, but take your time."}, {"MessageId": 10021380, "Flag": 2, "Type": 3, "NextId": 10011390, "MessageType": "Text", "ImagePath": "", "MessageJP": "急いで行くね、待ってて", "MessageTW": "我盡快到，等著", "MessageEN": "I'm on my way."}, {"MessageId": 10011390, "Flag": 2, "Type": 2, "NextId": 10021390, "MessageType": "Text", "ImagePath": "", "MessageJP": "アズサ（水着）の絆イベントへ", "MessageTW": "前往梓(泳裝)的羈絆劇情", "MessageEN": "Go to Azusa (Swimsuit)'s Event"}, {"MessageId": 10021390, "Flag": 2, "Type": 0, "NextId": 10021391, "MessageType": "Text", "ImagePath": "", "MessageJP": "さっきはごちそうさま。ありがとう、先生", "MessageTW": "剛才多謝款待。謝謝老師", "MessageEN": "I enjoyed the meal, Sensei. Thank you."}, {"MessageId": 10021391, "Flag": 2, "Type": 0, "NextId": 10021392, "MessageType": "Text", "ImagePath": "", "MessageJP": "さっきも言ったように、言葉だけで済ませたりしないから", "MessageTW": "就像剛才說過的，光用說的無法表達我的謝意", "MessageEN": "And as I said, I don't thank people without meaning."}, {"MessageId": 10021392, "Flag": 2, "Type": 0, "NextId": 10021400, "MessageType": "Text", "ImagePath": "", "MessageJP": "この借りは、いつか必ず返す", "MessageTW": "這筆債，總有一天必定會還", "MessageEN": "I'll pay you back sometime later."}, {"MessageId": 10021400, "Flag": 2, "Type": 3, "NextId": 10021410, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、楽しみにしてるね", "MessageTW": "嗯，很期待唷", "MessageEN": "Sure, I'll look forward to it."}, {"MessageId": 10021400, "Flag": 2, "Type": 3, "NextId": 10021420, "MessageType": "Text", "ImagePath": "", "MessageJP": "何だか、復讐でもされそうな台詞だね……", "MessageTW": "怎麼，這句話聽起來像是要復仇一樣……", "MessageEN": "You sound like you're out for revenge when you put it like that."}, {"MessageId": 10021410, "Flag": 2, "Type": 0, "NextId": 10021430, "MessageType": "Text", "ImagePath": "", "MessageJP": "期待には応えるよ", "MessageTW": "不會辜負你的期待的", "MessageEN": "Yes. Good."}, {"MessageId": 10021420, "Flag": 1, "Type": 0, "NextId": 10021421, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 10021421, "Flag": 2, "Type": 0, "NextId": 10021430, "MessageType": "Text", "ImagePath": "", "MessageJP": "そういうのじゃない……こんなにお世話になってるのに", "MessageTW": "不是那樣的……我都這麼受你關照了", "MessageEN": "It's not like that."}, {"MessageId": 10021430, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあまた、先生", "MessageTW": "那麼，回頭見，老師", "MessageEN": "I'll speak with you later, Sensei."}]