[{"ImagePath": "https://schale.gg/images/student/collection/23005.webp", "MessageJP": "アヤネ", "MessageTW": "綾音", "MessageEN": "Ayane"}, {"MessageId": 230040270, "Flag": 2, "Type": 4, "NextId": 230050278, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 230050278, "Flag": 2, "Type": 0, "NextId": 230050285, "MessageType": "Text", "ImagePath": "", "MessageJP": "最近、学校の施設の故障が続いているのが、ちょっと心配でして", "MessageTW": "最近，學校的設施持續故障，有點擔心", "MessageEN": "I'm worried because it feels like the school is falling apart."}, {"MessageId": 230050285, "Flag": 1, "Type": 0, "NextId": 230050282, "MessageType": "Text", "ImagePath": "", "MessageJP": "昨日は停電が2時間も続きましたし……", "MessageTW": "昨天停電持續了2個小時……", "MessageEN": "We had two blackouts yesterday."}, {"MessageId": 230050282, "Flag": 2, "Type": 0, "NextId": 230050299, "MessageType": "Text", "ImagePath": "", "MessageJP": "一昨日は水道も止まってしまいました", "MessageTW": "前天自來水也停了", "MessageEN": "And we had no water the other day."}, {"MessageId": 230050299, "Flag": 1, "Type": 0, "NextId": 230050300, "MessageType": "Text", "ImagePath": "", "MessageJP": "地下の施設に、何か問題でも起きたのでしょうか……", "MessageTW": "地下設施出了什麼問題嗎……", "MessageEN": "I wonder if something's wrong with the basement facilities?"}, {"MessageId": 230050300, "Flag": 2, "Type": 3, "NextId": 230050310, "MessageType": "Text", "ImagePath": "", "MessageJP": "業者の人を呼んでみたら？", "MessageTW": "叫業者來看看？", "MessageEN": "Why don't you call a technician?"}, {"MessageId": 230050300, "Flag": 2, "Type": 3, "NextId": 230050310, "MessageType": "Text", "ImagePath": "", "MessageJP": "誰か分かる人を呼んで、見てもらったらいいんじゃない？", "MessageTW": "叫個懂這些的人來看看不就行了嗎？", "MessageEN": "You should call someone to do an inspection."}, {"MessageId": 230050310, "Flag": 2, "Type": 0, "NextId": 230050317, "MessageType": "Text", "ImagePath": "", "MessageJP": "怪しい所を全部業者さんに見てもらうと、すごい金額になりそうなので……", "MessageTW": "如果每個有問題的地方都請業者去看的話，可能會是筆龐大的金額……", "MessageEN": "Hahaha... We don't have the budget."}, {"MessageId": 230050317, "Flag": 2, "Type": 0, "NextId": 230050320, "MessageType": "Text", "ImagePath": "", "MessageJP": "時間を見つけて、少しずつお休みの日にでも直さないと……", "MessageTW": "只好找時間，在休息日一點一點地修復了……", "MessageEN": "I'll just have to carve time out to fix things here and there."}, {"MessageId": 230050320, "Flag": 2, "Type": 3, "NextId": 230050338, "MessageType": "Text", "ImagePath": "", "MessageJP": "私も手伝う？", "MessageTW": "我也能幫忙嗎？", "MessageEN": "Do you need help?"}, {"MessageId": 230050320, "Flag": 2, "Type": 3, "NextId": 230050338, "MessageType": "Text", "ImagePath": "", "MessageJP": "一緒に直そう", "MessageTW": "我們一起修理吧", "MessageEN": "Let's fix them together."}, {"MessageId": 230050338, "Flag": 2, "Type": 0, "NextId": 230050335, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ……本当ですか？", "MessageTW": "啊……真的嗎？", "MessageEN": "Do you mean that?"}, {"MessageId": 230050335, "Flag": 2, "Type": 0, "NextId": 230040340, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、お言葉に甘えて。ありがとうございます、先生", "MessageTW": "那就恭敬不如從命了。非常謝謝，老師", "MessageEN": "That would be amazing, Sensei!"}, {"MessageId": 230040340, "Flag": 2, "Type": 2, "NextId": 230050342, "MessageType": "Text", "ImagePath": "", "MessageJP": "アヤネの絆イベントへ", "MessageTW": "前往綾音的羈絆劇情", "MessageEN": "Go to Ayane's Event"}, {"MessageId": 230050342, "Flag": 2, "Type": 0, "NextId": 230050349, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日はお疲れ様でした。先生", "MessageTW": "今天辛苦了。老師", "MessageEN": "You worked so hard today, Sensei."}, {"MessageId": 230050349, "Flag": 2, "Type": 0, "NextId": 230050350, "MessageType": "Text", "ImagePath": "", "MessageJP": "おかげさまで、無事に修理を終えることができました", "MessageTW": "託你的福，順利完成了修理", "MessageEN": "Everything at the school is fixed now."}, {"MessageId": 230050350, "Flag": 2, "Type": 3, "NextId": 230050360, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、お疲れ様", "MessageTW": "嗯，辛苦了", "MessageEN": "You did a great job today."}, {"MessageId": 230050350, "Flag": 2, "Type": 3, "NextId": 230050360, "MessageType": "Text", "ImagePath": "", "MessageJP": "アヤネも、お疲れ様", "MessageTW": "綾音也辛苦了", "MessageEN": "Great work, Ayane."}, {"MessageId": 230050360, "Flag": 2, "Type": 0, "NextId": 230050377, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと……あの……", "MessageTW": "呃……這個……", "MessageEN": "Oh, um..."}, {"MessageId": 230050377, "Flag": 1, "Type": 0, "NextId": 230050380, "MessageType": "Text", "ImagePath": "", "MessageJP": "その……", "MessageTW": "那個……", "MessageEN": "So..."}, {"MessageId": 230050380, "Flag": 2, "Type": 3, "NextId": 230050398, "MessageType": "Text", "ImagePath": "", "MessageJP": "ん？", "MessageTW": "嗯？", "MessageEN": "Huh?"}, {"MessageId": 230050380, "Flag": 2, "Type": 3, "NextId": 230050398, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "What's wrong?"}, {"MessageId": 230050398, "Flag": 2, "Type": 0, "NextId": 230050405, "MessageType": "Text", "ImagePath": "", "MessageJP": "また……お呼びしてもいいですか？その……", "MessageTW": "還能再……叫你出來嗎？那個……", "MessageEN": "Um... Can I call you again next time?"}, {"MessageId": 230050405, "Flag": 1, "Type": 0, "NextId": 230050410, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は、ご迷惑をおかけしてしまったのですが……私、先生と……", "MessageTW": "很抱歉今天給您添麻煩了……老師和我……", "MessageEN": "I asked a lot of you today, but..."}, {"MessageId": 230050410, "Flag": 2, "Type": 3, "NextId": 230050426, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろんだよ", "MessageTW": "當然囉", "MessageEN": "Of course."}, {"MessageId": 230050410, "Flag": 2, "Type": 3, "NextId": 230050426, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん、いつでも呼んで", "MessageTW": "當然，隨時都歡迎喔", "MessageEN": "Call me anytime."}, {"MessageId": 230050426, "Flag": 2, "Type": 0, "NextId": 230050423, "MessageType": "Text", "ImagePath": "", "MessageJP": "……ありがとうございます、先生", "MessageTW": "……非常感謝，老師", "MessageEN": "Hahaha... Thank you, Sensei."}, {"MessageId": 230050423, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "それでは、またご連絡しますね", "MessageTW": "那麼，再連絡唷", "MessageEN": "See you later."}]