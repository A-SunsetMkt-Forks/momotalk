[{"ImagePath": "https://schale.gg/images/student/collection/20019.webp", "MessageJP": "アカネ（バニーガール）", "MessageTW": "朱音(兔女郎)", "MessageEN": "Akane (Bunny)"}, {"MessageId": 200180010, "Flag": 2, "Type": 4, "NextId": 200190010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200190010, "Flag": 2, "Type": 0, "NextId": 200190020, "MessageType": "Text", "ImagePath": "", "MessageJP": "おはようございます、ご主人様", "MessageTW": "您好，主人", "MessageEN": "Hello, Master."}, {"MessageId": 200190020, "Flag": 1, "Type": 0, "NextId": 200190021, "MessageType": "Text", "ImagePath": "", "MessageJP": "メイドとは、奉仕を仕事とする者", "MessageTW": "所謂女僕，是以服務為業的人", "MessageEN": "A maid's purpose is to serve."}, {"MessageId": 200190021, "Flag": 2, "Type": 0, "NextId": 200190030, "MessageType": "Text", "ImagePath": "", "MessageJP": "ご主人様に仕える者は、メイド服を着る……", "MessageTW": "侍奉主人的人，就該穿著女僕裝……", "MessageEN": "So, at its core, a maid uniform is one of service."}, {"MessageId": 200190030, "Flag": 2, "Type": 3, "NextId": 200190040, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん……そう、かな？", "MessageTW": "嗯……應該，是吧？", "MessageEN": "Uh...Yeah? I guess?"}, {"MessageId": 200190040, "Flag": 2, "Type": 0, "NextId": 200190041, "MessageType": "Text", "ImagePath": "", "MessageJP": "そう思っていたのですが、", "MessageTW": "雖然我一直是這麼認為", "MessageEN": "Yes! I'm glad you understand, Sensei."}, {"MessageId": 200190041, "Flag": 2, "Type": 0, "NextId": 200190050, "MessageType": "Text", "ImagePath": "", "MessageJP": "最近新しい気付きを得たのです", "MessageTW": "但最近我有了新的想法", "MessageEN": "And...I had a revelation recently."}, {"MessageId": 200190050, "Flag": 1, "Type": 0, "NextId": 200190060, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうぞ、楽しみにお待ちくださいね", "MessageTW": "敬請期待", "MessageEN": "Now, now. Don't get too excited."}, {"MessageId": 200190060, "Flag": 2, "Type": 3, "NextId": 200190070, "MessageType": "Text", "ImagePath": "", "MessageJP": "う……うん？", "MessageTW": "嗯……嗯？", "MessageEN": "Excited...about what exactly?"}, {"MessageId": 200190060, "Flag": 2, "Type": 3, "NextId": 200190070, "MessageType": "Text", "ImagePath": "", "MessageJP": "楽しみにしておきます？", "MessageTW": "我很期待？", "MessageEN": "Well, I'm not sure what you're talking about, but I'm looking forward to it!"}, {"MessageId": 200190070, "Flag": 2, "Type": 0, "NextId": 200190071, "MessageType": "Text", "ImagePath": "", "MessageJP": "今度お時間がありましたら、ぜひ私の部屋に寄っていただけませんか？", "MessageTW": "之後有時間的話，可以請您務必到我房裡一下嗎？", "MessageEN": "Can you stop by my room when you're free?"}, {"MessageId": 200190071, "Flag": 2, "Type": 0, "NextId": 200190080, "MessageType": "Text", "ImagePath": "", "MessageJP": "お待ちしておりますね", "MessageTW": "我會等著您的", "MessageEN": "I'll be waiting,"}, {"MessageId": 200190080, "Flag": 1, "Type": 0, "NextId": 200180080, "MessageType": "Text", "ImagePath": "", "MessageJP": "私のご主人様", "MessageTW": "我的主人", "MessageEN": "Master!"}, {"MessageId": 200180080, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "アカネ（バニーガール）の絆イベントへ", "MessageTW": "前往朱音(兔女郎)的羈絆劇情", "MessageEN": "Go to Akane (Bunny)'s Event"}]