[{"ImagePath": "https://schale.gg/images/student/collection/26002.webp", "MessageJP": "ジュリ", "MessageTW": "茱莉", "MessageEN": "Juri"}, {"MessageId": 260010010, "Flag": 2, "Type": 4, "NextId": 260020014, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 260020014, "Flag": 2, "Type": 0, "NextId": 260020020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生は、料理で一番重要な調味料って何だと思いますか？", "MessageTW": "老師覺得料理中最重要的調味料是什麼？", "MessageEN": "Sensei, guess what the most important spice is when you're cooking."}, {"MessageId": 260020020, "Flag": 2, "Type": 3, "NextId": 260020035, "MessageType": "Text", "ImagePath": "", "MessageJP": "砂糖？", "MessageTW": "糖？", "MessageEN": "Sugar?"}, {"MessageId": 260020020, "Flag": 2, "Type": 3, "NextId": 260020035, "MessageType": "Text", "ImagePath": "", "MessageJP": "塩？", "MessageTW": "鹽？", "MessageEN": "Salt?"}, {"MessageId": 260020035, "Flag": 2, "Type": 0, "NextId": 260020040, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうですね、それらも勿論大切なのですが……", "MessageTW": "是啊，那些當然也很重要，但是……", "MessageEN": "Well, that's important too."}, {"MessageId": 260020040, "Flag": 2, "Type": 3, "NextId": 260020059, "MessageType": "Text", "ImagePath": "", "MessageJP": "ですが……？", "MessageTW": "但是……？", "MessageEN": "What could be more important than that?"}, {"MessageId": 260020059, "Flag": 2, "Type": 0, "NextId": 260020056, "MessageType": "Text", "ImagePath": "", "MessageJP": "本当に大事な調味料……", "MessageTW": "真正重要的調味料……", "MessageEN": "Huh? But..."}, {"MessageId": 260020056, "Flag": 2, "Type": 0, "NextId": 260020053, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは、砂糖や塩など、そういった普通のものではありません", "MessageTW": "不是糖和鹽那種普通的東西", "MessageEN": "There's something else that's extremely important."}, {"MessageId": 260020053, "Flag": 2, "Type": 0, "NextId": 260020060, "MessageType": "Text", "ImagePath": "", "MessageJP": "正解は……", "MessageTW": "答案是……", "MessageEN": "The answer is..."}, {"MessageId": 260020060, "Flag": 1, "Type": 0, "NextId": 260020070, "MessageType": "Text", "ImagePath": "", "MessageJP": "愛情です！", "MessageTW": "愛！", "MessageEN": "LOVE!"}, {"MessageId": 260020070, "Flag": 2, "Type": 3, "NextId": 260020081, "MessageType": "Text", "ImagePath": "", "MessageJP": "愛情……？", "MessageTW": "愛……？", "MessageEN": "Love...?"}, {"MessageId": 260020070, "Flag": 2, "Type": 3, "NextId": 260020081, "MessageType": "Text", "ImagePath": "", "MessageJP": "確かに……", "MessageTW": "確實……", "MessageEN": "Well, that's what some people say."}, {"MessageId": 260020081, "Flag": 2, "Type": 0, "NextId": 260020088, "MessageType": "Text", "ImagePath": "", "MessageJP": "でしょう？", "MessageTW": "是吧？", "MessageEN": "Right?"}, {"MessageId": 260020088, "Flag": 2, "Type": 0, "NextId": 260020090, "MessageType": "Text", "ImagePath": "", "MessageJP": "ただ、私ひとつ疑問があるんです", "MessageTW": "但是，我有一個疑問", "MessageEN": "But if that's the case, I have a question."}, {"MessageId": 260020090, "Flag": 2, "Type": 3, "NextId": 260020102, "MessageType": "Text", "ImagePath": "", "MessageJP": "疑問？", "MessageTW": "疑問？", "MessageEN": "What's your question?"}, {"MessageId": 260020102, "Flag": 2, "Type": 0, "NextId": 260020109, "MessageType": "Text", "ImagePath": "", "MessageJP": "料理にとって最高の調味料は愛情、それに異存はありません", "MessageTW": "愛是料理最佳的調味料，這一點沒有異議", "MessageEN": "Well, I do agree that love is the greatest spice when it comes to cooking."}, {"MessageId": 260020109, "Flag": 2, "Type": 0, "NextId": 260020106, "MessageType": "Text", "ImagePath": "", "MessageJP": "それは良いとして……", "MessageTW": "但就算這樣……", "MessageEN": "But then..."}, {"MessageId": 260020106, "Flag": 2, "Type": 0, "NextId": 260020110, "MessageType": "Text", "ImagePath": "", "MessageJP": "料理に入れる愛情って、どれくらいが適量なのでしょうか？", "MessageTW": "在料理中要放入多少的愛才是適量的呢？", "MessageEN": "How much love do you need to put in?"}, {"MessageId": 260020110, "Flag": 2, "Type": 3, "NextId": 260020120, "MessageType": "Text", "ImagePath": "", "MessageJP": "……適量？", "MessageTW": "……適量？", "MessageEN": "How much love?"}, {"MessageId": 260020120, "Flag": 2, "Type": 0, "NextId": 260020127, "MessageType": "Text", "ImagePath": "", "MessageJP": "ほんの少し、隠し味程度でしょうか？", "MessageTW": "微微的，提味的程度嗎？", "MessageEN": "Maybe just a teensy bit, like a spice?"}, {"MessageId": 260020127, "Flag": 2, "Type": 0, "NextId": 260020124, "MessageType": "Text", "ImagePath": "", "MessageJP": "それとも、はっきりと分かるくらいに？", "MessageTW": "還是說要清楚明顯？", "MessageEN": "Or just enough for you to be able to taste it?"}, {"MessageId": 260020124, "Flag": 2, "Type": 0, "NextId": 260020131, "MessageType": "Text", "ImagePath": "", "MessageJP": "はたまたどっさり入れた方が効果的だったり……？", "MessageTW": "或者是要多放一點，效果才會好……？", "MessageEN": "Or a whole pot-load of it?"}, {"MessageId": 260020131, "Flag": 1, "Type": 0, "NextId": 260020140, "MessageType": "Text", "ImagePath": "", "MessageJP": "私、気になって夜も眠れません！", "MessageTW": "我在意到晚上也睡不著！", "MessageEN": "I can't figure it out!"}, {"MessageId": 260020140, "Flag": 2, "Type": 3, "NextId": 260020155, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、それは……", "MessageTW": "那個，那是……", "MessageEN": "U-Um?"}, {"MessageId": 260020155, "Flag": 2, "Type": 0, "NextId": 260020160, "MessageType": "Text", "ImagePath": "", "MessageJP": "その答えを探そうと思うのですが、手伝っていただけませんでしょうか？", "MessageTW": "我想找到那個答案，能幫我嗎？", "MessageEN": "Can you please help me figure this out?"}, {"MessageId": 260020160, "Flag": 2, "Type": 3, "NextId": 260010160, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、いいけど……", "MessageTW": "嗯，是可以……", "MessageEN": "What?"}, {"MessageId": 260010160, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ジュリの絆イベントへ", "MessageTW": "前往茱莉的羈絆劇情", "MessageEN": "Go to Juri's Event"}]