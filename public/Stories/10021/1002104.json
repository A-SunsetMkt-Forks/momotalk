[{"ImagePath": "https://schale.gg/images/student/collection/10021.webp", "MessageJP": "アズサ（水着）", "MessageTW": "梓(泳裝)", "MessageEN": "Azusa (Swimsuit)"}, {"MessageId": 10011570, "Flag": 2, "Type": 4, "NextId": 10021570, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 10021570, "Flag": 2, "Type": 0, "NextId": 10021580, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei."}, {"MessageId": 10021580, "Flag": 1, "Type": 0, "NextId": 10021590, "MessageType": "Text", "ImagePath": "", "MessageJP": "今、時間ある？", "MessageTW": "現在有時間嗎？", "MessageEN": "Can you spare some time?"}, {"MessageId": 10021590, "Flag": 2, "Type": 3, "NextId": 10021600, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫だよ、どうしたの？", "MessageTW": "沒問題唷，怎麼了？", "MessageEN": "Spare some time? Is there something wrong?"}, {"MessageId": 10021590, "Flag": 2, "Type": 3, "NextId": 10021600, "MessageType": "Text", "ImagePath": "", "MessageJP": "助けに行った方が良い感じ？", "MessageTW": "感覺最好去幫忙？", "MessageEN": "Do you need more support?"}, {"MessageId": 10021600, "Flag": 2, "Type": 0, "NextId": 10021601, "MessageType": "Text", "ImagePath": "", "MessageJP": "いや", "MessageTW": "不", "MessageEN": "No."}, {"MessageId": 10021601, "Flag": 2, "Type": 0, "NextId": 10021610, "MessageType": "Text", "ImagePath": "", "MessageJP": "大したことじゃない", "MessageTW": "沒什麼大不了的", "MessageEN": "That's not it."}, {"MessageId": 10021610, "Flag": 1, "Type": 0, "NextId": 10021620, "MessageType": "Text", "ImagePath": "", "MessageJP": "用事があるなら、無理して来なくても大丈夫", "MessageTW": "如果有事的話，不勉強來也沒關係", "MessageEN": "You don't have to come if you're busy."}, {"MessageId": 10021620, "Flag": 2, "Type": 3, "NextId": 10021630, "MessageType": "Text", "ImagePath": "", "MessageJP": "ううん、すぐ行くね", "MessageTW": "不會，我馬上過去", "MessageEN": "It's fine. I'm free."}, {"MessageId": 10021620, "Flag": 2, "Type": 3, "NextId": 10021630, "MessageType": "Text", "ImagePath": "", "MessageJP": "せっかくアズサが呼んでくれたんだから、喜んで", "MessageTW": "難得梓呼叫我，我很樂意", "MessageEN": "If you need me, Azusa, then I'm there."}, {"MessageId": 10021630, "Flag": 2, "Type": 0, "NextId": 10021631, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 10021631, "Flag": 2, "Type": 0, "NextId": 10021632, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとう、先生", "MessageTW": "謝謝老師", "MessageEN": "Thank you, Sensei."}, {"MessageId": 10021632, "Flag": 2, "Type": 0, "NextId": 10011640, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、待ってるから", "MessageTW": "那麼，我等你", "MessageEN": "I'll be waiting."}, {"MessageId": 10011640, "Flag": 2, "Type": 2, "NextId": 10021640, "MessageType": "Text", "ImagePath": "", "MessageJP": "アズサ（水着）の絆イベントへ", "MessageTW": "前往梓(泳裝)的羈絆劇情", "MessageEN": "Go to Azusa (Swimsuit)'s Event"}, {"MessageId": 10021640, "Flag": 2, "Type": 0, "NextId": 10021650, "MessageType": "Text", "ImagePath": "", "MessageJP": "遅い時間に呼び出してごめん、来てくれてありがとう", "MessageTW": "很抱歉這麼晚叫你出來，謝謝你來", "MessageEN": "Apologies if I've been contacting you too late at night."}, {"MessageId": 10021650, "Flag": 2, "Type": 3, "NextId": 10021660, "MessageType": "Text", "ImagePath": "", "MessageJP": "夜行性だから大丈夫", "MessageTW": "我是夜行性動物，沒事的", "MessageEN": "It's fine. I'm a night owl, anyway."}, {"MessageId": 10021650, "Flag": 2, "Type": 3, "NextId": 10021660, "MessageType": "Text", "ImagePath": "", "MessageJP": "全然、いつでも大歓迎だよ", "MessageTW": "別在意，任何時候都歡迎", "MessageEN": "Don't worry about it."}, {"MessageId": 10021660, "Flag": 2, "Type": 0, "NextId": 10021661, "MessageType": "Text", "ImagePath": "", "MessageJP": "……そっか", "MessageTW": "……這樣啊", "MessageEN": "Oh."}, {"MessageId": 10021661, "Flag": 2, "Type": 0, "NextId": 10021670, "MessageType": "Text", "ImagePath": "", "MessageJP": "じゃあ、また", "MessageTW": "那麼，回頭見", "MessageEN": "Then I'll put it out of my mind."}, {"MessageId": 10021670, "Flag": 1, "Type": 0, "NextId": 10021671, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとう、先生", "MessageTW": "謝謝你，老師", "MessageEN": "Thank you, Sensei."}, {"MessageId": 10021671, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "えっと、今日のことだけじゃなくて……その、色々と", "MessageTW": "那個，不只是今天的事……還有其他很多……", "MessageEN": "For everything."}]