[{"ImagePath": "https://schale.gg/images/student/collection/26004.webp", "MessageJP": "シミコ", "MessageTW": "志美子", "MessageEN": "Shimiko"}, {"MessageId": 260030150, "Flag": 2, "Type": 4, "NextId": 260040158, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 260040158, "Flag": 2, "Type": 0, "NextId": 260040165, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、今お忙しいでしょうか？", "MessageTW": "老師，現在在忙嗎？", "MessageEN": "Are you busy, Sensei?"}, {"MessageId": 260040165, "Flag": 1, "Type": 0, "NextId": 260040162, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっとお願いしたいことがありまして", "MessageTW": "有件事想請你幫忙", "MessageEN": "I have a special favor to ask of you today."}, {"MessageId": 260040162, "Flag": 2, "Type": 0, "NextId": 260040170, "MessageType": "Text", "ImagePath": "", "MessageJP": "よろしければ、付き合っていただけませんか？", "MessageTW": "如果可以的話，能來找我一下嗎？", "MessageEN": "Can you stay with me for a little bit?"}, {"MessageId": 260040170, "Flag": 2, "Type": 3, "NextId": 260040183, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうしたの？", "MessageTW": "怎麼了？", "MessageEN": "What's going on?"}, {"MessageId": 260040170, "Flag": 2, "Type": 3, "NextId": 260040183, "MessageType": "Text", "ImagePath": "", "MessageJP": "デートのお誘いかな？", "MessageTW": "是約會的邀請嗎？", "MessageEN": "Are we going on a date?"}, {"MessageId": 260040183, "Flag": 2, "Type": 0, "NextId": 260040180, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふふっ、それは秘密です", "MessageTW": "呵呵，這是秘密", "MessageEN": "Heeheehee."}, {"MessageId": 260040180, "Flag": 2, "Type": 0, "NextId": 260040197, "MessageType": "Text", "ImagePath": "", "MessageJP": "来ていただけたらすぐに分かりますよ", "MessageTW": "你來的話馬上就會知道了唷", "MessageEN": "It's a secret. You'll see when you get here."}, {"MessageId": 260040197, "Flag": 1, "Type": 0, "NextId": 260030200, "MessageType": "Text", "ImagePath": "", "MessageJP": "ヒントを出しますと、「宝探し」みたいなものです！(・∀・*)", "MessageTW": "給你一個提示，是「尋寶」之類的！(．∀．*)", "MessageEN": "It'll be like a treasure hunt!"}, {"MessageId": 260030200, "Flag": 2, "Type": 2, "NextId": 260040204, "MessageType": "Text", "ImagePath": "", "MessageJP": "シミコの絆イベントへ", "MessageTW": "前往志美子的羈絆劇情", "MessageEN": "Go to Shimiko's Event"}, {"MessageId": 260040204, "Flag": 2, "Type": 0, "NextId": 260040201, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日は本当にありがとうございました、先生！", "MessageTW": "今天真的非常感謝你，老師！", "MessageEN": "Thanks so much for today, Sensei!"}, {"MessageId": 260040201, "Flag": 2, "Type": 0, "NextId": 260040208, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生にご連絡して正解でした、楽しかったです！", "MessageTW": "有聯絡老師果然是對的，我很開心！", "MessageEN": "It would have been impossible without you!"}, {"MessageId": 260040208, "Flag": 2, "Type": 0, "NextId": 260040215, "MessageType": "Text", "ImagePath": "", "MessageJP": "何て言えばいいんでしょう、その、", "MessageTW": "該怎麼說才好呢，那個", "MessageEN": "For some reason..."}, {"MessageId": 260040215, "Flag": 1, "Type": 0, "NextId": 260040212, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生と一緒だといつも楽しくて、毎回良いことが起きるような気がします", "MessageTW": "和老師在一起時總是很開心，感覺每次都會發生好事", "MessageEN": "I feel like good things happen whenever I'm with you."}, {"MessageId": 260040212, "Flag": 2, "Type": 0, "NextId": 260040219, "MessageType": "Text", "ImagePath": "", "MessageJP": "これからもよろしくお願いしますね！先生！(・∀・*)", "MessageTW": "今後也請多多關照囉！老師！(・∀・*)", "MessageEN": "So I hope to see you again soon, Sensei!"}, {"MessageId": 260040219, "Flag": 2, "Type": 0, "NextId": 260040226, "MessageType": "Text", "ImagePath": "", "MessageJP": "……", "MessageTW": "……", "MessageEN": "..."}, {"MessageId": 260040226, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "……返却不可、ですからね？", "MessageTW": "……不接受退貨的唷？", "MessageEN": "You can't say no!"}]