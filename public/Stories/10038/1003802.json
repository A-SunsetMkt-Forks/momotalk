[{"ImagePath": "https://schale.gg/images/student/collection/10038.webp", "MessageJP": "ミヤコ", "MessageTW": "都子", "MessageEN": "Miyako"}, {"MessageId": 100370070, "Flag": 2, "Type": 4, "NextId": 100380070, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100380070, "Flag": 2, "Type": 0, "NextId": 100380080, "MessageType": "Text", "ImagePath": "", "MessageJP": "突然ですが、先生は犬派ですか？猫派ですか?", "MessageTW": "雖然有點突然，不過老師是狗派還是貓派呢？", "MessageEN": "Sensei, are you a dog person or a cat person?"}, {"MessageId": 100380080, "Flag": 2, "Type": 3, "NextId": 100380090, "MessageType": "Text", "ImagePath": "", "MessageJP": "犬派だよ", "MessageTW": "是狗派哦", "MessageEN": "A dog person."}, {"MessageId": 100380080, "Flag": 2, "Type": 3, "NextId": 100380100, "MessageType": "Text", "ImagePath": "", "MessageJP": "猫派だよ", "MessageTW": "是貓派哦", "MessageEN": "A cat person."}, {"MessageId": 100380090, "Flag": 2, "Type": 0, "NextId": 100380110, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうですか。確かに犬は忠誠心があり、人懐っこいということで好きな方が多いですね", "MessageTW": "這樣啊。確實，狗既忠誠又容易與人親近，所以很多人都喜歡呢", "MessageEN": "I see. Dogs are loyal and obedient, so a lot of people love them."}, {"MessageId": 100380100, "Flag": 1, "Type": 0, "NextId": 100380110, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうですか。確かに猫は可愛くてつんとした魅力があって、好きな方が多いですね", "MessageTW": "這樣啊。確實，貓既可愛又有著高傲的魅力，所以很多人都喜歡呢", "MessageEN": "I see. Cats are adorable and sassy, so a lot of people love them."}, {"MessageId": 100380110, "Flag": 2, "Type": 3, "NextId": 100380120, "MessageType": "Text", "ImagePath": "", "MessageJP": "ミヤコは犬派？猫派？", "MessageTW": "都子是狗派還是貓派呢？", "MessageEN": "Are you a dog or cat person, Miyako?"}, {"MessageId": 100380120, "Flag": 2, "Type": 0, "NextId": 100380121, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうですね、 私は……", "MessageTW": "這個嘛，我是……", "MessageEN": "Well, actually... I think..."}, {"MessageId": 100380121, "Flag": 2, "Type": 0, "NextId": 100370130, "MessageType": "Text", "ImagePath": "", "MessageJP": "……強いて言えば、ウサギ派でしょうか", "MessageTW": "……硬要說的話，是兔子派吧", "MessageEN": "I'm a rabbit person."}, {"MessageId": 100370130, "Flag": 2, "Type": 2, "NextId": 100380130, "MessageType": "Text", "ImagePath": "", "MessageJP": "ミヤコの絆イベントへ", "MessageTW": "前往都子的羈絆劇情", "MessageEN": "Go to Miyako's Event"}, {"MessageId": 100380130, "Flag": 2, "Type": 0, "NextId": 100380140, "MessageType": "Image", "ImagePath": "./Stickers/Mo_Miyako_01.webp", "MessageJP": "", "MessageTW": "", "MessageEN": ""}, {"MessageId": 100380140, "Flag": 1, "Type": 0, "NextId": 100380141, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、写真をどうぞ", "MessageTW": "老師，請看照片", "MessageEN": "Sensei, here's a picture of the rabbit we adopted."}, {"MessageId": 100380141, "Flag": 2, "Type": 0, "NextId": 100380150, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょうど餌を食べて寝たところです", "MessageTW": "牠剛吃完飼料正準備要睡覺", "MessageEN": "This is how she sleeps after eating."}, {"MessageId": 100380150, "Flag": 2, "Type": 3, "NextId": 100380160, "MessageType": "Text", "ImagePath": "", "MessageJP": "すごく可愛いね", "MessageTW": "真可愛呢", "MessageEN": "What a cute rabbit."}, {"MessageId": 100380160, "Flag": 2, "Type": 0, "NextId": 100380161, "MessageType": "Text", "ImagePath": "", "MessageJP": "ですよね？ウサギには、犬や猫とはまた違った魅力があります", "MessageTW": "對吧？兔子有著和貓狗不同的魅力", "MessageEN": "Right? Rabbits have a different charm from dogs or cats."}, {"MessageId": 100380161, "Flag": 2, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "後で目覚めたら、また写真を撮って送りますね", "MessageTW": "等牠醒來之後，再傳照片給老師哦", "MessageEN": "I'll take another picture when she wakes up for you."}]