[{"ImagePath": "https://schale.gg/images/student/collection/23001.webp", "MessageJP": "フウカ", "MessageTW": "風華", "MessageEN": "Fuuka"}, {"MessageId": 230000130, "Flag": 2, "Type": 4, "NextId": 230010135, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 230010135, "Flag": 2, "Type": 0, "NextId": 230010140, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、お目覚めになりましたか？体調は如何でしょう……？", "MessageTW": "老師，您醒了嗎？身體還好嗎……？", "MessageEN": "Are you awake, Sensei? How are you feeling?"}, {"MessageId": 230010140, "Flag": 2, "Type": 3, "NextId": 230010156, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、今起きたところ", "MessageTW": "嗯，剛醒來", "MessageEN": "Yes, I'm up."}, {"MessageId": 230010140, "Flag": 2, "Type": 3, "NextId": 230010156, "MessageType": "Text", "ImagePath": "", "MessageJP": "大丈夫、何ともないよ", "MessageTW": "沒事，什麼問題都沒有唷", "MessageEN": "I'm okay. I feel fine."}, {"MessageId": 230010156, "Flag": 2, "Type": 0, "NextId": 230010153, "MessageType": "Text", "ImagePath": "", "MessageJP": "ふう、良かったです……", "MessageTW": "呼，太好了……", "MessageEN": "Phew, that's a relief."}, {"MessageId": 230010153, "Flag": 2, "Type": 0, "NextId": 230010160, "MessageType": "Text", "ImagePath": "", "MessageJP": "どうやったら埋め合わせができるだろうと、先ほどまで色々考えていまして", "MessageTW": "我一直在想，要做什麼才能夠彌補老師", "MessageEN": "I want to redeem myself for that failure from before."}, {"MessageId": 230010160, "Flag": 1, "Type": 0, "NextId": 230010167, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょうど、良いアイデアが浮かんだのですが……", "MessageTW": "正好，我想到了一個好主意……", "MessageEN": "I have a great idea..."}, {"MessageId": 230010167, "Flag": 2, "Type": 0, "NextId": 230010170, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日の午後、お会いできませんか？", "MessageTW": "今天下午能見個面嗎？", "MessageEN": "Can I see you this afternoon, Sensei?"}, {"MessageId": 230010170, "Flag": 2, "Type": 3, "NextId": 230010181, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、喜んで", "MessageTW": "嗯，我很樂意", "MessageEN": "Of course."}, {"MessageId": 230010181, "Flag": 2, "Type": 0, "NextId": 230000190, "MessageType": "Text", "ImagePath": "", "MessageJP": "ありがとうございます！では、待ってますね！", "MessageTW": "非常感謝！那麼，我等老師來唷！", "MessageEN": "Okay then, I'll be waiting!"}, {"MessageId": 230000190, "Flag": 2, "Type": 2, "NextId": 230010198, "MessageType": "Text", "ImagePath": "", "MessageJP": "フウカの絆イベントへ", "MessageTW": "前往風華的羈絆劇情", "MessageEN": "Go to Fuuka's Event"}, {"MessageId": 230010198, "Flag": 2, "Type": 0, "NextId": 230010195, "MessageType": "Text", "ImagePath": "", "MessageJP": "今日のウナギ料理はいかがでしたか？", "MessageTW": "今天的鰻魚料理老師覺得怎麼樣呢？", "MessageEN": "What did you think about the eel today?"}, {"MessageId": 230010195, "Flag": 2, "Type": 0, "NextId": 230010202, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生のお口に合ったのならよかったのですが", "MessageTW": "要是有合老師的口味就好了", "MessageEN": "I hope it suited you."}, {"MessageId": 230010202, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "次はもっと美味しい料理を振舞いますね！", "MessageTW": "下次我會用更美味的料理招待老師唷！", "MessageEN": "I'll serve you something even better next time!"}]