[{"ImagePath": "https://schale.gg/images/student/collection/20013.webp", "MessageJP": "チヒロ", "MessageTW": "千尋", "MessageEN": "Chihiro"}, {"MessageId": 200120010, "Flag": 2, "Type": 4, "NextId": 200130010, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 200130010, "Flag": 2, "Type": 0, "NextId": 200130020, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生、忙しいところにごめん", "MessageTW": "老師，百忙之中打擾你，真不好意思", "MessageEN": "Sensei, I'm sorry, I know you're busy."}, {"MessageId": 200130020, "Flag": 1, "Type": 0, "NextId": 200130021, "MessageType": "Text", "ImagePath": "", "MessageJP": "ちょっとセキュリティ関連の問題で、シャーレのサーバーを調べたくって", "MessageTW": "有一些和系統安全相關的問題，所以想要調查一下夏萊的伺服器", "MessageEN": "There was a security breach at Schale, so I'd like to take a look at the servers."}, {"MessageId": 200130021, "Flag": 2, "Type": 0, "NextId": 200130030, "MessageType": "Text", "ImagePath": "", "MessageJP": "その場にあるPCを開かないといけないから、後でシャーレのオフィスで会える？", "MessageTW": "因為必須要使用到夏萊的電腦，稍後可以在夏萊辦公室見個面嗎？", "MessageEN": "I need the password for the PC. Can you come to the Schale office?"}, {"MessageId": 200130030, "Flag": 2, "Type": 3, "NextId": 200130040, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ、そしたらちょっと待ってて", "MessageTW": "啊，那稍等我一下", "MessageEN": "Okay, be right there."}, {"MessageId": 200130040, "Flag": 2, "Type": 3, "NextId": 200130050, "MessageType": "Text", "ImagePath": "", "MessageJP": "パスワードを書くから", "MessageTW": "我把密碼打出來", "MessageEN": "The password is..."}, {"MessageId": 200130050, "Flag": 2, "Type": 0, "NextId": 200130051, "MessageType": "Text", "ImagePath": "", "MessageJP": "待った", "MessageTW": "等等", "MessageEN": "Wait."}, {"MessageId": 200130051, "Flag": 2, "Type": 0, "NextId": 200130060, "MessageType": "Text", "ImagePath": "", "MessageJP": "普通の回線でパスワードを書くなんて絶対にダメ", "MessageTW": "絕對不可以在一般的線路上把密碼打出來", "MessageEN": "Never write passwords on an unprotected line."}, {"MessageId": 200130060, "Flag": 2, "Type": 3, "NextId": 200130070, "MessageType": "Text", "ImagePath": "", "MessageJP": "モニターの下にパスワードを書いたメモがあるから、それを見ても大丈夫だよ", "MessageTW": "螢幕底下有張寫著密碼的便條紙，只要看那個就可以了", "MessageEN": "I put a sticky note with the password on it at the bottom of the monitor. Use that."}, {"MessageId": 200130070, "Flag": 2, "Type": 0, "NextId": 200130071, "MessageType": "Text", "ImagePath": "", "MessageJP": "え", "MessageTW": "咦", "MessageEN": "Huh?!"}, {"MessageId": 200130071, "Flag": 2, "Type": 0, "NextId": 200120070, "MessageType": "Text", "ImagePath": "", "MessageJP": "……冗談、だよね？", "MessageTW": "……你是開玩笑的，對吧？", "MessageEN": "Are you kidding?"}, {"MessageId": 200120070, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "チヒロの絆イベントへ", "MessageTW": "前往千尋的羈絆劇情", "MessageEN": "Go to Chihiro's Event"}]