[{"ImagePath": "https://schale.gg/images/student/collection/10057.webp", "MessageJP": "ハルナ（正月）", "MessageTW": "羽留奈(正月)", "MessageEN": "Haruna (New Year)"}, {"MessageId": 100560200, "Flag": 2, "Type": 4, "NextId": 100570200, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100570200, "Flag": 2, "Type": 0, "NextId": 100570201, "MessageType": "Text", "ImagePath": "", "MessageJP": "先生", "MessageTW": "老師", "MessageEN": "Sensei..."}, {"MessageId": 100570201, "Flag": 2, "Type": 0, "NextId": 100570210, "MessageType": "Text", "ImagePath": "", "MessageJP": "以前の約束、覚えていらっしゃいますか？", "MessageTW": "之前的約定，您還記得嗎？", "MessageEN": "Do you remember our promise?"}, {"MessageId": 100570210, "Flag": 2, "Type": 3, "NextId": 100570240, "MessageType": "Text", "ImagePath": "", "MessageJP": "覚えてるよ。最高の美食を楽しむんだよね", "MessageTW": "記得唷。是要享受最棒的美食對吧", "MessageEN": "I do. It was to enjoy the best gourmet food possible, right?"}, {"MessageId": 100570210, "Flag": 2, "Type": 3, "NextId": 100570220, "MessageType": "Text", "ImagePath": "", "MessageJP": "もちろん、ワタリガニ漁に行く約束だよね？", "MessageTW": "當然，是要一起去捕梭子蟹對吧？", "MessageEN": "I remember. We said we were gonna catch crabs, right?"}, {"MessageId": 100570220, "Flag": 2, "Type": 0, "NextId": 100570230, "MessageType": "Text", "ImagePath": "", "MessageJP": "……？", "MessageTW": "……？", "MessageEN": "...?"}, {"MessageId": 100570230, "Flag": 1, "Type": 0, "NextId": 100570231, "MessageType": "Text", "ImagePath": "", "MessageJP": "どなたと勘違いされているのか分かりませんが……", "MessageTW": "雖然不知道您把我和誰搞錯了……", "MessageEN": "What are you saying?"}, {"MessageId": 100570231, "Flag": 2, "Type": 0, "NextId": 100570240, "MessageType": "Text", "ImagePath": "", "MessageJP": "最高の美食を、という件ですわ。まあ、カニも良いのですが……。", "MessageTW": "是要享受最棒美食的約定。不過捕螃蟹似乎也不錯……", "MessageEN": "I said we should enjoy the best gourmet food."}, {"MessageId": 100570240, "Flag": 1, "Type": 0, "NextId": 100570241, "MessageType": "Text", "ImagePath": "", "MessageJP": "当日の連絡になってしまいましたが、後ほどお時間頂けますか？", "MessageTW": "雖然當天才連絡您，稍後能占用您一點時間嗎？", "MessageEN": "Do you think you could come see me today?"}, {"MessageId": 100570241, "Flag": 2, "Type": 0, "NextId": 100560240, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうですわね、時間と場所は……", "MessageTW": "那麼，時間和地點是……", "MessageEN": "I'll be waiting for you."}, {"MessageId": 100560240, "Flag": 2, "Type": 2, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "ハルナ（正月）の絆イベントへ", "MessageTW": "前往羽留奈(正月)的羈絆劇情", "MessageEN": "Go to Haruna (New Year)'s Event"}]