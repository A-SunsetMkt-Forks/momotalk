[{"ImagePath": "https://schale.gg/images/student/collection/10008.webp", "MessageJP": "ネル", "MessageTW": "寧瑠", "MessageEN": "Neru"}, {"MessageId": 100070010, "Flag": 2, "Type": 4, "NextId": 100080018, "MessageType": "Text", "ImagePath": "", "MessageJP": "既読", "MessageTW": "已讀", "MessageEN": "Message read"}, {"MessageId": 100080018, "Flag": 2, "Type": 0, "NextId": 100080025, "MessageType": "Text", "ImagePath": "", "MessageJP": "よお先生、最近は忙しいのか？", "MessageTW": "喲，老師，最近忙嗎？", "MessageEN": "Hey, Sensei. Are you busy?"}, {"MessageId": 100080025, "Flag": 1, "Type": 0, "NextId": 100080022, "MessageType": "Text", "ImagePath": "", "MessageJP": "ミレニアムでも見かけねえし、タイミングが合ったら久々に来たらどうだ？", "MessageTW": "在千年也沒見到面，時機合適的話，要不久違地來一下呢？", "MessageEN": "You should stop by the Millennium School sometime."}, {"MessageId": 100080022, "Flag": 2, "Type": 0, "NextId": 100080030, "MessageType": "Text", "ImagePath": "", "MessageJP": "あたしも忙しいけど、ちょっと会うくらいの時間は作れるからさ", "MessageTW": "雖然我也很忙，但能抽出時間見面", "MessageEN": "I'm busy too, but I can make time to see you!"}, {"MessageId": 100080030, "Flag": 2, "Type": 3, "NextId": 100080043, "MessageType": "Text", "ImagePath": "", "MessageJP": "うん、じゃあ遊びに行くね", "MessageTW": "嗯，那就去玩吧", "MessageEN": "Sure, I'll head over when I get the chance."}, {"MessageId": 100080030, "Flag": 2, "Type": 3, "NextId": 100080043, "MessageType": "Text", "ImagePath": "", "MessageJP": "そうだね、じゃあ今度行こうかな", "MessageTW": "說的也是，那就下次去吧", "MessageEN": "Okay. Wait for me."}, {"MessageId": 100080043, "Flag": 2, "Type": 0, "NextId": 100070050, "MessageType": "Text", "ImagePath": "", "MessageJP": "はっ、期待しねえで待ってるよ", "MessageTW": "哈，我會不抱期待地等著唷", "MessageEN": "Hahaha, all right."}, {"MessageId": 100070050, "Flag": 2, "Type": 2, "NextId": 100080050, "MessageType": "Text", "ImagePath": "", "MessageJP": "ネルの絆イベントへ", "MessageTW": "前往寧瑠的羈絆劇情", "MessageEN": "Go to Neru's Event"}, {"MessageId": 100080050, "Flag": 2, "Type": 0, "NextId": 100080060, "MessageType": "Text", "ImagePath": "", "MessageJP": "ったく、来るんだったら先に言ってくれよ。油断してたじゃねえか……", "MessageTW": "真是的，要來的話先告訴我一聲啊。這不是害我大意了嗎……", "MessageEN": "Aw, Sensei! Give me a heads up next time you visit."}, {"MessageId": 100080060, "Flag": 2, "Type": 3, "NextId": 100080071, "MessageType": "Text", "ImagePath": "", "MessageJP": "ネルが呼んでくれたんじゃなかったっけ？", "MessageTW": "不是寧瑠叫我來的嗎？", "MessageEN": "But you told me to come."}, {"MessageId": 100080060, "Flag": 2, "Type": 3, "NextId": 100080071, "MessageType": "Text", "ImagePath": "", "MessageJP": "遊びに行くって言ったはず……？", "MessageTW": "不是說了會去玩……？", "MessageEN": "I told you I was stopping by."}, {"MessageId": 100080071, "Flag": 2, "Type": 0, "NextId": 100080078, "MessageType": "Text", "ImagePath": "", "MessageJP": "あ？そうだったか？", "MessageTW": "啊？是嗎？", "MessageEN": "D-Did I say that? "}, {"MessageId": 100080078, "Flag": 2, "Type": 0, "NextId": 100080075, "MessageType": "Text", "ImagePath": "", "MessageJP": "そう言われるとそんな気もしてきたな……", "MessageTW": "這麼一說，好像有這麼一回事……", "MessageEN": "Maybe I did..."}, {"MessageId": 100080075, "Flag": 2, "Type": 0, "NextId": 100080072, "MessageType": "Text", "ImagePath": "", "MessageJP": "と、とにかくだな！！", "MessageTW": "總，總之呢！！", "MessageEN": "A-Anyway!"}, {"MessageId": 100080072, "Flag": 2, "Type": 0, "NextId": 100080089, "MessageType": "Text", "ImagePath": "", "MessageJP": "っと……なんてーかその……", "MessageTW": "嗯……那個什麼……", "MessageEN": "Ahem."}, {"MessageId": 100080089, "Flag": 1, "Type": 0, "NextId": -1, "MessageType": "Text", "ImagePath": "", "MessageJP": "朝から会えてびっくりしたっていうか……あ、約束は守れよ！？", "MessageTW": "一大早碰面害我嚇一跳……啊，要遵守約定唷！？", "MessageEN": "Anyway, I had a fun morning."}]